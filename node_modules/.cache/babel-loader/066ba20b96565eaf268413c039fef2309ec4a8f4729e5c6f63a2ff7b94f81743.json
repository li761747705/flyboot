{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: 'JobsView',\n  data() {\n    return {\n      jobs: [{\n        id: 1,\n        title: '无人机系统工程师',\n        type: '研发类',\n        salary: '面议',\n        date: '2025-06-18',\n        location: '珠海',\n        duties: ['负责无人机系统集成与测试', '参与无人机系统方案设计', '协助项目推进与技术支持'],\n        requirements: ['本科及以上学历，电子、自动化等相关专业', '有无人机相关经验优先', '具备良好的沟通与团队协作能力']\n      }, {\n        id: 2,\n        title: '无人机软件工程师',\n        type: '研发类',\n        salary: '面议',\n        date: '2025-06-18',\n        location: '珠海',\n        duties: ['负责无人机软件开发与维护', '参与飞控系统开发', '优化软件性能与稳定性'],\n        requirements: ['本科及以上学历，计算机、自动化等相关专业', '熟悉C/C++/Python等编程语言', '有嵌入式开发经验优先']\n      }, {\n        id: 3,\n        title: '无人机设计工程师',\n        type: '研发类',\n        salary: '面议',\n        date: '2025-06-18',\n        location: '珠海',\n        duties: ['负责无人机结构设计', '参与产品外观与功能设计', '协助生产工艺改进'],\n        requirements: ['本科及以上学历，机械设计等相关专业', '熟练使用CAD/SolidWorks等设计软件', '有无人机设计经验优先']\n      }, {\n        id: 4,\n        title: 'PCB设计工程师',\n        type: '研发类',\n        salary: '面议',\n        date: '2025-06-18',\n        location: '珠海',\n        duties: ['负责PCB电路板设计', '参与硬件方案评审', '协助产品测试与调试'],\n        requirements: ['本科及以上学历，电子相关专业', '熟练使用Altium Designer等EDA工具', '有PCB设计经验优先']\n      }, {\n        id: 5,\n        title: '大客户销售经理',\n        type: '销售类',\n        salary: '20K-40K/月',\n        date: '2025-06-18',\n        location: '上海',\n        duties: ['负责公司无人机产品的大客户销售与市场拓展', '维护与开发重点行业客户关系', '参与销售策略制定与执行'],\n        requirements: ['本科及以上学历，市场营销等相关专业', '3年以上大客户销售经验，有无人机或高科技行业经验优先', '具备良好的沟通与商务谈判能力']\n      }, {\n        id: 6,\n        title: '渠道销售专员',\n        type: '销售类',\n        salary: '10K-20K/月',\n        date: '2025-06-18',\n        location: '深圳',\n        duties: ['负责区域渠道的开发与维护', '协助完成销售目标', '收集市场信息，反馈客户需求'],\n        requirements: ['大专及以上学历，市场营销等相关专业', '有渠道销售经验优先', '具备较强的执行力和团队合作精神']\n      }, {\n        id: 7,\n        title: '客户服务专员',\n        type: '客户类',\n        salary: '8K-15K/月',\n        date: '2025-06-18',\n        location: '广州',\n        duties: ['负责客户售前、售中、售后服务支持', '解答客户咨询，处理客户投诉', '协助客户培训与技术支持'],\n        requirements: ['大专及以上学历，专业不限', '有客户服务或技术支持经验优先', '具备良好的服务意识和沟通能力']\n      }, {\n        id: 8,\n        title: '售后技术支持工程师',\n        type: '客户类',\n        salary: '10K-18K/月',\n        date: '2025-06-18',\n        location: '成都',\n        duties: ['负责无人机产品的售后技术支持', '现场故障排查与维修', '客户技术培训与指导'],\n        requirements: ['大专及以上学历，电子、自动化等相关专业', '有无人机或相关产品售后经验优先', '能适应短期出差']\n      }],\n      showDetail: false,\n      selectedJob: {},\n      search: '',\n      searchKey: ''\n    };\n  },\n  methods: {\n    showJobDetail(job) {\n      this.selectedJob = job;\n      this.showDetail = true;\n    },\n    doSearch() {\n      this.searchKey = this.search.trim();\n    }\n  },\n  computed: {\n    filteredJobs() {\n      if (!this.searchKey) return this.jobs;\n      const key = this.searchKey.toLowerCase();\n      return this.jobs.filter(job => job.title.toLowerCase().includes(key) || job.type.toLowerCase().includes(key));\n    }\n  }\n};","map":{"version":3,"names":["name","data","jobs","id","title","type","salary","date","location","duties","requirements","showDetail","selectedJob","search","searchKey","methods","showJobDetail","job","doSearch","trim","computed","filteredJobs","key","toLowerCase","filter","includes"],"sources":["C:\\Users\\75132\\Desktop\\flyboot\\flyboot\\src\\views\\about\\Jobs.vue"],"sourcesContent":["<template>\r\n  <div class=\"jobs-page\">\r\n    <section class=\"py-5\">\r\n      <div class=\"container\">\r\n        <h2 class=\"fw-bold mb-4\">在招职位</h2>\r\n        <div class=\"jobs-search-bar mb-3\">\r\n          <input v-model=\"search\" @keyup.enter=\"doSearch\" type=\"text\" class=\"jobs-search-input\" placeholder=\"输入职位关键词\" />\r\n          <button class=\"jobs-search-btn\" @click=\"doSearch\">搜索</button>\r\n        </div>\r\n        <div class=\"jobs-table-wrapper\">\r\n          <div class=\"jobs-table-header\">\r\n            <div class=\"jobs-table-cell jobs-table-title\">职位名称</div>\r\n            <div class=\"jobs-table-cell jobs-table-type\">类别</div>\r\n            <div class=\"jobs-table-cell jobs-table-salary\">薪资</div>\r\n            <div class=\"jobs-table-cell jobs-table-date\">发布时间</div>\r\n            <div class=\"jobs-table-cell jobs-table-action\"></div>\r\n          </div>\r\n          <div\r\n            v-for=\"(job, idx) in filteredJobs\"\r\n            :key=\"job.id\"\r\n            class=\"jobs-table-row\"\r\n            @click=\"showJobDetail(job)\"\r\n          >\r\n            <div class=\"jobs-table-cell jobs-table-title\">{{ job.title }}</div>\r\n            <div class=\"jobs-table-cell jobs-table-type\">{{ job.type }}</div>\r\n            <div class=\"jobs-table-cell jobs-table-salary\">{{ job.salary }}</div>\r\n            <div class=\"jobs-table-cell jobs-table-date\">{{ job.date }}</div>\r\n            <div class=\"jobs-table-cell jobs-table-action\">\r\n              <span class=\"jobs-table-arrow\">&gt;</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- 弹窗 -->\r\n        <div v-if=\"showDetail\" class=\"job-modal-mask\" @click.self=\"showDetail = false\">\r\n          <div class=\"job-modal\">\r\n            <div class=\"job-modal-header\">\r\n              <span class=\"job-modal-title\">{{ selectedJob.title }}</span>\r\n              <span class=\"job-modal-close\" @click=\"showDetail = false\">×</span>\r\n            </div>\r\n            <div class=\"job-modal-body\">\r\n              <div class=\"mb-2\"><b>类别：</b>{{ selectedJob.type }}</div>\r\n              <div class=\"mb-2\"><b>薪资：</b>{{ selectedJob.salary }}</div>\r\n              <div class=\"mb-2\"><b>发布时间：</b>{{ selectedJob.date }}</div>\r\n              <div class=\"mb-2\"><b>工作地点：</b>{{ selectedJob.location }}</div>\r\n              <div class=\"mb-2\"><b>公司介绍：</b>紫燕无人机是一家专注于工业级无人机研发、生产与应用的高科技企业，致力于为测绘、农业、安防等行业提供高效智能的无人机解决方案。</div>\r\n              <div class=\"mb-2\"><b>岗位职责：</b>\r\n                <ul>\r\n                  <li v-for=\"(duty, i) in selectedJob.duties\" :key=\"i\">{{ duty }}</li>\r\n                </ul>\r\n              </div>\r\n              <div class=\"mb-2\"><b>任职要求：</b>\r\n                <ul>\r\n                  <li v-for=\"(req, i) in selectedJob.requirements\" :key=\"i\">{{ req }}</li>\r\n                </ul>\r\n              </div>\r\n              <div class=\"mb-2\"><b>福利待遇：</b>五险一金、带薪年假、节日福利、年度体检、团建活动、项目奖金等。</div>\r\n              <div class=\"mb-2\"><b>联系方式：</b>hr@ziyanuav.cn</div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'JobsView',\r\n  data() {\r\n    return {\r\n      jobs: [\r\n        {\r\n          id: 1,\r\n          title: '无人机系统工程师',\r\n          type: '研发类',\r\n          salary: '面议',\r\n          date: '2025-06-18',\r\n          location: '珠海',\r\n          duties: [\r\n            '负责无人机系统集成与测试',\r\n            '参与无人机系统方案设计',\r\n            '协助项目推进与技术支持'\r\n          ],\r\n          requirements: [\r\n            '本科及以上学历，电子、自动化等相关专业',\r\n            '有无人机相关经验优先',\r\n            '具备良好的沟通与团队协作能力'\r\n          ]\r\n        },\r\n        {\r\n          id: 2,\r\n          title: '无人机软件工程师',\r\n          type: '研发类',\r\n          salary: '面议',\r\n          date: '2025-06-18',\r\n          location: '珠海',\r\n          duties: [\r\n            '负责无人机软件开发与维护',\r\n            '参与飞控系统开发',\r\n            '优化软件性能与稳定性'\r\n          ],\r\n          requirements: [\r\n            '本科及以上学历，计算机、自动化等相关专业',\r\n            '熟悉C/C++/Python等编程语言',\r\n            '有嵌入式开发经验优先'\r\n          ]\r\n        },\r\n        {\r\n          id: 3,\r\n          title: '无人机设计工程师',\r\n          type: '研发类',\r\n          salary: '面议',\r\n          date: '2025-06-18',\r\n          location: '珠海',\r\n          duties: [\r\n            '负责无人机结构设计',\r\n            '参与产品外观与功能设计',\r\n            '协助生产工艺改进'\r\n          ],\r\n          requirements: [\r\n            '本科及以上学历，机械设计等相关专业',\r\n            '熟练使用CAD/SolidWorks等设计软件',\r\n            '有无人机设计经验优先'\r\n          ]\r\n        },\r\n        {\r\n          id: 4,\r\n          title: 'PCB设计工程师',\r\n          type: '研发类',\r\n          salary: '面议',\r\n          date: '2025-06-18',\r\n          location: '珠海',\r\n          duties: [\r\n            '负责PCB电路板设计',\r\n            '参与硬件方案评审',\r\n            '协助产品测试与调试'\r\n          ],\r\n          requirements: [\r\n            '本科及以上学历，电子相关专业',\r\n            '熟练使用Altium Designer等EDA工具',\r\n            '有PCB设计经验优先'\r\n          ]\r\n        },\r\n        {\r\n          id: 5,\r\n          title: '大客户销售经理',\r\n          type: '销售类',\r\n          salary: '20K-40K/月',\r\n          date: '2025-06-18',\r\n          location: '上海',\r\n          duties: [\r\n            '负责公司无人机产品的大客户销售与市场拓展',\r\n            '维护与开发重点行业客户关系',\r\n            '参与销售策略制定与执行'\r\n          ],\r\n          requirements: [\r\n            '本科及以上学历，市场营销等相关专业',\r\n            '3年以上大客户销售经验，有无人机或高科技行业经验优先',\r\n            '具备良好的沟通与商务谈判能力'\r\n          ]\r\n        },\r\n        {\r\n          id: 6,\r\n          title: '渠道销售专员',\r\n          type: '销售类',\r\n          salary: '10K-20K/月',\r\n          date: '2025-06-18',\r\n          location: '深圳',\r\n          duties: [\r\n            '负责区域渠道的开发与维护',\r\n            '协助完成销售目标',\r\n            '收集市场信息，反馈客户需求'\r\n          ],\r\n          requirements: [\r\n            '大专及以上学历，市场营销等相关专业',\r\n            '有渠道销售经验优先',\r\n            '具备较强的执行力和团队合作精神'\r\n          ]\r\n        },\r\n        {\r\n          id: 7,\r\n          title: '客户服务专员',\r\n          type: '客户类',\r\n          salary: '8K-15K/月',\r\n          date: '2025-06-18',\r\n          location: '广州',\r\n          duties: [\r\n            '负责客户售前、售中、售后服务支持',\r\n 