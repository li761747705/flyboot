{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: 'FPVDrone',\n  data() {\n    return {\n      activeFilter: 'all',\n      drones: [{\n        id: 'fpv-p1',\n        type: 'racing',\n        name: 'P1 竞速穿越机',\n        description: '专业竞速型穿越无人机，极致速度体验，适合专业FPV竞速比赛',\n        image: '/images/products/drones/fpv-p1.jpg',\n        specifications: {\n          '机身尺寸': '220mm',\n          '重量(不含电池)': '350g',\n          '最大速度': '160km/h',\n          '续航时间': '8分钟',\n          '图传系统': 'HD低延迟',\n          '控制距离': '2km',\n          '飞控': 'F7 MPU6000',\n          '电调': '45A BLHeli_32',\n          '电机': '2306 2450KV',\n          '电池': '4S 1500mAh'\n        }\n      }, {\n        id: 'fpv-p2x',\n        type: 'freestyle',\n        name: 'P2X 特技穿越机',\n        description: '自由式特技飞行穿越机，优秀的机动性能，适合特技飞行和创意拍摄',\n        image: '/images/products/drones/fpv-p2x.jpg',\n        specifications: {\n          '机身尺寸': '250mm',\n          '重量(不含电池)': '380g',\n          '最大速度': '140km/h',\n          '续航时间': '10分钟',\n          '图传系统': '数字高清',\n          '控制距离': '3km',\n          '飞控': 'F7 ICM20689',\n          '电调': '50A BLHeli_32',\n          '电机': '2207 1850KV',\n          '电池': '6S 1300mAh'\n        }\n      }, {\n        id: 'fpv-c1',\n        type: 'cinewhoop',\n        name: 'C1 电影穿越机',\n        description: '电影级穿越无人机，带有螺旋桨保护罩，适合室内拍摄和人物跟随',\n        image: '/images/products/drones/fpv-c1.jpg',\n        specifications: {\n          '机身尺寸': '95mm',\n          '重量(不含电池)': '280g',\n          '最大速度': '80km/h',\n          '续航时间': '6分钟',\n          '图传系统': '数字高清',\n          '控制距离': '1km',\n          '飞控': 'F4 MPU6000',\n          '电调': '20A BLHeli_S',\n          '电机': '1404 3800KV',\n          '电池': '4S 850mAh'\n        }\n      }]\n    };\n  },\n  computed: {\n    filteredDrones() {\n      if (this.activeFilter === 'all') {\n        return this.drones;\n      }\n      return this.drones.filter(drone => drone.type === this.activeFilter);\n    }\n  },\n  methods: {\n    setFilter(filter) {\n      this.activeFilter = filter;\n    },\n    getMainSpecs(drone) {\n      const keys = ['机身尺寸', '重量(不含电池)', '最大速度', '续航时间'];\n      return Object.fromEntries(Object.entries(drone.specifications).filter(([key]) => keys.includes(key)));\n    },\n    getTypeName(type) {\n      const names = {\n        'racing': '竞速型',\n        'freestyle': '特技型',\n        'cinewhoop': '电影机'\n      };\n      return names[type] || '穿越机';\n    }\n  }\n};","map":{"version":3,"names":["name","data","activeFilter","drones","id","type","description","image","specifications","computed","filteredDrones","filter","drone","methods","setFilter","getMainSpecs","keys","Object","fromEntries","entries","key","includes","getTypeName","names"],"sources":["C:\\Users\\75132\\Desktop\\flyboot\\flyboot\\src\\views\\products\\FPV.vue"],"sourcesContent":["<!-- FPV穿越无人机页面 -->\n<template>\n  <div class=\"fpv-drone-page py-5\">\n    <div class=\"container\">\n      <!-- 页面标题 -->\n      <div class=\"row mb-5\">\n        <div class=\"col-12 text-center\">\n          <h1 class=\"display-5 fw-bold mb-3\">穿越无人机</h1>\n          <p class=\"lead text-secondary\">高速穿越无人机，搭载高清FPV系统，提供极致的飞行体验，适用于竞速、特技飞行和航拍创作</p>\n        </div>\n      </div>\n\n      <!-- 产品筛选 -->\n      <div class=\"row mb-4\">\n        <div class=\"col-12\">\n          <div class=\"category-filter d-flex flex-wrap justify-content-center gap-3\">\n            <button class=\"btn\" :class=\"activeFilter === 'all' ? 'btn-warning' : 'btn-outline-warning'\" @click=\"setFilter('all')\">\n              全部穿越机\n            </button>\n            <button class=\"btn\" :class=\"activeFilter === 'racing' ? 'btn-warning' : 'btn-outline-warning'\" @click=\"setFilter('racing')\">\n              <i class=\"bi bi-speedometer me-1\"></i>竞速型\n            </button>\n            <button class=\"btn\" :class=\"activeFilter === 'freestyle' ? 'btn-warning' : 'btn-outline-warning'\" @click=\"setFilter('freestyle')\">\n              <i class=\"bi bi-stars me-1\"></i>特技型\n            </button>\n            <button class=\"btn\" :class=\"activeFilter === 'cinewhoop' ? 'btn-warning' : 'btn-outline-warning'\" @click=\"setFilter('cinewhoop')\">\n              <i class=\"bi bi-camera-video me-1\"></i>电影机\n            </button>\n          </div>\n        </div>\n      </div>\n\n      <!-- 产品展示 -->\n      <div class=\"row g-4\">\n        <div class=\"col-12 col-md-6 col-lg-4\" v-for=\"drone in filteredDrones\" :key=\"drone.id\">\n          <div class=\"product-card animate__animated animate__fadeInUp\">\n            <div class=\"product-image-container\">\n              <img :src=\"drone.image\" :alt=\"drone.name\" class=\"img-fluid\">\n            </div>\n            <div class=\"product-overlay\">\n              <div class=\"product-content\">\n                <div class=\"product-badge bg-warning\">{{ getTypeName(drone.type) }}</div>\n                <h3 class=\"product-title\">{{drone.name}}</h3>\n                <p class=\"product-brief\">{{drone.description}}</p>\n                <div class=\"product-actions\">\n                  <router-link :to=\"'/products/platform/detail/' + drone.id\" class=\"btn btn-light rounded-pill px-4\">查看详情</router-link>\n                  <button class=\"btn btn-outline-light btn-icon ms-2 rounded-circle\" title=\"添加收藏\">\n                    <i class=\"bi bi-heart\"></i>\n                  </button>\n                </div>\n              </div>\n            </div>\n            <div class=\"product-tags\">\n              <span class=\"product-tag\" v-for=\"(value, key) in getMainSpecs(drone)\" :key=\"key\">\n                {{key}}: {{value}}\n              </span>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 产品特点 -->\n      <div class=\"row mt-5\">\n        <div class=\"col-12\">\n          <div class=\"features-card p-4 animate__animated animate__fadeIn\">\n            <div class=\"feature-header mb-4\">\n              <h4 class=\"mb-3\"><i class=\"bi bi-camera-video-fill text-warning me-2\"></i>穿越无人机特点</h4>\n              <p class=\"feature-subtitle\">体验极速飞行与卓越性能，为特技表演和竞速比赛而生</p>\n            </div>\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <ul class=\"feature-list\">\n                  <li class=\"animate__animated animate__fadeInLeft animate__delay-1s\">\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>高速穿越飞行体验\n                    <span class=\"feature-desc\">最高飞行速度可达180km/h，满足竞速需求</span>\n                  </li>\n                  <li class=\"animate__animated animate__fadeInLeft animate__delay-2s\">\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>低延迟FPV图传系统\n                    <span class=\"feature-desc\">图传延迟低至28ms，实时响应控制指令</span>\n                  </li>\n                  <li class=\"animate__animated animate__fadeInLeft animate__delay-3s\">\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>灵活操控与特技飞行\n                    <span class=\"feature-desc\">支持360°翻转、急速俯冲等复杂动作</span>\n                  </li>\n                </ul>\n              </div>\n              <div class=\"col-md-6\">\n                <ul class=\"feature-list\">\n                  <li class=\"animate__animated animate__fadeInRight animate__delay-1s\">\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>高清航拍能力\n                    <span class=\"feature-desc\">搭载4K高清相机，捕捉稳定流畅的画面</span>\n                  </li>\n                  <li class=\"animate__animated animate__fadeInRight animate__delay-2s\">\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>轻量化设计\n                    <span class=\"feature-desc\">采用碳纤维材料，整机重量低至0.8kg</span>\n                  </li>\n                  <li class=\"animate__animated animate__fadeInRight animate__delay-3s\">\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>竞速与表演场景应用\n                    <span class=\"feature-desc\">广泛应用于竞速比赛、特技表演和创意拍摄</span>\n                  </li>\n                </ul>\n              </div>\n            </div>\n            <div class=\"text-center mt-4\">\n              <a href=\"#\" class=\"btn btn-outline-warning rounded-pill\">了解更多穿越无人机技术 <i class=\"bi bi-arrow-right\"></i></a>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 应用场景 -->\n      <div class=\"row mt-5\">\n        <div class=\"col-12 text-center mb-4\">\n          <h3 class=\"section-title\">应用场景</h3>\n          <p class=\"section-description\">穿越无人机广泛应用于以下场景</p>\n        </div>\n        \n        <div class=\"col-md-4 mb-4\">\n          <div class=\"application-card text-center p-4\">\n            <div class=\"application-icon mb-3\">\n              <i class=\"bi bi-trophy fs-1 text-warning\"></i>\n            </div>\n            <h4>FPV竞速</h4>\n            <p>专业无人机竞速比赛，穿越复杂赛道，体验速度与激情</p>\n          </div>\n        </div>\n        \n        <div class=\"col-md-4 mb-4\">\n          <div class=\"application-card text-center p-4\">\n            <i class=\"bi bi-camera-reels fs-1 text-warning mb-3\"></i>\n            <h4>创意航拍</h4>\n            <p>灵活机动的航拍视角，为影视创作提供独特视觉体验</p>\n          </div>\n        </div>\n        \n        <div class=\"col-md-4 mb-4\">\n          <div class=\"application-card text-center p-4\">\n            <i class=\"bi bi-stars fs-1 text-warning mb-3\"></i>\n            <h4>特技表演</h4>\n            <p>高难度特技飞行表演，展示卓越的飞行技巧和操控精度</p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'FPVDrone',\n  data() {\n    return {\n      activeFilter: 'all',\n      drones: [\n        {\n          id: 'fpv-p1',\n          type: 'racing',\n          name: 'P1 竞速穿越机',\n          description: '专业竞速型穿越无人机，极致速度体验，适合专业FPV竞速比赛',\n          image: '/images/products/drones/fpv-p1.jpg',\n          specifications: {\n            '机身尺寸': '220mm',\n            '重量(不含电池)': '350g',\n            '最大速度': '160km/h',\n            '续航时间': '8分钟',\n            '图传系统': 'HD低延迟',\n            '控制距离': '2km',\n            '飞控': 'F7 MPU6000',\n            '电调': '45A BLHeli_32',\n            '电机': '2306 2450KV',\n            '电池': '4S 1500mAh'\n          }\n        },\n        {\n          id: 'fpv-p2x',\n          type: 'freestyle',\n          name: 'P2X 特技穿越机',\n          description: '自由式特技飞行穿越机，优秀的机动性能，适合特技飞行和创意拍摄',\n          image: '/images/products/drones/fpv-p2x.jpg',\n          specifications: {\n            '机身尺寸': '250mm',\n            '重量(不含电池)': '380g',\n            '最大速度': '140km/h',\n            '续航时间': '10分钟',\n            '图传系统': '数字高清',\n            '控制距离': '3km',\n            '飞控': 'F7 ICM20689',\n            '电调': '50A BLHeli_32',\n            '电机': '2207 1850KV',\n            '电池': '6S 1300mAh'\n          }\n        },\n        {\n          id: 'fpv-c1',\n          type: 'cinewhoop',\n          name: 'C1 电影穿越机',\n          description: '电影级穿越无人机，带有螺旋桨保护罩，适合室内拍摄和人物跟随',\n          image: '/images/products/drones/fpv-c1.jpg',\n          specifications: {\n            '机身尺寸': '95mm',\n            '重量(不含电池)': '280g',\n            '最大速度': '80km/h',\n            '续航时间': '6分钟',\n            '图传系统': '数字高清',\n            '控制距离': '1km',\n            '飞控': 'F4 MPU6000',\n            '电调': '20A BLHeli_S',\n            '电机': '1404 3800KV',\n            '电池': '4S 850mAh'\n          }\n        }\n      ]\n    };\n  },\n  computed: {\n    filteredDrones() {\n      if (this.activeFilter === 'all') {\n        return this.drones;\n      }\n      return this.drones.filter(drone => drone.type === this.activeFilter);\n    }\n  },\n  methods: {\n    setFilter(filter) {\n      this.activeFilter = filter;\n    },\n    getMainSpecs(drone) {\n      const keys = ['机身尺寸', '重量(不含电池)', '最大速度', '续航时间'];\n      return Object.fromEntries(\n        Object.entries(drone.specifications)\n          .filter(([key]) => keys.includes(key))\n      );\n    },\n    getTypeName(type) {\n      const names = {\n        'racing': '竞速型',\n        'freestyle': '特技型',\n        'cinewhoop': '电影机'\n      };\n      return names[type] || '穿越机';\n    }\n  }\n};\n</script>\n\n<style scoped>\n.fpv-drone-page {\n  min-height: 80vh;\n}\n\n.product-card {\n  border-radius: 12px;\n  overflow: hidden;\n  box-shadow: 0 5px 15px rgba(0,0,0,0.08);\n  position: relative;\n  transition: all 0.3s ease;\n  margin-bottom: 30px;\n  height: 100%;\n}\n\n.product-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 10px 25px rgba(0,0,0,0.15);\n}\n\n.product-image-container {\n  height: 250px;\n  overflow: hidden;\n}\n\n.product-image-container img {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  transition: all 0.5s ease;\n}\n\n.product-card:hover .product-image-container img {\n  transform: scale(1.1);\n}\n\n.product-overlay {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background: linear-gradient(to top, rgba(0,0,0,0.8), transparent);\n  opacity: 0;\n  transition: all 0.3s ease;\n  display: flex;\n  align-items: flex-end;\n  padding: 20px;\n}\n\n.product-card:hover .product-overlay {\n  opacity: 1;\n}\n\n.product-content {\n  color: white;\n}\n\n.product-badge {\n  display: inline-block;\n  padding: 5px 10px;\n  border-radius: 4px;\n  font-size: 0.8rem;\n  font-weight: 500;\n  margin-bottom: 10px;\n}\n\n.product-title {\n  font-size: 1.5rem;\n  margin-bottom: 8px;\n}\n\n.product-brief {\n  font-size: 0.9rem;\n  margin-bottom: 15px;\n  display: -webkit-box;\n  -webkit-line-clamp: 2;\n  -webkit-box-orient: vertical;\n  overflow: hidden;\n}\n\n.product-actions {\n  display: flex;\n  align-items: center;\n}\n\n.product-tags {\n  position: absolute;\n  top: 10px;\n  right: 10px;\n  display: flex;\n  flex-direction: column;\n  gap: 5px;\n}\n\n.product-tag {\n  background: rgba(255,255,255,0.9);\n  color: #333;\n  font-size: 0.75rem;\n  padding: 4px 8px;\n  border-radius: 4px;\n  backdrop-filter: blur(4px);\n}\n\n.features-card {\n  background-color: #fff;\n  border-radius: 12px;\n  box-shadow: 0 5px 15px rgba(0,0,0,0.05);\n  margin-bottom: 30px;\n}\n\n.feature-list {\n  list-style: none;\n  padding-left: 0;\n}\n\n.feature-list li {\n  margin-bottom: 20px;\n  position: relative;\n}\n\n.feature-desc {\n  display: block;\n  font-size: 0.9rem;\n  color: #6c757d;\n  margin-top: 5px;\n  margin-left: 25px;\n}\n\n.application-card {\n  background-color: #fff;\n  border-radius: 12px;\n  box-shadow: 0 5px 15px rgba(0,0,0,0.05);\n  transition: all 0.3s ease;\n  height: 100%;\n}\n\n.application-card:hover {\n  transform: translateY(-5px);\n  box-shadow: 0 10px 25px rgba(0,0,0,0.1);\n}\n</style>\n\n"],"mappings":";;AAoJA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MACnBC,MAAM,EAAE,CACN;QACEC,EAAE,EAAE,QAAQ;QACZC,IAAI,EAAE,QAAQ;QACdL,IAAI,EAAE,UAAU;QAChBM,WAAW,EAAE,+BAA+B;QAC5CC,KAAK,EAAE,oCAAoC;QAC3CC,cAAc,EAAE;UACd,MAAM,EAAE,OAAO;UACf,UAAU,EAAE,MAAM;UAClB,MAAM,EAAE,SAAS;UACjB,MAAM,EAAE,KAAK;UACb,MAAM,EAAE,OAAO;UACf,MAAM,EAAE,KAAK;UACb,IAAI,EAAE,YAAY;UAClB,IAAI,EAAE,eAAe;UACrB,IAAI,EAAE,aAAa;UACnB,IAAI,EAAE;QACR;MACF,CAAC,EACD;QACEJ,EAAE,EAAE,SAAS;QACbC,IAAI,EAAE,WAAW;QACjBL,IAAI,EAAE,WAAW;QACjBM,WAAW,EAAE,gCAAgC;QAC7CC,KAAK,EAAE,qCAAqC;QAC5CC,cAAc,EAAE;UACd,MAAM,EAAE,OAAO;UACf,UAAU,EAAE,MAAM;UAClB,MAAM,EAAE,SAAS;UACjB,MAAM,EAAE,MAAM;UACd,MAAM,EAAE,MAAM;UACd,MAAM,EAAE,KAAK;UACb,IAAI,EAAE,aAAa;UACnB,IAAI,EAAE,eAAe;UACrB,IAAI,EAAE,aAAa;UACnB,IAAI,EAAE;QACR;MACF,CAAC,EACD;QACEJ,EAAE,EAAE,QAAQ;QACZC,IAAI,EAAE,WAAW;QACjBL,IAAI,EAAE,UAAU;QAChBM,WAAW,EAAE,+BAA+B;QAC5CC,KAAK,EAAE,oCAAoC;QAC3CC,cAAc,EAAE;UACd,MAAM,EAAE,MAAM;UACd,UAAU,EAAE,MAAM;UAClB,MAAM,EAAE,QAAQ;UAChB,MAAM,EAAE,KAAK;UACb,MAAM,EAAE,MAAM;UACd,MAAM,EAAE,KAAK;UACb,IAAI,EAAE,YAAY;UAClB,IAAI,EAAE,cAAc;UACpB,IAAI,EAAE,aAAa;UACnB,IAAI,EAAE;QACR;MACF;IAEJ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,cAAcA,CAAA,EAAG;MACf,IAAI,IA