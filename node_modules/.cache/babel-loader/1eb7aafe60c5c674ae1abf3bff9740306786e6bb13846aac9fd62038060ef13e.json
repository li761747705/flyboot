{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: 'FPVDrone',\n  data() {\n    return {\n      activeFilter: 'all',\n      drones: [{\n        id: 'fpv-p1',\n        type: 'racing',\n        name: 'P1 竞速穿越机',\n        description: '专业竞速型穿越无人机，极致速度体验，适合专业FPV竞速比赛',\n        image: '/images/products/drones/fpv-p1.jpg',\n        specifications: {\n          '机身尺寸': '220mm',\n          '重量(不含电池)': '350g',\n          '最大速度': '160km/h',\n          '续航时间': '8分钟',\n          '图传系统': 'HD低延迟',\n          '控制距离': '2km',\n          '飞控': 'F7 MPU6000',\n          '电调': '45A BLHeli_32',\n          '电机': '2306 2450KV',\n          '电池': '4S 1500mAh'\n        }\n      }, {\n        id: 'fpv-p2x',\n        type: 'freestyle',\n        name: 'P2X 特技穿越机',\n        description: '自由式特技飞行穿越机，优秀的机动性能，适合特技飞行和创意拍摄',\n        image: '/images/products/drones/fpv-p2x.jpg',\n        specifications: {\n          '机身尺寸': '250mm',\n          '重量(不含电池)': '380g',\n          '最大速度': '140km/h',\n          '续航时间': '10分钟',\n          '图传系统': '数字高清',\n          '控制距离': '3km',\n          '飞控': 'F7 ICM20689',\n          '电调': '50A BLHeli_32',\n          '电机': '2207 1850KV',\n          '电池': '6S 1300mAh'\n        }\n      }, {\n        id: 'fpv-c1',\n        type: 'cinewhoop',\n        name: 'C1 电影穿越机',\n        description: '电影级穿越无人机，带有螺旋桨保护罩，适合室内拍摄和人物跟随',\n        image: '/images/products/drones/fpv-c1.jpg',\n        specifications: {\n          '机身尺寸': '95mm',\n          '重量(不含电池)': '280g',\n          '最大速度': '80km/h',\n          '续航时间': '6分钟',\n          '图传系统': '数字高清',\n          '控制距离': '1km',\n          '飞控': 'F4 MPU6000',\n          '电调': '20A BLHeli_S',\n          '电机': '1404 3800KV',\n          '电池': '4S 850mAh'\n        }\n      }]\n    };\n  },\n  computed: {\n    filteredDrones() {\n      if (this.activeFilter === 'all') {\n        return this.drones;\n      }\n      return this.drones.filter(drone => drone.type === this.activeFilter);\n    }\n  },\n  methods: {\n    setFilter(filter) {\n      this.activeFilter = filter;\n    },\n    getMainSpecs(drone) {\n      const keys = ['机身尺寸', '重量(不含电池)', '最大速度', '续航时间'];\n      return Object.fromEntries(Object.entries(drone.specifications).filter(([key]) => keys.includes(key)));\n    },\n    getTypeName(type) {\n      const names = {\n        'racing': '竞速型',\n        'freestyle': '特技型',\n        'cinewhoop': '电影机'\n      };\n      return names[type] || '穿越机';\n    }\n  }\n};","map":{"version":3,"names":["name","data","activeFilter","drones","id","type","description","image","specifications","computed","filteredDrones","filter","drone","methods","setFilter","getMainSpecs","keys","Object","fromEntries","entries","key","includes","getTypeName","names"],"sources":["C:\\Users\\75132\\Desktop\\flyboot\\flyboot\\src\\views\\products\\drone\\FPV.vue"],"sourcesContent":["<!-- FPV无人机页面 -->\r\n<template>\r\n  <div class=\"fpv-drone-page py-5\">\r\n    <div class=\"container\">\r\n      <!-- 页面标题 -->\r\n      <div class=\"row mb-5\">\r\n        <div class=\"col-12 text-center\">\r\n          <h1 class=\"display-5 fw-bold mb-3\">穿越无人机</h1>\r\n          <p class=\"lead text-secondary\">高速穿越无人机，搭载高清FPV系统，提供极致的飞行体验，适用于竞速、特技飞行和航拍创作</p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 产品筛选 -->\r\n      <div class=\"row mb-4\">\r\n        <div class=\"col-12\">\r\n          <div class=\"category-filter d-flex flex-wrap justify-content-center gap-3\">\r\n            <button class=\"btn\" :class=\"activeFilter === 'all' ? 'btn-warning' : 'btn-outline-warning'\" @click=\"setFilter('all')\">\r\n              全部穿越机\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'racing' ? 'btn-warning' : 'btn-outline-warning'\" @click=\"setFilter('racing')\">\r\n              <i class=\"bi bi-speedometer me-1\"></i>竞速型\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'freestyle' ? 'btn-warning' : 'btn-outline-warning'\" @click=\"setFilter('freestyle')\">\r\n              <i class=\"bi bi-stars me-1\"></i>特技型\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'cinewhoop' ? 'btn-warning' : 'btn-outline-warning'\" @click=\"setFilter('cinewhoop')\">\r\n              <i class=\"bi bi-camera-video me-1\"></i>电影机\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 产品展示 -->\r\n      <div class=\"row g-4\">\r\n        <div class=\"col-12 col-md-6 col-lg-4\" v-for=\"drone in filteredDrones\" :key=\"drone.id\">\r\n          <div class=\"product-card animate__animated animate__fadeInUp\">\r\n            <div class=\"product-image-container\">\r\n              <img :src=\"drone.image\" :alt=\"drone.name\" class=\"img-fluid\">\r\n            </div>\r\n            <div class=\"product-overlay\">\r\n              <div class=\"product-content\">\r\n                <div class=\"product-badge bg-warning\">{{ getTypeName(drone.type) }}</div>\r\n                <h3 class=\"product-title\">{{drone.name}}</h3>\r\n                <p class=\"product-brief\">{{drone.description}}</p>\r\n                <div class=\"product-actions\">\r\n                  <router-link :to=\"'/products/platform/detail/' + drone.id\" class=\"btn btn-light rounded-pill px-4\">查看详情</router-link>\r\n                  <button class=\"btn btn-outline-light btn-icon ms-2 rounded-circle\" title=\"添加收藏\">\r\n                    <i class=\"bi bi-heart\"></i>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"product-tags\">\r\n              <span class=\"product-tag\" v-for=\"(value, key) in getMainSpecs(drone)\" :key=\"key\">\r\n                {{key}}: {{value}}\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 产品特点 -->\r\n      <div class=\"row mt-5\">\r\n        <div class=\"col-12\">\r\n          <div class=\"features-card p-4 animate__animated animate__fadeIn\">\r\n            <div class=\"feature-header mb-4\">\r\n              <h4 class=\"mb-3\"><i class=\"bi bi-camera-video-fill text-warning me-2\"></i>穿越无人机特点</h4>\r\n              <p class=\"feature-subtitle\">体验极速飞行与卓越性能，为特技表演和竞速比赛而生</p>\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col-md-6\">\r\n                <ul class=\"feature-list\">\r\n                  <li class=\"animate__animated animate__fadeInLeft animate__delay-1s\">\r\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>高速穿越飞行体验\r\n                    <span class=\"feature-desc\">最高飞行速度可达180km/h，满足竞速需求</span>\r\n                  </li>\r\n                  <li class=\"animate__animated animate__fadeInLeft animate__delay-2s\">\r\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>低延迟FPV图传系统\r\n                    <span class=\"feature-desc\">图传延迟低至28ms，实时响应控制指令</span>\r\n                  </li>\r\n                  <li class=\"animate__animated animate__fadeInLeft animate__delay-3s\">\r\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>灵活操控与特技飞行\r\n                    <span class=\"feature-desc\">支持360°翻转、急速俯冲等复杂动作</span>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n              <div class=\"col-md-6\">\r\n                <ul class=\"feature-list\">\r\n                  <li class=\"animate__animated animate__fadeInRight animate__delay-1s\">\r\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>高清航拍能力\r\n                    <span class=\"feature-desc\">搭载4K高清相机，捕捉稳定流畅的画面</span>\r\n                  </li>\r\n                  <li class=\"animate__animated animate__fadeInRight animate__delay-2s\">\r\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>轻量化设计\r\n                    <span class=\"feature-desc\">采用碳纤维材料，整机重量低至0.8kg</span>\r\n                  </li>\r\n                  <li class=\"animate__animated animate__fadeInRight animate__delay-3s\">\r\n                    <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>竞速与表演场景应用\r\n                    <span class=\"feature-desc\">广泛应用于竞速比赛、特技表演和创意拍摄</span>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n            <div class=\"text-center mt-4\">\r\n              <a href=\"#\" class=\"btn btn-outline-warning rounded-pill\">了解更多穿越无人机技术 <i class=\"bi bi-arrow-right\"></i></a>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 应用场景 -->\r\n      <div class=\"row mt-5\">\r\n        <div class=\"col-12 text-center mb-4\">\r\n          <h3 class=\"section-title\">应用场景</h3>\r\n          <p class=\"section-description\">穿越无人机广泛应用于以下场景</p>\r\n        </div>\r\n        \r\n        <div class=\"col-md-4 mb-4\">\r\n          <div class=\"application-card text-center p-4\">\r\n            <div class=\"application-icon mb-3\">\r\n              <i class=\"bi bi-trophy fs-1 text-warning\"></i>\r\n            </div>\r\n            <h4>FPV竞速</h4>\r\n            <p>专业无人机竞速比赛，穿越复杂赛道，体验速度与激情</p>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"col-md-4 mb-4\">\r\n          <div class=\"application-card text-center p-4\">\r\n            <i class=\"bi bi-camera-reels fs-1 text-warning mb-3\"></i>\r\n            <h4>创意航拍</h4>\r\n            <p>灵活机动的航拍视角，为影视创作提供独特视觉体验</p>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"col-md-4 mb-4\">\r\n          <div class=\"application-card text-center p-4\">\r\n            <i class=\"bi bi-stars fs-1 text-warning mb-3\"></i>\r\n            <h4>特技表演</h4>\r\n            <p>高难度特技飞行表演，展示卓越的飞行技巧和操控精度</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'FPVDrone',\r\n  data() {\r\n    return {\r\n      activeFilter: 'all',\r\n      drones: [\r\n        {\r\n          id: 'fpv-p1',\r\n          type: 'racing',\r\n          name: 'P1 竞速穿越机',\r\n          description: '专业竞速型穿越无人机，极致速度体验，适合专业FPV竞速比赛',\r\n          image: '/images/products/drones/fpv-p1.jpg',\r\n          specifications: {\r\n            '机身尺寸': '220mm',\r\n            '重量(不含电池)': '350g',\r\n            '最大速度': '160km/h',\r\n            '续航时间': '8分钟',\r\n            '图传系统': 'HD低延迟',\r\n            '控制距离': '2km',\r\n            '飞控': 'F7 MPU6000',\r\n            '电调': '45A BLHeli_32',\r\n            '电机': '2306 2450KV',\r\n            '电池': '4S 1500mAh'\r\n          }\r\n        },\r\n        {\r\n          id: 'fpv-p2x',\r\n          type: 'freestyle',\r\n          name: 'P2X 特技穿越机',\r\n          description: '自由式特技飞行穿越机，优秀的机动性能，适合特技飞行和创意拍摄',\r\n          image: '/images/products/drones/fpv-p2x.jpg',\r\n          specifications: {\r\n            '机身尺寸': '250mm',\r\n            '重量(不含电池)': '380g',\r\n            '最大速度': '140km/h',\r\n            '续航时间': '10分钟',\r\n            '图传系统': '数字高清',\r\n            '控制距离': '3km',\r\n            '飞控': 'F7 ICM20689',\r\n            '电调': '50A BLHeli_32',\r\n            '电机': '2207 1850KV',\r\n            '电池': '6S 1300mAh'\r\n          }\r\n        },\r\n        {\r\n          id: 'fpv-c1',\r\n          type: 'cinewhoop',\r\n          name: 'C1 电影穿越机',\r\n          description: '电影级穿越无人机，带有螺旋桨保护罩，适合室内拍摄和人物跟随',\r\n          image: '/images/products/drones/fpv-c1.jpg',\r\n          specifications: {\r\n            '机身尺寸': '95mm',\r\n            '重量(不含电池)': '280g',\r\n            '最大速度': '80km/h',\r\n            '续航时间': '6分钟',\r\n            '图传系统': '数字高清',\r\n            '控制距离': '1km',\r\n            '飞控': 'F4 MPU6000',\r\n            '电调': '20A BLHeli_S',\r\n            '电机': '1404 3800KV',\r\n            '电池': '4S 850mAh'\r\n          }\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    filteredDrones() {\r\n      if (this.activeFilter === 'all') {\r\n        return this.drones;\r\n      }\r\n      return this.drones.filter(drone => drone.type === this.activeFilter);\r\n    }\r\n  },\r\n  methods: {\r\n    setFilter(filter) {\r\n      this.activeFilter = filter;\r\n    },\r\n    getMainSpecs(drone) {\r\n      const keys = ['机身尺寸', '重量(不含电池)', '最大速度', '续航时间'];\r\n      return Object.fromEntries(\r\n        Object.entries(drone.specifications)\r\n          .filter(([key]) => keys.includes(key))\r\n      );\r\n    },\r\n    getTypeName(type) {\r\n      const names = {\r\n        'racing': '竞速型',\r\n        'freestyle': '特技型',\r\n        'cinewhoop': '电影机'\r\n      };\r\n      return names[type] || '穿越机';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.fpv-drone-page {\r\n  min-height: 80vh;\r\n}\r\n\r\n.product-card {\r\n  border-radius: 12px;\r\n  overflow: hidden;\r\n  box-shadow: 0 5px 15px rgba(0,0,0,0.08);\r\n  position: relative;\r\n  transition: all 0.3s ease;\r\n  margin-bottom: 30px;\r\n  height: 100%;\r\n}\r\n\r\n.product-card:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: 0 10px 25px rgba(0,0,0,0.15);\r\n}\r\n\r\n.product-image-container {\r\n  height: 250px;\r\n  overflow: hidden;\r\n}\r\n\r\n.product-image-container img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n  transition: all 0.5s ease;\r\n}\r\n\r\n.product-card:hover .product-image-container img {\r\n  transform: scale(1.1);\r\n}\r\n\r\n.product-overlay {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: linear-gradient(to top, rgba(0,0,0,0.8), transparent);\r\n  opacity: 0;\r\n  transition: all 0.3s ease;\r\n  display: flex;\r\n  align-items: flex-end;\r\n  padding: 20px;\r\n}\r\n\r\n.product-card:hover .product-overlay {\r\n  opacity: 1;\r\n}\r\n\r\n.product-content {\r\n  color: white;\r\n}\r\n\r\n.product-badge {\r\n  display: inline-block;\r\n  padding: 5px 10px;\r\n  border-ra