ransform: translateY(-6px);\n}\n.card-header {\n  position: relative;\n  padding: 0;\n}\n.product-badge {\n  position: absolute;\n  left: 0;\n  top: 0;\n  z-index: 2;\n  padding: 0.25rem 1.2rem;\n  border-radius: 0 0 16px 0;\n  font-size: 1rem;\n  font-weight: 600;\n  height: 2.2rem;\n  display: flex;\n  align-items: center;\n}\n.badge {\n  background: rgb(178,0,0);\n  color: #fff;\n}\n.badge-gray { background: #888; }\n.badge-green { background: #27ae60; }\n.badge-yellow { background: #ffc107; color: #b20000; }\n.badge-red { background: rgb(178,0,0); }\n.product-image-wrapper {\n  width: 100%;\n  height: 380px;\n  position: relative;\n  overflow: hidden;\n  border-bottom: 2px solid rgb(178,0,0);\n}\n.product-image {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  display: block;\n}\n.image-mask {\n  position: absolute;\n  left: 0; top: 0; right: 0; bottom: 0;\n  background: rgba(178,0,0,0.08);\n  pointer-events: none;\n}\n.card-body {\n  flex: 1;\n  padding: 2rem 1.5rem 1rem 1.5rem;\n  display: flex;\n  flex-direction: column;\n  gap: 0.5rem;\n}\n.product-name {\n  font-size: 1.4rem;\n  font-weight: 700;\n  color: #222;\n  margin-bottom: 0.5rem;\n}\n.product-description {\n  color: #b20000;\n  font-size: 1.05rem;\n  margin-bottom: 0.5rem;\n  font-weight: 500;\n}\n.product-specs {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 0.5rem 1.5rem;\n  margin-bottom: 0.5rem;\n}\n.spec-item {\n  font-size: 0.98rem;\n  color: #444;\n  background: #f8f9fa;\n  border-radius: 8px;\n  padding: 0.3rem 0.8rem;\n  min-width: 120px;\n  display: flex;\n  justify-content: space-between;\n}\n.spec-label {\n  color: #b20000;\n  font-weight: 500;\n}\n.spec-value {\n  color: #222;\n  font-weight: 600;\n}\n.card-footer {\n  padding: 1rem 1.5rem;\n  background: #fff;\n  border-top: 1px solid #f3f3f3;\n  display: flex;\n  justify-content: flex-end;\n}\n.btn {\n  display: inline-flex;\n  align-items: center;\n  gap: 0.5rem;\n  padding: 0.5rem 1.2rem;\n  border-radius: 8px;\n  font-weight: 600;\n  font-size: 1rem;\n  border: none;\n  cursor: pointer;\n  transition: all 0.2s;\n}\n.btn-outline-red {\n  color: rgb(178,0,0);\n  border: 2px solid rgb(178,0,0);\n  background: #fff;\n}\n.btn-outline-red:hover {\n  background: rgb(178,0,0);\n  color: #fff;\n}\n@media (max-width: 900px) {\n  .product-image-wrapper { height: 220px; }\n  .card-body { padding: 1.2rem 1rem 0.7rem 1rem; }\n}\n@media (max-width: 600px) {\n  .product-grid { grid-template-columns: 1fr; gap: 1.2rem; }\n  .product-image-wrapper { height: 160px; }\n  .card-body { padding: 1rem 0.7rem 0.5rem 0.7rem; }\n  .card-footer { padding: 0.7rem 0.7rem; }\n}\n</style>\n\n\n"],"mappings":";AAmFA,eAAe;EACbA,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CACN;QACEC,EAAE,EAAE,YAAY;QAChBH,IAAI,EAAE,eAAe;QACrBI,QAAQ,EAAE,KAAK;QACfC,WAAW,EAAE,kBAAkB;QAC/BC,KAAK,EAAE,4CAA4C;QACnDC,KAAK,EAAE;UACL,QAAQ,EAAE,MAAM;UAChB,MAAM,EAAE,KAAK;UACb,MAAM,EAAE,MAAM;UACd,QAAQ,EAAE;QACZ;MACF,CAAC,EACD;QACEJ,EAAE,EAAE,WAAW;QACfH,IAAI,EAAE,eAAe;QACrBI,QAAQ,EAAE,KAAK;QACfC,WAAW,EAAE,eAAe;QAC5BC,KAAK,EAAE,2CAA2C;QAClDC,KAAK,EAAE;UACL,QAAQ,EAAE,MAAM;UAChB,MAAM,EAAE,KAAK;UACb,MAAM,EAAE,MAAM;UACd,QAAQ,EAAE;QACZ;MACF,CAAC,EACD;QACEJ,EAAE,EAAE,WAAW;QACfH,IAAI,EAAE,cAAc;QACpBI,QAAQ,EAAE,KAAK;QACfC,WAAW,EAAE,cAAc;QAC3BC,KAAK,EAAE,2CAA2C;QAClDC,KAAK,EAAE;UACL,QAAQ,EAAE,MAAM;UAChB,MAAM,EAAE,KAAK;UACb,MAAM,EAAE,OAAO;UACf,QAAQ,EAAE;QACZ;MACF,CAAC,EACD;QACEJ,EAAE,EAAE,WAAW;QACfH,IAAI,EAAE,eAAe;QACrBI,QAAQ,EAAE,KAAK;QACfC,WAAW,EAAE,aAAa;QAC1BC,KAAK,EAAE,2CAA2C;QAClDC,KAAK,EAAE;UACL,QAAQ,EAAE,KAAK;UACf,MAAM,EAAE,KAAK;UACb,MAAM,EAAE,MAAM;UACd,QAAQ,EAAE;QACZ;MACF;IAEJ;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAACC,KAAK,EAAE;MACnB;MACA,MAAMC,OAAM,GAAI,CAAC,WAAW,EAAE,YAAY,EAAE,aAAa,EAAE,cAAc;MACzE,OAAOA,OAAO,CAACD,KAAI,GAAIC,OAAO,CAACC,MAAM;IACvC,CAAC;IACDC,UAAUA,CAACC,OAAO,EAAE;MAClB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,gCAAgCF,OAAO,EAAE;IAC7D;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies"