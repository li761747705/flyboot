{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      activeTab: 'agriculture'\n    };\n  },\n  mounted() {\n    // 初始化Bootstrap的选项卡功能\n    this.initTabFunctionality();\n    // 为窗口添加加载事件，确保在bootstrap完全加载后初始化\n    window.addEventListener('load', this.initTabFunctionality);\n\n    // 处理手动点击导航\n    this.setupManualNavigation();\n  },\n  beforeUnmount() {\n    // 组件销毁前移除事件监听\n    window.removeEventListener('load', this.initTabFunctionality);\n\n    // 移除手动导航事件监听\n    const navLinks = document.querySelectorAll('.app-tabs-nav .nav-link');\n    navLinks.forEach(link => {\n      link.removeEventListener('click', this.handleNavClick);\n    });\n  },\n  methods: {\n    // 初始化标签功能\n    initTabFunctionality() {\n      if (typeof window !== 'undefined' && window.bootstrap) {\n        const tabElms = document.querySelectorAll('.nav-link[data-bs-toggle=\"pill\"]');\n        tabElms.forEach(tabEl => {\n          tabEl.addEventListener('shown.bs.tab', event => {\n            this.activeTab = event.target.id.replace('-tab', '');\n          });\n        });\n\n        // 确保初始化时显示第一个标签\n        setTimeout(() => {\n          this.switchTab('agriculture');\n        }, 100);\n      }\n    },\n    // 手动切换选项卡\n    switchTab(tabId) {\n      this.activeTab = tabId;\n      // 找到对应的选项卡元素并激活它\n      if (typeof window !== 'undefined' && window.bootstrap) {\n        const tabEl = document.getElementById(tabId + '-tab');\n        if (tabEl) {\n          try {\n            const tab = new window.bootstrap.Tab(tabEl);\n            tab.show();\n\n            // 手动更新内容区域的显示\n            const contentPanes = document.querySelectorAll('.tab-pane');\n            contentPanes.forEach(pane => {\n              if (pane.id === tabId) {\n                pane.classList.add('show', 'active');\n              } else {\n                pane.classList.remove('show', 'active');\n              }\n            });\n          } catch (error) {\n            console.error('切换选项卡时出错:', error);\n          }\n        }\n      }\n    },\n    // 设置手动导航点击事件\n    setupManualNavigation() {\n      setTimeout(() => {\n        const navLinks = document.querySelectorAll('.app-tabs-nav .nav-link');\n        navLinks.forEach(link => {\n          link.addEventListener('click', this.handleNavClick);\n        });\n      }, 300);\n    },\n    // 处理导航点击\n    handleNavClick(event) {\n      event.preventDefault();\n      const tabId = event.target.id.replace('-tab', '');\n      this.switchTab(tabId);\n    }\n  }\n};","map":{"version":3,"names":["name","data","activeTab","mounted","initTabFunctionality","window","addEventListener","setupManualNavigation","beforeUnmount","removeEventListener","navLinks","document","querySelectorAll","forEach","link","handleNavClick","methods","bootstrap","tabElms","tabEl","event","target","id","replace","setTimeout","switchTab","tabId","getElementById","tab","Tab","show","contentPanes","pane","classList","add","remove","error","console","preventDefault"],"sources":["C:\\Users\\75132\\Desktop\\flyboot\\flyboot\\src\\views\\Home.vue"],"sourcesContent":["<template>\n  <div class=\"home-page-wrapper\">\n    <!-- 顶部大图与主标题 -->\n    <section class=\"hero-section position-relative overflow-hidden w-100 px-0 d-flex justify-content-center align-items-center\">\n      <div id=\"heroCarousel\" class=\"carousel slide w-100\" data-bs-ride=\"carousel\">\n        <div class=\"carousel-indicators\">\n          <button type=\"button\" data-bs-target=\"#heroCarousel\" data-bs-slide-to=\"0\" class=\"active\"></button>\n          <button type=\"button\" data-bs-target=\"#heroCarousel\" data-bs-slide-to=\"1\"></button>\n          <button type=\"button\" data-bs-target=\"#heroCarousel\" data-bs-slide-to=\"2\"></button>\n        </div>\n        <div class=\"carousel-inner\">\n          <!-- 轮播项1: 创新无人机技术 -->\n          <div class=\"carousel-item active\">\n            <div class=\"carousel-content\">\n              <div class=\"container position-relative z-2\">\n                <div class=\"row align-items-center justify-content-center min-vh-50\">\n                  <div class=\"col-12 col-md-7 py-4 py-md-5 text-center text-md-start\">\n                    <h1 class=\"display-3 fw-bold mb-3 text-white\">创新无人机技术，引领智能飞行</h1>\n                    <h2 class=\"h4 fw-normal text-white mb-4\">无人机提供专业级无人机解决方案，广泛应用于农业、测绘、安防等多个领域，助力行业智能化升级</h2>\n                    <a href=\"/products\" class=\"btn btn-light btn-lg px-md-5 py-md-2 px-4 py-2 rounded-pill shadow-sm\">查看产品</a>\n                  </div>\n                  <div class=\"col-12 col-md-5 d-flex justify-content-center align-items-center\">\n                    <div class=\"image-container d-none d-md-block\">\n                      <img src=\"/images/home/drone-hero.jpg\" alt=\"无人机\" class=\"img-fluid hero-drone-img rounded\"/>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          \n          <!-- 轮播项2: F15无人机 -->\n          <div class=\"carousel-item\">\n            <div class=\"carousel-content\">\n              <div class=\"container position-relative z-2\">\n                <div class=\"row align-items-center justify-content-center min-vh-50\">\n                  <div class=\"col-12 col-md-7 py-4 py-md-5 text-center text-md-start\">\n                    <h1 class=\"display-3 fw-bold mb-3 text-white\">{{ $t('home.carousel.f15.title') }}</h1>\n                    <h2 class=\"h2 fw-normal text-white mb-4\">{{ $t('home.carousel.f15.subtitle') }}</h2>\n                    <router-link to=\"/products/platform/f15\" class=\"btn btn-light btn-lg px-md-5 py-md-2 px-4 py-2 rounded-pill shadow-sm\">{{ $t('home.carousel.f15.btn') }}</router-link>\n                  </div>\n                  <div class=\"col-12 col-md-5 d-flex justify-content-center align-items-center\">\n                    <div class=\"image-container d-none d-md-block\">\n                      <img src=\"/images/products/f15/main.svg\" alt=\"F15无人机\" class=\"img-fluid hero-drone-img\"/>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          \n          <!-- 轮播项3: 影S3无人机 -->\n          <div class=\"carousel-item\">\n            <div class=\"carousel-content\">\n              <div class=\"container position-relative z-2\">\n                <div class=\"row align-items-center justify-content-center min-vh-50\">\n                  <div class=\"col-12 col-md-7 py-4 py-md-5 text-center text-md-start\">\n                    <h1 class=\"display-3 fw-bold mb-3 text-white\">{{ $t('home.carousel.yings3.title') }}</h1>\n                    <h2 class=\"h2 fw-normal text-white mb-4\">{{ $t('home.carousel.yings3.subtitle') }}</h2>\n                    <router-link to=\"/products/platform/ying-s3\" class=\"btn btn-light btn-lg px-md-5 py-md-2 px-4 py-2 rounded-pill shadow-sm\">{{ $t('home.carousel.yings3.btn') }}</router-link>\n                  </div>\n                  <div class=\"col-12 col-md-5 d-flex justify-content-center align-items-center\">\n                    <div class=\"image-container d-none d-md-block\">\n                      <img src=\"/images/products/ying-s3/main.jpg\" alt=\"影S3无人机\" class=\"img-fluid hero-drone-img rounded\"/>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n        \n        <button class=\"carousel-control-prev\" type=\"button\" data-bs-target=\"#heroCarousel\" data-bs-slide=\"prev\">\n          <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\n          <span class=\"visually-hidden\">{{ $t('home.previous') }}</span>\n        </button>\n        <button class=\"carousel-control-next\" type=\"button\" data-bs-target=\"#heroCarousel\" data-bs-slide=\"next\">\n          <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\n          <span class=\"visually-hidden\">{{ $t('home.next') }}</span>\n        </button>\n      </div>\n      <!-- 背景渐变 -->\n      <div class=\"hero-bg\"></div>\n    </section>\n\n    <!-- 底部统计数据栏 -->\n    <!-- <section class=\"stats-bar text-white py-4 w-100 px-0\" style=\"margin-top:0;\">\n      <div class=\"container\">\n        <div class=\"row text-center\">\n          <div class=\"col-6 col-md-3 mb-3 mb-md-0\">\n            <div class=\"stats-number\">10年</div>\n            <div class=\"stats-label\">2015年-至今</div>\n          </div>\n          <div class=\"col-6 col-md-3 mb-3 mb-md-0\">\n            <div class=\"stats-number\">1,000,000+公里</div>\n            <div class=\"stats-label\">安全飞行</div> \n          </div>\n          <div class=\"col-6 col-md-3\">\n            <div class=\"stats-number\">100+家</div>\n            <div class=\"stats-label\">服务客户</div>\n          </div>\n          <div class=\"col-6 col-md-3\">\n            <div class=\"stats-number\">100,000+小时</div>\n            <div class=\"stats-label\">飞行时长</div>\n          </div>\n        </div>\n      </div>\n    </section> -->\n\n\n      <!-- 热门机型 -->\n      <section class=\"industry-applications py-responsive bg-light\">\n      <div class=\"container\">\n        <h2 class=\"text-center mb-4 mb-md-5 fw-bold\">{{ $t('home.popularModels') }}</h2>\n        <div class=\"row g-4 mb-5\">\n          <div class=\"col-md-6 col-lg-4\">\n            <div class=\"application-card bg-white rounded shadow-sm p-4 h-100\">\n              <h3 class=\"fw-bold mb-3\">{{ $t('home.defenseApp.title') }}</h3>\n              <p class=\"mb-4\">{{ $t('home.defenseApp.desc') }}</p>\n              <h4 class=\"h5 mb-3\">{{ $t('home.defenseApp.advantage') }}</h4>\n              <ul class=\"list-unstyled mb-4\">\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.defenseApp.features.portable') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.defenseApp.features.deploy') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.defenseApp.features.wind') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.defenseApp.features.payload') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.defenseApp.features.environment') }}</li>\n              </ul>\n              <router-link to=\"/applications/defense\" class=\"btn btn-outline-primary rounded-pill\">{{ $t('home.defenseApp.more') }}</router-link>\n            </div>\n          </div>\n          <div class=\"col-md-6 col-lg-4\">\n            <div class=\"application-card bg-white rounded shadow-sm p-4 h-100\">\n              <h3 class=\"fw-bold mb-3\">{{ $t('home.publicSafety.title') }}</h3>\n              <p class=\"mb-4\">{{ $t('home.publicSafety.desc') }}</p>\n              <h4 class=\"h5 mb-3\">{{ $t('home.publicSafety.advantage') }}</h4>\n              <ul class=\"list-unstyled mb-4\">\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.publicSafety.features.longFlight') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.publicSafety.features.deploy') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.publicSafety.features.wind') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.publicSafety.features.payload') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.publicSafety.features.environment') }}</li>\n              </ul>\n              <router-link to=\"/applications/public-safety\" class=\"btn btn-outline-primary rounded-pill\">{{ $t('home.publicSafety.more') }}</router-link>\n            </div>\n          </div>\n          <div class=\"col-md-6 col-lg-4\">\n            <div class=\"application-card bg-white rounded shadow-sm p-4 h-100\">\n              <h3 class=\"fw-bold mb-3\">{{ $t('home.publicSafety.title') }}</h3>\n              <p class=\"mb-4\">{{ $t('home.publicSafety.desc') }}</p>\n              <h4 class=\"h5 mb-3\">{{ $t('home.publicSafety.advantage') }}</h4>\n              <ul class=\"list-unstyled mb-4\">\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.publicSafety.features.longFlight') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.publicSafety.features.deploy') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.publicSafety.features.wind') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.publicSafety.features.payload') }}</li>\n                <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>{{ $t('home.publicSafety.features.environment') }}</li>\n              </ul>\n              <router-link to=\"/applications/public-safety\" class=\"btn btn-outline-primary rounded-pill\">{{ $t('home.publicSafety.more') }}</router-link>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    \n    <!-- 行业应用区 -->\n    <section class=\"industry-applications py-responsive\">\n      <div class=\"container\">\n        <h2 class=\"text-center mb-4 mb-md-5 fw-bold\">{{ $t('home.applications.title') }}</h2>\n        \n        <!-- 行业应用选项卡导航 -->\n        <div class=\"app-tabs-nav mb-4\">\n          <div class=\"nav nav-pills nav-fill flex-column flex-md-row\">\n            <a class=\"nav-link mb-2 mb-md-0\" :class=\"{ active: activeTab === 'agriculture' }\" id=\"agriculture-tab\" @click.prevent=\"switchTab('agriculture')\" data-bs-toggle=\"pill\" href=\"#agriculture\" role=\"tab\">{{ $t('home.applications.agriculture') }}</a>\n            <a class=\"nav-link mb-2 mb-md-0\" :class=\"{ active: activeTab === 'survey' }\" id=\"survey-tab\" @click.prevent=\"switchTab('survey')\" data-bs-toggle=\"pill\" href=\"#survey\" role=\"tab\">{{ $t('home.applications.survey') }}</a>\n            <a class=\"nav-link mb-2 mb-md-0\" :class=\"{ active: activeTab === 'mapping' }\" id=\"mapping-tab\" @click.prevent=\"switchTab('mapping')\" data-bs-toggle=\"pill\" href=\"#mapping\" role=\"tab\">{{ $t('home.applications.mapping') }}</a>\n            <a class=\"nav-link mb-2 mb-md-0\" :class=\"{ active: activeTab === 'monitoring' }\" id=\"monitoring-tab\" @click.prevent=\"switchTab('monitoring')\" data-bs-toggle=\"pill\" href=\"#monitoring\" role=\"tab\">{{ $t('home.applications.monitoring') }}</a>\n            <a class=\"nav-link\" :class=\"{ active: activeTab === 'power' }\" id=\"power-tab\" @click.prevent=\"switchTab('power')\" data-bs-toggle=\"pill\" href=\"#power\" role=\"tab\">{{ $t('home.applications.power') }}</a>\n          </div>\n        </div>\n        \n        <!-- 行业应用内容区 -->\n        <div class=\"tab-content\">\n          <!-- 农业植保 -->\n          <div class=\"tab-pane fade\" :class=\"{ 'show active': activeTab === 'agriculture' }\" id=\"agriculture\" role=\"tabpanel\" aria-labelledby=\"agriculture-tab\">\n            <div class=\"row g-4 align-items-center\">\n              <div class=\"col-lg-6 order-2 order-lg-1\">\n                <div class=\"app-card h-100\">\n                  <div class=\"app-card-content\">\n                    <h3 class=\"app-card-title\">{{ $t('home.agricultureTab.title') }}</h3>\n                    <p class=\"app-card-desc\">{{ $t('home.agricultureTab.desc') }}</p>\n                    <div class=\"app-card-features\">\n                      <div class=\"feature-item\">\n                        <i class=\"bi bi-check2-circle\"></i>\n                        <span>{{ $t('home.agricultureTab.features.efficiency') }}</span>\n                      </div>\n                      <div class=\"feature-item\">\n                        <i class=\"bi bi-check2-circle\"></i>\n                        <span>{{ $t('home.agricultureTab.features.cost') }}</span>\n                      </div>\n                      <div class=\"feature-item\">\n                        <i class=\"bi bi-check2-circle\"></i>\n                        <span>{{ $t('home.agricultureTab.features.precision') }}</span>\n                      </div>\n                      <div class=\"feature-item\">\n                        <i class=\"bi bi-check2-circle\"></i>\n                        <span>{{ $t('home.agricultureTab.features.coverage') }}</span>\n                      </div>\n                    </div>\n                    <div class=\"app-card-action\">\n                      <router-link to=\"/applications/agriculture\" class=\"btn-more\">\n                        了解更多 <i class=\"bi bi-arrow-right\"></i>\n                      </router-link>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-lg-6 order-1 order-lg-2\">\n                <div class=\"app-card-image\">\n                  <img src=\"/images/applications/agriculture.jpg\" alt=\"农业植保\" class=\"img-fluid rounded shadow-sm\">\n                </div>\n              </div>\n            </div>\n          </div>\n          \n          <!-- 勘探 -->\n          <div class=\"tab-pane fade\" :class=\"{ 'show active': activeTab === 'survey' }\" id=\"survey\" role=\"tabpanel\" aria-labelledby=\"survey-tab\">\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <div class=\"app-card h-100\">\n                  <div class=\"app-card-content\">\n                    <h3 class=\"app-card-title\">勘探</h3>\n                    <p class=\"app-card-desc\">矿区勘测 | 地质勘探 | 油气田勘探 | 资源调查 | 地形勘测</p>\n                    <div class=\"app-card-features\">\n                      <div class=\"feature-item\">\n                        <i class=\"bi bi-check2-circle\"></i>\n                        <span>减少人员风险</span>\n                      </div>\n                      <div class=\"feature-item\">\n                        <i class=\"bi bi-check2-circle\"></i>\n                        <span>提高勘探精度</span>\n                      </div>\n                      <div class=\"feature-item\">\n                        <i class=\"bi bi-check2-circle\"></i>\n                        <span>缩短作业周期</span>\n                      </div>\n                      <div class=\"feature-item\">\n                        <i class=\"bi bi-check2-circle\"></i>\n                        <span>降低勘探成本</span>\n                      </div>\n                    </div>\n                    <div class=\"app-card-action\">\n                      <router-link to=\"/applications/survey\" class=\"btn-more\">\n                        了解更多 <i class=\"bi bi-arrow-right\"></i>\n                      </router-link>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-md-6\">\n                <div class=\"app-card-image\">\n                  <img src=\"/images/applications/survey.jpg\" alt=\"勘探\" class=\"img-fluid\">\n                </div>\n              </div>\n            </div>\n          </div>\n          \n          <!-- 测绘 -->\n          <div class=\"tab-pane fade\" :class=\"{ 'show active': activeTab === 'mapping' }\" id=\"mapping\" role=\"tabpanel\" aria-labelledby=\"mapping-tab\">\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <div class=\"app-card h-100\">\n                  <div class=\"app-card-content\">\n                    <h3 class=\"app-card-title\">测绘</h3>\n                    <p class=\"app-card-desc\">航空摄影测量 | 地形图测绘 | 三维建模 | 实景建模 | 城市规划</p>\n                    <div class=\"app-card-features\">\n                      <div class=\"feature-item\">\n                    