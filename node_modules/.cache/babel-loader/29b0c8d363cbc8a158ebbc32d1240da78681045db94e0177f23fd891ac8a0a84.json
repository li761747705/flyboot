{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'FPVDrone',\n  data() {\n    return {\n      drones: [{\n        id: 'fpv-p1',\n        name: 'P1 竞速穿越机',\n        category: '竞速型',\n        image: '/images/products/fpv/02.png'\n      }, {\n        id: 'fpv-p2x',\n        name: 'P2X 特技穿越机',\n        category: '特技型',\n        image: '/images/products/fpv/02.png'\n      }, {\n        id: 'fpv-c1',\n        name: 'C1 电影穿越机',\n        category: '电影级',\n        image: '/images/products/fpv/02.png'\n      }, {\n        id: 'fpv-x4',\n        name: 'X4 四轴穿越机',\n        category: '入门型',\n        image: '/images/products/fpv/02.png'\n      }],\n      isLoading: false\n    };\n  },\n  methods: {\n    getBadgeClass(index) {\n      const classes = ['badge-primary', 'badge-secondary', 'badge-success', 'badge-warning'];\n      return classes[index % classes.length];\n    },\n    async goToDetail(droneId) {\n      this.isLoading = true;\n      try {\n        await this.$router.push(`/products/details/fpv/${droneId}`);\n      } catch (error) {\n        console.error('Navigation error:', error);\n      } finally {\n        this.isLoading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","drones","id","category","image","isLoading","methods","getBadgeClass","index","classes","length","goToDetail","droneId","$router","push","error","console"],"sources":["D:\\Hys\\flyboot1\\flyboot\\src\\views\\products\\FPV.vue"],"sourcesContent":["<template>\n  <div class=\"product-page fpv-page\">\n    <section class=\"hero-section\">\n      <div class=\"hero-background\">\n        <div class=\"hero-pattern\"></div>\n        <div class=\"hero-overlay\">\n          <div class=\"container\">\n            <div class=\"hero-content text-center\">\n              <div class=\"hero-badge\">\n                <i class=\"bi bi-camera-video\"></i>\n                <span>FPV穿越无人机</span>\n              </div>\n              <h1 class=\"hero-title\">穿越无人机</h1>\n              <p class=\"hero-subtitle\">体验极速穿越的激情，感受第一人称视角的震撼</p>\n              <div class=\"hero-features\">\n                <div class=\"feature-item\">\n                  <i class=\"bi bi-speedometer2\"></i>\n                  <span>高速穿越</span>\n                </div>\n                <div class=\"feature-item\">\n                  <i class=\"bi bi-camera-video-fill\"></i>\n                  <span>高清FPV</span>\n                </div>\n                <div class=\"feature-item\">\n                  <i class=\"bi bi-stars\"></i>\n                  <span>竞速特技</span>\n                </div>\n                <div class=\"feature-item\">\n                  <i class=\"bi bi-film\"></i>\n                  <span>航拍创作</span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n    <section class=\"products-section\">\n      <div class=\"container\">\n        <div class=\"section-header text-center mb-5\">\n          <h2 class=\"section-title\">产品系列</h2>\n          <p class=\"section-subtitle\">专业级FPV穿越无人机，满足不同场景需求</p>\n        </div>\n        <div class=\"product-grid\">\n          <div class=\"product-card\" v-for=\"(drone, index) in drones\" :key=\"drone.id\" @click=\"goToDetail(drone.id)\" :style=\"{backgroundImage: `url(${drone.image})`}\" >\n            <div class=\"card-header\">\n              <div class=\"product-badge\" :class=\"getBadgeClass(index)\">{{ drone.category }}</div>\n            </div>\n            <div class=\"card-body\">\n            </div>\n            <div class=\"card-footer\">\n              <h3 class=\"product-name\">{{ drone.name }}</h3>\n              <button class=\"btn btn-primary btn-sm\">查看详情 <i class=\"bi bi-arrow-right\"></i></button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n    <div class=\"loading-overlay\" v-if=\"isLoading\">\n      <div class=\"loading-content\">\n        <div class=\"loading-spinner\"></div>\n        <p>正在加载产品信息...</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'FPVDrone',\n  data() {\n    return {\n      drones: [\n        {\n          id: 'fpv-p1',\n          name: 'P1 竞速穿越机',\n          category: '竞速型',\n          image: '/images/products/fpv/02.png',\n        },\n        {\n          id: 'fpv-p2x',\n          name: 'P2X 特技穿越机',\n          category: '特技型',\n          image: '/images/products/fpv/02.png',\n        },\n        {\n          id: 'fpv-c1',\n          name: 'C1 电影穿越机',\n          category: '电影级',\n          image: '/images/products/fpv/02.png',\n        },\n        {\n          id: 'fpv-x4',\n          name: 'X4 四轴穿越机',\n          category: '入门型',\n          image: '/images/products/fpv/02.png',\n        }\n      ],\n      isLoading: false\n    }\n  },\n  methods: {\n    getBadgeClass(index) {\n      const classes = ['badge-primary', 'badge-secondary', 'badge-success', 'badge-warning']\n      return classes[index % classes.length]\n    },\n    async goToDetail(droneId) {\n      this.isLoading = true\n      try {\n        await this.$router.push(`/products/details/fpv/${droneId}`)\n      } catch (error) {\n        console.error('Navigation error:', error)\n      } finally {\n        this.isLoading = false\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.product-grid {\n  display: grid;\n  grid-template-columns: repeat(2, 1fr);\n  gap: 40px;\n  justify-content: center;\n  max-width: 900px;\n  margin: 0 auto;\n}\n@media (max-width: 900px) {\n  .product-grid {\n    grid-template-columns: 1fr;\n    max-width: 100%;\n    gap: 24px;\n  }\n}\n.product-card {\n  width: 577px;\n  height: 577px;\n  background: #fff;\n  border: none;\n  border-radius: 18px;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: flex-start;\n  position: relative;\n  box-shadow: none;\n  transition: box-shadow 0.2s;\n  margin-bottom: 0;\n  padding: 0 32px;\n  background-size: auto;\n  background-position: center;\n  background-repeat: no-repeat;\n}\n.card-header {\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-top: 38px;\n  margin-bottom: 18px;\n}\n.product-badge {\n  display: inline-block;\n  margin: 0 auto 2.2rem auto;\n  padding: 4px 22px;\n  border-radius: 16px;\n  font-size: 1.08rem;\n  font-weight: 600;\n  background: linear-gradient(90deg, #eaf6ff 0%, #d2eafd 100%);\n  color: #1cb0f6;\n  border: 1.5px solid #b6e2fa;\n  text-align: center;\n  position: static;\n  box-shadow: 0 2px 8px rgba(28,176,246,0.08);\n  letter-spacing: 1px;\n  transition: box-shadow 0.2s, border-color 0.2s;\n  margin-bottom: 10px;\n}\n.product-badge:hover {\n  border-color: #1cb0f6;\n  box-shadow: 0 4px 16px rgba(28,176,246,0.15);\n}\n.card-footer {\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  margin-bottom: 32px;\n}\n.product-name {\n  font-size: 1.45rem;\n  font-weight: 700;\n  color: #222;\n  margin-bottom: 10px;\n  text-align: center;\n  letter-spacing: 1px;\n}\n.product-card .btn {\n  border: 1.5px solid rgb(178,0,0) !important;\n  border-radius: 16px;\n  background: #fff !important;\n  color: rgb(178,0,0) !important;\n  font-size: 0.95rem;\n  padding: 0.75rem 2.5rem !important;\n  font-weight: 500;\n  box-shadow: none;\n  transition: all 0.2s;\n}\n.product-card .btn:hover {\n  background: rgb(178,0,0) !important;\n  color: #fff !important;\n}\n.section-line, .section-title:after {\n  background: rgb(178,0,0) !important;\n}\n</style>\n\n"],"mappings":";AAoEA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CACN;QACEC,EAAE,EAAE,QAAQ;QACZH,IAAI,EAAE,UAAU;QAChBI,QAAQ,EAAE,KAAK;QACfC,KAAK,EAAE;MACT,CAAC,EACD;QACEF,EAAE,EAAE,SAAS;QACbH,IAAI,EAAE,WAAW;QACjBI,QAAQ,EAAE,KAAK;QACfC,KAAK,EAAE;MACT,CAAC,EACD;QACEF,EAAE,EAAE,QAAQ;QACZH,IAAI,EAAE,UAAU;QAChBI,QAAQ,EAAE,KAAK;QACfC,KAAK,EAAE;MACT,CAAC,EACD;QACEF,EAAE,EA