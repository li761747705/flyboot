or: #fff;\n}\n\n.btn-primary:hover {\n  background: #0056b3;\n  transform: translateY(-1px);\n}\n\n@keyframes patternMove {\n  0%, 100% { transform: translate(0, 0); }\n  50% { transform: translate(-10px, -10px); }\n}\n\n/* 响应式设计 */\n@media (max-width: 768px) {\n  .hero-section {\n    height: 400px;\n  }\n  \n  .hero-title {\n    font-size: 2.5rem;\n  }\n  \n  .hero-features {\n    gap: 1rem;\n  }\n  \n  .feature-item {\n    padding: 0.5rem 1rem;\n    font-size: 0.9rem;\n  }\n  \n  .product-grid {\n    grid-template-columns: 1fr;\n    gap: 1.5rem;\n    max-width: 100%;\n  }\n  \n  .section-title {\n    font-size: 2rem;\n  }\n}\n</style>\n\n\n"],"mappings":";AAoFA,eAAe;EACbA,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CACN;QACEC,EAAE,EAAE,YAAY;QAChBH,IAAI,EAAE,eAAe;QACrBI,QAAQ,EAAE,KAAK;QACfC,WAAW,EAAE,kBAAkB;QAC/BC,KAAK,EAAE;UACL,QAAQ,EAAE,MAAM;UAChB,MAAM,EAAE,KAAK;UACb,MAAM,EAAE,MAAM;UACd,QAAQ,EAAE;QACZ;MACF,CAAC,EACD;QACEH,EAAE,EAAE,WAAW;QACfH,IAAI,EAAE,eAAe;QACrBI,QAAQ,EAAE,KAAK;QACfC,WAAW,EAAE,eAAe;QAC5BC,KAAK,EAAE;UACL,QAAQ,EAAE,MAAM;UAChB,MAAM,EAAE,KAAK;UACb,MAAM,EAAE,MAAM;UACd,QAAQ,EAAE;QACZ;MACF,CAAC,EACD;QACEH,EAAE,EAAE,WAAW;QACfH,IAAI,EAAE,cAAc;QACpBI,QAAQ,EAAE,KAAK;QACfC,WAAW,EAAE,cAAc;QAC3BC,KAAK,EAAE;UACL,QAAQ,EAAE,MAAM;UAChB,MAAM,EAAE,KAAK;UACb,MAAM,EAAE,OAAO;UACf,QAAQ,EAAE;QACZ;MACF,CAAC,EACD;QACEH,EAAE,EAAE,WAAW;QACfH,IAAI,EAAE,eAAe;QACrBI,QAAQ,EAAE,KAAK;QACfC,WAAW,EAAE,aAAa;QAC1BC,KAAK,EAAE;UACL,QAAQ,EAAE,KAAK;UACf,MAAM,EAAE,KAAK;UACb,MAAM,EAAE,MAAM;UACd,QAAQ,EAAE;QACZ;MACF;IAEJ;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAACC,KAAK,EAAE;MACjB,MAAMC,SAAQ,GAAI,CAChB,2CAA2C,EAC3C,2CAA2C,EAC3C,2CAA2C,EAC3C,2CAA0C,CAC5C;MACA,OAAOA,SAAS,CAACD,KAAI,GAAIC,SAAS,CAACC,MAAM;IAC3C,CAAC;IACDC,aAAaA,CAACH,KAAK,EAAE;MACnB,MAAMI,OAAM,GAAI,CAAC,eAAe,EAAE,iBAAiB,EAAE,eAAe,EAAE,eAAe;MACrF,OAAOA,OAAO,CAACJ,KAAI,GAAII,OAAO,CAACF,MAAM;IACvC,CAAC;IACDG,UAAUA,CAACC,OAAO,EAAE;MAClB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,gCAAgCF,OAAO,EAAE;IAC7D;EACF;AACF","ignoreLi