<div class=\"feature-text\">{{ feature }}</div>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    \r\n                    <h5 class=\"mb-3\">关键参数</h5>\r\n                    <div class=\"row g-3\">\r\n                      <div class=\"col-6\" v-for=\"(key, index) in getMainSpecs(selectedDrone)\" :key=\"index\">\r\n                        <div class=\"spec-box p-3 rounded bg-light\">\r\n                          <div class=\"spec-title text-secondary small\">{{ key }}</div>\r\n                          <div class=\"spec-value fw-bold\">{{ selectedDrone.specifications[key] }}</div>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              <!-- 技术参数 -->\r\n              <div class=\"tab-pane fade\" id=\"specs\" role=\"tabpanel\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-lg-6\">\r\n                    <div class=\"table-responsive\">\r\n                      <table class=\"table table-hover\">\r\n                        <thead>\r\n                          <tr>\r\n                            <th scope=\"col\" class=\"bg-light\">参数</th>\r\n                            <th scope=\"col\" class=\"bg-light\">规格</th>\r\n                          </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                          <tr v-for=\"(spec, key) in selectedDrone.specifications\" :key=\"key\">\r\n                            <td>{{ key }}</td>\r\n                            <td>{{ spec }}</td>\r\n                          </tr>\r\n                        </tbody>\r\n                      </table>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-lg-6\">\r\n                    <img :src=\"selectedDrone.image\" class=\"img-fluid rounded\" :alt=\"selectedDrone.name\">\r\