{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nexport default {\n  name: 'Platform',\n  data() {\n    return {\n      selectedDrone: null,\n      activeFilter: 'all',\n      drones: [{\n        id: 'f15',\n        type: 'fixedWing',\n        name: 'F15 电动垂起航空器',\n        description: '全新一代电动垂直起降航空器，具备长航时、大载重、高可靠性等特点，适用于长距离航线巡检和大面积测绘等任务。',\n        image: '/images/products/f15/main.jpg',\n        gallery: ['/images/products/f15/detail-1.jpg', '/images/products/f15/detail-2.jpg', '/images/products/f15/detail-3.jpg'],\n        features: ['电动垂直起降，无需跑道', '长航时飞行，续航可达120分钟', '大载重能力，最大载重5kg', '高可靠性设计，冗余保障', '智能控制系统，支持自主航线规划', '一键起飞/降落，操作简便', '模块化设计，方便维护和更换', '防尘防水设计，可在恶劣环境下作业'],\n        specifications: {\n          '最大起飞重量': '15kg',\n          '最大载重': '5kg',\n          '续航时间': '120分钟',\n          '最大飞行速度': '72km/h',\n          '巡航速度': '50km/h',\n          '最大飞行高度': '3000m',\n          '抗风能力': '6级',\n          '工作温度': '-10°C ~ 45°C',\n          '通信距离': '30km',\n          '定位精度': '水平±1.5m，垂直±0.5m'\n        },\n        applications: ['电力巡检', '测绘测量', '管道巡检', '安防监控', '应急救援', '农林植保'],\n        video: 'https://www.youtube.com/embed/dQw4w9WgXcQ'\n      }, {\n        id: 'ying-s3',\n        type: 'helicopter',\n        name: '影S3 长航时巡察无人直升机',\n        description: '专业级长航时巡察无人直升机，适用于大范围区域巡查任务，具备超长航时和全天候作业能力。',\n        image: '/images/products/ying-s3/main.jpg',\n        gallery: ['/images/products/ying-s3/detail-1.jpg', '/images/products/ying-s3/detail-2.jpg', '/images/products/ying-s3/detail-3.jpg'],\n        features: ['超长航时，单次飞行可达3小时', '高稳定性，适应复杂气象条件', '全天候作业，IP54防护等级', '智能避障系统，安全可靠', '精准定位，支持RTK厘米级定位', '多任务载荷舱，快速更换任务设备', '自主任务规划，支持预设航线执行', '实时图传，高清视频回传'],\n        specifications: {\n          '最大起飞重量': '25kg',\n          '最大载重': '8kg',\n          '续航时间': '180分钟',\n          '最大飞行速度': '90km/h',\n          '巡航速度': '60km/h',\n          '最大飞行高度': '4000m',\n          '抗风能力': '7级',\n          '工作温度': '-20°C ~ 50°C',\n          '通信距离': '50km',\n          '定位精度': '水平±0.1m，垂直±0.1m (RTK模式)'\n        },\n        applications: ['边境巡逻', '森林防火', '海洋监测', '应急救援', '电力巡检', '环境监测'],\n        video: 'https://www.youtube.com/embed/dQw4w9WgXcQ'\n      }, {\n        id: 'p2x',\n        type: 'fpv',\n        name: 'P2X 竞速无人机',\n        description: '高性能FPV竞速无人机，专为竞速和特技飞行设计，具备极佳的操控性和灵活性。',\n        image: '/images/products/p2x/main.jpg',\n        gallery: ['/images/products/p2x/detail-1.jpg', '/images/products/p2x/detail-2.jpg', '/images/products/p2x/detail-3.jpg'],\n        features: ['碳纤维一体化机身，轻量坚固', '高性能电机，强劲动力输出', '低延迟图传系统，实时响应', '高精度飞控，极致操控体验', '可自定义飞行参数，适应不同飞行风格', '快拆设计，便于维护和更换', '支持3S-6S电池，灵活选择', '内置LED灯，夜间飞行更炫酷'],\n        specifications: {\n          '机身尺寸': '220mm',\n          '重量(不含电池)': '350g',\n          '推荐电池': '4S 1500mAh',\n          '最大速度': '140km/h',\n          '电机': '2306 2450KV',\n          '飞控': 'F4 飞控',\n          '图传': '5.8GHz 600mW',\n          '接收机': 'SBUS协议',\n          '摄像头': 'FPV相机 1200TVL',\n          '续航时间': '5-8分钟'\n        },\n        applications: ['FPV竞速', '特技飞行', '航拍摄影', '娱乐休闲'],\n        video: 'https://www.youtube.com/embed/dQw4w9WgXcQ'\n      }, {\n        id: 'j10',\n        type: 'multiRotor',\n        name: 'J10 工业级多旋翼无人机',\n        description: '工业级六旋翼无人机，专为专业航测、巡检和监控任务设计，稳定可靠。',\n        image: '/images/products/j10/main.jpg',\n        gallery: ['/images/products/j10/detail-1.jpg', '/images/products/j10/detail-2.jpg', '/images/products/j10/detail-3.jpg'],\n        features: ['六轴设计，冗余保障，安全可靠', '折叠机臂，便于运输和存储', '最大载重3kg，适配多种专业设备', '双电池供电系统，安全冗余', '智能飞控系统，支持多种飞行模式', '工业级设计，耐用防护', '支持多种任务载荷，一机多用', '内置RTK定位，厘米级精度'],\n        specifications: {\n          '最大起飞重量': '10kg',\n          '最大载重': '3kg',\n          '续航时间': '45分钟',\n          '最大飞行速度': '65km/h',\n          '巡航速度': '40km/h',\n          '最大飞行高度': '2000m',\n          '抗风能力': '6级',\n          '工作温度': '-10°C ~ 45°C',\n          '通信距离': '10km',\n          '定位精度': '水平±1cm，垂直±2cm (RTK模式)'\n        },\n        applications: ['航空测绘', '电力巡检', '安防监控', '农业植保', '影视航拍', '3D建模'],\n        video: 'https://www.youtube.com/embed/dQw4w9WgXcQ'\n      }, {\n        id: 'swarm',\n        type: 'system',\n        name: '蜂群协同作业系统',\n        description: '智能集群控制系统，支持多机协同作业，实现大区域快速覆盖和复杂任务协同执行。',\n        image: '/images/products/swarm/main.jpg',\n        gallery: ['/images/products/swarm/detail-1.jpg', '/images/products/swarm/detail-2.jpg', '/images/products/swarm/detail-3.jpg'],\n        features: ['多机协同，最多支持50架无人机同时作业', '智能编队，自动保持队形', '自主避障，智能规避冲突', '任务规划，一键执行复杂任务', '实时监控，全程掌握每台设备状态', '自动化部署，快速响应', '故障自动处理，确保任务连续性', '可视化指挥系统，直观操作'],\n        specifications: {\n          '最大集群数量': '50架',\n          '控制距离': '10km',\n          '编队精度': '±0.5m',\n          '响应时间': '<100ms',\n          '任务规划': '支持',\n          '控制终端': '工业级平板/笔记本',\n          '通信方式': '自组网+4G/5G',\n          '数据存储': '本地+云端',\n          '操作人员': '1-2人',\n          '部署时间': '<5分钟'\n        },\n        applications: ['集群表演', '协同作业', '区域覆盖', '应急通信', '大范围搜索', '区域监控'],\n        video: 'https://www.youtube.com/embed/dQw4w9WgXcQ'\n      }]\n    };\n  },\n  computed: {\n    filteredDrones() {\n      if (this.activeFilter === 'all') {\n        return this.drones;\n      }\n      return this.drones.filter(drone => drone.type === this.activeFilter);\n    }\n  },\n  mounted() {\n    // 检查路由参数，判断是否需要显示特定产品\n    const id = this.$route.params.id;\n    if (id) {\n      const drone = this.drones.find(d => d.id === id);\n      if (drone) {\n        this.$nextTick(() => {\n          this.showDetails(drone);\n        });\n      }\n    }\n  },\n  methods: {\n    setFilter(filter) {\n      this.activeFilter = filter;\n    },\n    getMainSpecs(drone) {\n      // 根据产品类型获取关键参数\n      const specKeys = {\n        fixedWing: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\n        multiRotor: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\n        helicopter: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\n        fpv: ['机身尺寸', '重量(不含电池)', '最大速度', '续航时间'],\n        system: ['最大集群数量', '控制距离', '编队精度', '响应时间']\n      };\n      const type = drone.type || 'multiRotor';\n      return specKeys[type] || Object.keys(drone.specifications).slice(0, 4);\n    },\n    getBadgeClass(type) {\n      const classes = {\n        fixedWing: 'bg-primary',\n        multiRotor: 'bg-success',\n        helicopter: 'bg-danger',\n        fpv: 'bg-warning',\n        system: 'bg-info'\n      };\n      return classes[type] || 'bg-secondary';\n    },\n    getTypeName(type) {\n      const names = {\n        fixedWing: '固定翼',\n        multiRotor: '多旋翼',\n        helicopter: '直升机',\n        fpv: 'FPV',\n        system: '系统平台'\n      };\n      return names[type] || '未分类';\n    },\n    getApplicationIcon(app) {\n      const icons = {\n        '电力巡检': 'bi bi-lightning fs-3',\n        '测绘测量': 'bi bi-map fs-3',\n        '管道巡检': 'bi bi-water fs-3',\n        '安防监控': 'bi bi-shield-check fs-3',\n        '应急救援': 'bi bi-heart-pulse fs-3',\n        '农林植保': 'bi bi-tree fs-3',\n        '边境巡逻': 'bi bi-geo-alt fs-3',\n        '森林防火': 'bi bi-fire fs-3',\n        '海洋监测': 'bi bi-water fs-3',\n        '环境监测': 'bi bi-droplet-half fs-3',\n        '航空测绘': 'bi bi-rulers fs-3',\n        '农业植保': 'bi bi-flower1 fs-3',\n        '影视航拍': 'bi bi-camera fs-3',\n        '3D建模': 'bi bi-box fs-3',\n        'FPV竞速': 'bi bi-speedometer2 fs-3',\n        '特技飞行': 'bi bi-stars fs-3',\n        '航拍摄影': 'bi bi-camera-video fs-3',\n        '娱乐休闲': 'bi bi-controller fs-3',\n        '集群表演': 'bi bi-stars fs-3',\n        '协同作业': 'bi bi-people fs-3',\n        '区域覆盖': 'bi bi-grid-3x3 fs-3',\n        '应急通信': 'bi bi-broadcast-pin fs-3',\n        '大范围搜索': 'bi bi-search fs-3',\n        '区域监控': 'bi bi-eye fs-3'\n      };\n      return icons[app] || 'bi bi-check-circle fs-3';\n    },\n    getApplicationDesc(app) {\n      const descriptions = {\n        '电力巡检': '对电力线路进行巡查，检测设备异常和线路故障',\n        '测绘测量': '进行地形测绘和三维模型构建，为工程规划提供数据支持',\n        '管道巡检': '对油气管道进行巡查监测，及时发现泄漏和故障',\n        '安防监控': '对重要区域进行实时监控，提升安全防护能力',\n        '应急救援': '在灾害现场提供空中视角，协助搜救和物资投放',\n        '农林植保': '进行农田和林地的植保作业，提高效率降低成本',\n        '边境巡逻': '对边境区域进行巡检，维护国土安全',\n        '森林防火': '监测森林火情，提供早期预警和火势监控',\n        '海洋监测': '对海洋环境进行监测，为海洋资源保护提供数据支持',\n        '环境监测': '监测空气质量和环境变化，辅助环保工作',\n        '航空测绘': '进行高精度航空摄影测量，生成正射影像图和数字高程模型',\n        '农业植保': '进行精准农业作业，提高农业生产效率',\n        '影视航拍': '为影视制作提供高质量的航拍素材，展现全新视角',\n        '3D建模': '通过航拍数据生成三维建筑和地形模型',\n        'FPV竞速': '参与第一人称视角竞速比赛，体验速度与激情',\n        '特技飞行': '进行各种特技飞行表演，展示飞行技巧',\n        '航拍摄影': '拍摄高质量的航空摄影作品，捕捉美丽景观',\n        '娱乐休闲': '作为个人休闲娱乐设备，体验飞行乐趣',\n        '集群表演': '多机编队进行灯光表演，创造震撼视觉效果',\n        '协同作业': '多机协同完成复杂任务，提高工作效率',\n        '区域覆盖': '对大面积区域进行快速覆盖，获取全面数据',\n        '应急通信': '在通信中断地区提供临时通信服务',\n        '大范围搜索': '对大范围区域进行搜索作业，提高搜索效率',\n        '区域监控': '对特定区域进行全天候监控，保障区域安全'\n      };\n      return descriptions[app] || '适用于各种专业应用场景';\n    },\n    showDetails(drone) {\n      this.selectedDrone = drone;\n      document.getElementById('droneModal').classList.add('show');\n      document.getElementById('droneModal').style.display = 'block';\n      document.body.classList.add('modal-open');\n      const modalBackdrop = document.createElement('div');\n      modalBackdrop.classList.add('modal-backdrop', 'fade', 'show');\n      document.body.appendChild(modalBackdrop);\n    },\n    closeModal() {\n      document.getElementById('droneModal').classList.remove('show');\n      document.getElementById('droneModal').style.display = 'none';\n      document.body.classList.remove('modal-open');\n      const backdrop = document.querySelector('.modal-backdrop');\n      if (backdrop) {\n        backdrop.remove();\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","selectedDrone","activeFilter","drones","id","type","description","image","gallery","features","specifications","applications","video","computed","filteredDrones","filter","drone","mounted","$route","params","find","d","$nextTick","showDetails","methods","setFilter","getMainSpecs","specKeys","fixedWing","multiRotor","helicopter","fpv","system","Object","keys","slice","getBadgeClass","classes","getTypeName","names","getApplicationIcon","app","icons","getApplicationDesc","descriptions","document","getElementById","classList","add","style","display","body","modalBackdrop","createElement","appendChild","closeModal","remove","backdrop","querySelector"],"sources":["C:\\Users\\Administrator\\Desktop\\flyboot\\src\\views\\products\\Platform.vue"],"sourcesContent":["<template>\r\n  <div class=\"platform-page py-5\">\r\n    <div class=\"container\">\r\n      <!-- 页面标题 -->\r\n      <div class=\"row mb-5\">\r\n        <div class=\"col-12 text-center\">\r\n          <h1 class=\"display-5 fw-bold mb-3\">飞行平台产品</h1>\r\n          <p class=\"lead text-secondary\">我们提供全系列无人机飞行平台，满足各种行业应用需求</p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 产品分类筛选 -->\r\n      <div class=\"row mb-4\">\r\n        <div class=\"col-12\">\r\n          <div class=\"category-filter d-flex flex-wrap justify-content-center gap-3\">\r\n            <button class=\"btn\" :class=\"activeFilter === 'all' ? 'btn-primary' : 'btn-outline-primary'\" @click=\"setFilter('all')\">\r\n              全部产品\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'fixedWing' ? 'btn-primary' : 'btn-outline-primary'\" @click=\"setFilter('fixedWing')\">\r\n              <i class=\"bi bi-airplane me-1\"></i>固定翼\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'multiRotor' ? 'btn-primary' : 'btn-outline-primary'\" @click=\"setFilter('multiRotor')\">\r\n              <i class=\"bi bi-hexagon me-1\"></i>多旋翼\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'helicopter' ? 'btn-primary' : 'btn-outline-primary'\" @click=\"setFilter('helicopter')\">\r\n              <i class=\"bi bi-wind me-1\"></i>直升机\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'fpv' ? 'btn-primary' : 'btn-outline-primary'\" @click=\"setFilter('fpv')\">\r\n              <i class=\"bi bi-camera-video me-1\"></i>FPV\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'system' ? 'btn-primary' : 'btn-outline-primary'\" @click=\"setFilter('system')\">\r\n              <i class=\"bi bi-grid-3x3 me-1\"></i>系统平台\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 产品展示 -->\r\n      <div class=\"row g-4\">\r\n        <div class=\"col-12 col-md-6 col-lg-4\" v-for=\"drone in filteredDrones\" :key=\"drone.id\">\r\n          <div class=\"card h-100 product-card\">\r\n            <div class=\"product-image\">\r\n              <img :src=\"drone.image\" class=\"card-img-top\" :alt=\"drone.name\">\r\n              <div class=\"product-overlay\">\r\n                <button class=\"btn btn-light\" @click=\"showDetails(drone)\">查看详情</button>\r\n              </div>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <div class=\"product-badge\" :class=\"getBadgeClass(drone.type)\">\r\n                {{ getTypeName(drone.type) }}\r\n              </div>\r\n              <h5 class=\"card-title\">{{ drone.name }}</h5>\r\n              <p class=\"card-text\">{{ drone.description }}</p>\r\n              <div class=\"specs\">\r\n                <div class=\"specs-item\" v-for=\"(key, index) in getMainSpecs(drone)\" :key=\"index\">\r\n                  <span class=\"specs-label\">{{ key }}:</span>\r\n                  <span class=\"specs-value\">{{ drone.specifications[key] }}</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"card-footer bg-white border-top-0\">\r\n              <div class=\"d-flex gap-2\">\r\n                <button class=\"btn btn-primary flex-grow-1\" @click=\"showDetails(drone)\">产品详情</button>\r\n                <button class=\"btn btn-outline-primary flex-grow-1\">获取报价</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 产品详情模态框 -->\r\n    <div class=\"modal fade\" id=\"droneModal\" tabindex=\"-1\">\r\n      <div class=\"modal-dialog modal-xl modal-dialog-scrollable\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\">{{ selectedDrone ? selectedDrone.name : '' }}</h5>\r\n            <button type=\"button\" class=\"btn-close\" @click=\"closeModal\" aria-label=\"Close\"></button>\r\n          </div>\r\n          <div class=\"modal-body\" v-if=\"selectedDrone\">\r\n            <!-- 产品导航标签 -->\r\n            <ul class=\"nav nav-tabs mb-4\" id=\"productTab\" role=\"tablist\">\r\n              <li class=\"nav-item\" role=\"presentation\">\r\n                <button class=\"nav-link active\" id=\"overview-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#overview\" type=\"button\" role=\"tab\">产品概述</button>\r\n              </li>\r\n              <li class=\"nav-item\" role=\"presentation\">\r\n                <button class=\"nav-link\" id=\"specs-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#specs\" type=\"button\" role=\"tab\">技术参数</button>\r\n              </li>\r\n              <li class=\"nav-item\" role=\"presentation\">\r\n                <button class=\"nav-link\" id=\"features-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#features\" type=\"button\" role=\"tab\">特点优势</button>\r\n              </li>\r\n              <li class=\"nav-item\" role=\"presentation\">\r\n                <button class=\"nav-link\" id=\"applications-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#applications\" type=\"button\" role=\"tab\">应用场景</button>\r\n              </li>\r\n              <li class=\"nav-item\" role=\"presentation\" v-if=\"selectedDrone.video\">\r\n                <button class=\"nav-link\" id=\"video-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#video\" type=\"button\" role=\"tab\">