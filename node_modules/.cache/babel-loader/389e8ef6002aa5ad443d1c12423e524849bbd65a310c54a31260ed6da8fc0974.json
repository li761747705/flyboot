e;\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","drones","id","category","description","specs","isLoading","methods","getGradient","index","gradients","length","getBadgeClass","classes","goToDetail","droneId","$router","push","error","console"],"sources":["D:\\Hys\\flyboot1\\flyboot\\src\\views\\products\\FPV.vue"],"sourcesContent":["<!-- FPV穿越无人机页面 -->\n<template>\n  <div class=\"product-page fpv-page\">\n    <!-- 顶部英雄区域 -->\n    <section class=\"hero-section\">\n      <div class=\"hero-background\">\n        <div class=\"hero-pattern\"></div>\n        <div class=\"hero-overlay\">\n          <div class=\"container\">\n            <div class=\"hero-content text-center\">\n              <div class=\"hero-badge\">\n                <i class=\"bi bi-camera-video\"></i>\n                <span>FPV穿越无人机</span>\n              </div>\n              <h1 class=\"hero-title\">穿越无人机</h1>\n              <p class=\"hero-subtitle\">体验极速穿越的激情，感受第一人称视角的震撼</p>\n              <div class=\"hero-features\">\n                <div class=\"feature-item\">\n                  <i class=\"bi bi-speedometer2\"></i>\n                  <span>高速穿越</span>\n                </div>\n                <div class=\"feature-item\">\n                  <i class=\"bi bi-camera-video-fill\"></i>\n                  <span>高清FPV</span>\n                </div>\n                <div class=\"feature-item\">\n                  <i class=\"bi bi-stars\"></i>\n                  <span>竞速特技</span>\n                </div>\n                <div class=\"feature-item\">\n                  <i class=\"bi bi-film\"></i>\n                  <span>航拍创作</span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- 产品展示区域 -->\n    <section class=\"products-section\">\n      <div class=\"container\">\n        <div class=\"section-header text-center mb-5\">\n          <h2 class=\"section-title\">产品系列</h2>\n          <p class=\"section-subtitle\">专业级FPV穿越无人机，满足不同场景需求</p>\n        </div>\n        \n        <div class=\"product-grid\">\n          <div class=\"product-card\" v-for=\"(drone, index) in drones\" :key=\"drone.id\" @click=\"goToDetail(drone.id)\">\n            <div class=\"card-header\">\n              <div class=\"product-image\" :style=\"{background: getGradient(index)}\">\n                <div class=\"image-overlay\">\n                  <i class=\"bi bi-camera-video\"></i>\n                </div>\n              </div>\n              <div class=\"product-badge\" :class=\"getBadgeClass(index)\">\n                {{ drone.category }}\n              </div>\n            </div>\n            <div class=\"card-body\">\n              <h3 class=\"product-name\">{{ drone.name }}</h3>\n              <p class=\"product-description\">{{ drone.description }}</p>\n              <div class=\"product-specs\">\n                <div class=\"spec-item\" v-for=\"(spec, key) in drone.specs\" :key=\"key\">\n                  <span class=\"spec-label\">{{ key }}</span>\n                  <span class=\"spec-value\">{{ spec }}</span>\n                </div>\n              </div>\n            </div>\n            <div class=\"card-footer\">\n              <button class=\"btn btn-primary btn-sm\">\n                <i class=\"bi bi-arrow-right\"></i>\n                查看详情\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- 加载状态 -->\n    <div class=\"loading-overlay\" v-if=\"isLoading\">\n      <div class=\"loading-content\">\n        <div class=\"loading-spinner\"></div>\n        <p>正在加载产品信息...</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'FPVDrone',\n  data() {\n    return {\n      drones: [\n        {\n          id: 'fpv-p1',\n          name: 'P1 竞速穿越机',\n          category: '竞速型',\n          description: '专为竞速比赛设计，极致的速度体验',\n          specs: {\n            '最大速度': '180km/h',\n            '续航时间': '8分钟',\n            '重量': '350g',\n            '图传': '5.8GHz'\n          }\n        },\n        {\n          id: 'fpv-p2x',\n          name: 'P2X 特技穿越机',\n          category: '特技型',\n          description: '自由式特技飞行，展现飞行技巧',\n          specs: {\n            '最大速度': '160km/h',\n            '续航时间': '10分钟',\n            '重量': '380g',\n            '图传': '5.8GHz'\n          }\n        },\n        {\n          id: 'fpv-c1',\n          name: 'C1 电影穿越机',\n          category: '电影级',\n          description: '专业航拍穿越机，高清画质输出',\n          specs: {\n            '最大速度': '120km/h',\n            '续航时间': '15分钟',\n            '重量': '450g',\n            '图传': '5.8GHz'\n          }\n        },\n        {\n          id: 'fpv-x4',\n          name: 'X4 四轴穿越机',\n          category: '入门型',\n          description: '新手友好，稳定易操控',\n          specs: {\n            '最大速度': '100km/h',\n            '续航时间': '12分钟',\n            '重量': '320g',\n            '图传': '5.8GHz'\n          }\n        }\n      ],\n      isLoading: false\n    }\n  },\n  methods: {\n    getGradient(index) {\n      const gradients = [\n        'linear-gradient(135deg, #ff6b6b, #ee5a24)',\n        'linear-gradient(135deg, #4834d4, #686de0)',\n        'linear-gradient(135deg, #00d2d3, #54a0ff)',\n        'linear-gradient(135deg, #ff9ff3, #f368e0)'\n      ]\n      return gradients[index % gradients.length]\n    },\n    getBadgeClass(index) {\n      const classes = ['badge-primary', 'badge-secondary', 'badge-success', 'badge-warning']\n      return classes[index % classes.length]\n    },\n    async goToDetail(droneId) {\n      this.isLoading = true\n      try {\n        await this.$router.push(`/products/details/fpv/${droneId}`)\n      } catch (error) {\n        console.error('Navigation error:', error)\n      } finally {\n        this.isLoading = false\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.product-page {\n  min-height: 100vh;\n  background: #f8f9fa;\n}\n\n/* 英雄区域样式 */\n.hero-section {\n  position: relative;\n  hei