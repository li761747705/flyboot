iv class=\"product-features\">\r\n                <h3>产品特点</h3>\r\n                <ul>\r\n                  <li v-for=\"(feature, index) in droneFeatures\" :key=\"index\">\r\n                    <i class=\"bi bi-check-circle-fill\"></i>\r\n                    {{ feature }}\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- 技术参数 -->\r\n    <section class=\"specifications\">\r\n      <div class=\"container\">\r\n        <h2 class=\"section-title text-center mb-5\">技术参数</h2>\r\n        <div class=\"row g-4\">\r\n          <div class=\"col-md-3 col-sm-6\" v-for=\"(spec, index) in droneSpecs\" :key=\"index\">\r\n            <div class=\"spec-card\">\r\n              <div class=\"spec-icon\">\r\n                <i :class=\"spec.icon\"></i>\r\n              </div>\r\n              <div class=\"spec-info\">\r\n                <div class=\"spec-value\">{{ spec.value }}</div>\r\n                <div class=\"spec-label\">{{ spec.label }}</div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- 应用场景 -->\r\n    <section class=\"applications\">\r\n      <div class=\"container\">\r\n        <h2 class=\"section-title text-center mb-5\">应用场景</h2>\r\n        <div class=\"row g-4\">\r\n          <div class=\"col-md-4\" v-for=\"(scene, index) in applicationScenes\" :key=\"index\">\r\n            <div class=\"scene-card\">\r\n              <div class=\"scene-icon\">\r\n                <i :class=\"scene.icon\"></i>\r\n              </div>\r\n              <h3 class=\"scene-title\">{{ scene.title }}</h3>\r\n              <p class=\"scene-description\">{{ scene.description }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'DroneDetail',\r\n  data() {\r\n    return {\r\n      colors: ['#6ad1e3', '#b6e388', '#f7c873', '#f7a6a6', '#b6b6f7', '#f7e3a6'],\r\n      drone: {\r\n        name: '加载中...',\r\n        code: '',\r\n        description: ''\r\n      },\r\n      droneFeatures: [\r\n        '高精度GPS定位系统',\r\n        '智能避障技术',\r\n        '全向感知系统',\r\n        '模块化设计',\r\n        '快速展开收纳',\r\n        '长续航能力'\r\n      ],\r\n      droneSpecs: [\r\n        { label: '最大起飞重量', value: '25kg', icon: 'bi bi-box-seam' },\r\n        { label: '最大飞行时间', value: '45分钟', icon: 'bi bi-clock' },\r\n        { label: '最大飞行速度', value: '72km/h', icon: 'bi bi-speedometer' },\r\n        { label: '抗风等级', value: '6级', icon: 'bi bi-wind' },\r\n        { label: '工作温度', value: '-10°C~40°C', icon: 'bi bi-thermometer-half' },\r\n        { label: '控制距离', value: '7km', icon: 'bi bi-broadcast' },\r\n        { label: '定位精度', value: '±0.1m', icon: 'bi bi-geo-alt' },\r\n        { label: '收纳尺寸', value: '68×45×28cm', icon: 'bi bi-arrows-angle-contract' }\r\n      ],\r\n      applicationScenes: [\r\n        {\r\n          title: '航空测绘',\r\n          description: '高精度航测系统,可快速完成大面积地形测绘任务',\r\n          icon: 'bi bi-map'\r\n        },\r\n        {\r\n          title: '工业巡检',\r\n          description: '搭载高清相机和红外传感器,可进行电力、石油等设施巡检',\r\n          icon: 'bi bi-building-gear'\r\n        },\r\n        {\r\n          title: '应急救援',\r\n          description: '快速响应,可在恶劣环境下执行搜救任务',\r\n          icon: 'bi bi-shield-check'\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    getRandomColor() {\r\n      return this.colors[Math.floor(Math.random() * this.colors.length)]\r\n    },\r\n    loadDroneData() {\r\n      const droneId = this.$route.params.id\r\n      // 这里模拟从父组件或API获取数据\r\n      const drones = {\r\n        drone1: {\r\n          name: '六旋翼无人机',\r\n          code: 'WW-1',\r\n          description: '专业级六旋翼无人机,适用于航拍测绘等场景'\r\n        },\r\n        drone2: {\r\n          name: '八旋翼无人机',\r\n          code: 'WW-2',\r\n          description: '重载型八旋翼无人机,适用于工业巡检等场景