{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: 'MultiRotorDrone',\n  data() {\n    return {\n      activeFilter: 'all',\n      drones: [{\n        id: 'multi-m300',\n        type: 'industrial',\n        name: 'M300 工业级八旋翼',\n        description: '专业工业级八旋翼无人机，高可靠性设计，适合工业巡检、测绘等复杂环境应用',\n        image: require('@/assets/drone-m300.jpg'),\n        specifications: {\n          '最大起飞重量': '9kg',\n          '最大载重': '2.7kg',\n          '续航时间': '55分钟',\n          '最大飞行速度': '83km/h',\n          '悬停精度': '±0.1m',\n          '抗风等级': '8级',\n          '防护等级': 'IP45',\n          '工作温度': '-20°C至50°C',\n          '最大通信距离': '15km',\n          '最大飞行海拔': '5000m'\n        }\n      }, {\n        id: 'multi-p4',\n        type: 'photography',\n        name: 'P4 专业航拍四旋翼',\n        description: '高端航拍四旋翼无人机，配备1英寸传感器相机，提供卓越的影像质量',\n        image: require('@/assets/drone-p4.jpg'),\n        specifications: {\n          '最大起飞重量': '1.4kg',\n          '最大载重': '0.5kg',\n          '续航时间': '30分钟',\n          '最大飞行速度': '72km/h',\n          '悬停精度': '±0.1m',\n          '抗风等级': '5级',\n          '工作温度': '0°C至40°C',\n          '最大通信距离': '8km',\n          '相机参数': '1英寸CMOS，2000万像素',\n          '视频分辨率': '4K/60fps'\n        }\n      }, {\n        id: 'multi-a16',\n        type: 'agriculture',\n        name: 'A16 植保无人机',\n        description: '专业农业植保无人机，16L药箱容量，高效率喷洒系统，适合大面积农田作业',\n        image: require('@/assets/drone-a16.jpg'),\n        specifications: {\n          '最大起飞重量': '42kg',\n          '最大载重': '16kg',\n          '药箱容量': '16L',\n          '续航时间': '15分钟',\n          '喷洒效率': '10亩/分钟',\n          '喷洒宽度': '5-7m',\n          '抗风等级': '6级',\n          '防护等级': 'IP67',\n          '工作温度': '-10°C至45°C',\n          '作业控制': 'RTK厘米级导航'\n        }\n      }, {\n        id: 'multi-l100',\n        type: 'logistics',\n        name: 'L100 物流无人机',\n        description: '专业物流配送无人机，全自动航线规划，支持精准降落和无人值守运行',\n        image: require('@/assets/drone-l100.jpg'),\n        specifications: {\n          '最大起飞重量': '25kg',\n          '最大载重': '5kg',\n          '货舱容积': '30L',\n          '续航时间': '25分钟',\n          '最大飞行速度': '65km/h',\n          '巡航速度': '40km/h',\n          '抗风等级': '5级',\n          '防护等级': 'IP54',\n          '工作温度': '-10°C至40°C',\n          '最大配送距离': '8km'\n        }\n      }, {\n        id: 'multi-s600',\n        type: 'industrial',\n        name: 'S600 折叠六旋翼',\n        description: '便携式工业级六旋翼无人机，折叠设计，快速部署，适合各类工业应用',\n        image: require('@/assets/drone-s600.jpg'),\n        specifications: {\n          '最大起飞重量': '5kg',\n          '最大载重': '1.5kg',\n          '续航时间': '35分钟',\n          '最大飞行速度': '70km/h',\n          '悬停精度': '±0.2m',\n          '抗风等级': '6级',\n          '防护等级': 'IP43',\n          '工作温度': '-10°C至45°C',\n          '最大通信距离': '10km',\n          '折叠尺寸': '30×25×20cm'\n        }\n      }, {\n        id: 'multi-i400',\n        type: 'photography',\n        name: 'I400 便携航拍无人机',\n        description: '小型便携航拍无人机，折叠设计，3轴云台稳定，4K高清拍摄',\n        image: require('@/assets/drone-i400.jpg'),\n        specifications: {\n          '最大起飞重量': '0.6kg',\n          '最大载重': '0.2kg',\n          '续航时间': '32分钟',\n          '最大飞行速度': '60km/h',\n          '悬停精度': '±0.1m',\n          '抗风等级': '5级',\n          '工作温度': '0°C至40°C',\n          '最大通信距离': '6km',\n          '相机参数': '1/2英寸CMOS，1200万像素',\n          '视频分辨率': '4K/30fps'\n        }\n      }]\n    };\n  },\n  computed: {\n    filteredDrones() {\n      if (this.activeFilter === 'all') {\n        return this.drones;\n      }\n      return this.drones.filter(drone => drone.type === this.activeFilter);\n    }\n  },\n  methods: {\n    setFilter(filter) {\n      this.activeFilter = filter;\n    },\n    getMainSpecs(drone) {\n      const keys = ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'];\n      return Object.fromEntries(Object.entries(drone.specifications).filter(([key]) => keys.includes(key)));\n    },\n    getTypeName(type) {\n      const names = {\n        'photography': '航拍型',\n        'industrial': '工业型',\n        'logistics': '物流型',\n        'agriculture': '植保型'\n      };\n      return names[type] || '多旋翼';\n    }\n  }\n};","map":{"version":3,"names":["name","data","activeFilter","drones","id","type","description","image","require","specifications","computed","filteredDrones","filter","drone","methods","setFilter","getMainSpecs","keys","Object","fromEntries","entries","key","includes","getTypeName","names"],"sources":["C:\\Users\\75132\\Desktop\\flyboot\\flyboot\\src\\views\\products\\drone\\MultiRotor.vue"],"sourcesContent":["<!-- 多旋翼无人机页面 -->\r\n<template>\r\n  <div class=\"multirotor-page py-5\">\r\n    <div class=\"container\">\r\n      <!-- 页面标题 -->\r\n      <div class=\"row mb-5\">\r\n        <div class=\"col-12 text-center\">\r\n          <h1 class=\"display-5 fw-bold mb-3\">多旋翼无人机</h1>\r\n          <p class=\"lead text-secondary\">专业级多旋翼无人机，稳定性强、操控精准，适用于航拍、测绘、植保和物流等多种场景</p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 产品筛选 -->\r\n      <div class=\"row mb-4\">\r\n        <div class=\"col-12\">\r\n          <div class=\"category-filter d-flex flex-wrap justify-content-center gap-3\">\r\n            <button class=\"btn\" :class=\"activeFilter === 'all' ? 'btn-success' : 'btn-outline-success'\" @click=\"setFilter('all')\">\r\n              全部多旋翼\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'photography' ? 'btn-success' : 'btn-outline-success'\" @click=\"setFilter('photography')\">\r\n              <i class=\"bi bi-camera me-1\"></i>航拍型\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'industrial' ? 'btn-success' : 'btn-outline-success'\" @click=\"setFilter('industrial')\">\r\n              <i class=\"bi bi-tools me-1\"></i>工业型\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'logistics' ? 'btn-success' : 'btn-outline-success'\" @click=\"setFilter('logistics')\">\r\n              <i class=\"bi bi-box-seam me-1\"></i>物流型\r\n            </button>\r\n            <button class=\"btn\" :class=\"activeFilter === 'agriculture' ? 'btn-success' : 'btn-outline-success'\" @click=\"setFilter('agriculture')\">\r\n              <i class=\"bi bi-flower1 me-1\"></i>植保型\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 产品展示 -->\r\n      <div class=\"row g-4\">\r\n        <div class=\"col-12 col-md-6 col-lg-4\" v-for=\"drone in filteredDrones\" :key=\"drone.id\">\r\n          <div class=\"product-card animate__animated animate__fadeInUp\">\r\n            <div class=\"product-image-container\">\r\n              <img :src=\"drone.image\" :alt=\"drone.name\" class=\"img-fluid\">\r\n            </div>\r\n            <div class=\"product-overlay\">\r\n              <div class=\"product-content\">\r\n                <div class=\"product-badge bg-success\">{{ getTypeName(drone.type) }}</div>\r\n                <h3 class=\"product-title\">{{drone.name}}</h3>\r\n                <p class=\"product-brief\">{{drone.description}}</p>\r\n                <div class=\"product-actions\">\r\n                  <router-link :to=\"'/products/platform/detail/' + drone.id\" class=\"btn btn-light rounded-pill px-4\">查看详情</router-link>\r\n                  <button class=\"btn btn-outline-light btn-icon ms-2 rounded-circle\" title=\"添加收藏\">\r\n                    <i class=\"bi bi-heart\"></i>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"product-tags\">\r\n              <span class=\"product-tag\" v-for=\"(value, key) in getMainSpecs(drone)\" :key=\"key\">\r\n                {{key}}: {{value}}\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 产品特点 -->\r\n      <div class=\"row mt-5\">\r\n        <div class=\"col-12\">\r\n          <div class=\"features-card p-4 animate__animated animate__fadeIn\">\r\n            <div class=\"feature-header mb-4\">\r\n              <h4 class=\"mb-3\"><i class=\"bi bi-grid-3x3 text-success me-2\"></i>多旋翼无人机特点</h4>\r\n              <p class=\"feature-subtitle\">灵活机动的空中平台，为多种应用场景提供稳定可靠的飞行体验</p>\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col-md-6\">\r\n                <ul class=\"feature-list\">\r\n                  <li class=\"animate__animated animate__fadeInLeft animate__delay-1s\">\r\n                    <i class=\"bi bi-check-circle-fill text-success me-2\"></i>精准悬停定位\r\n                    <span class=\"feature-desc\">厘米级悬停精度，实现稳定的空中定位</span>\r\n                  </li>\r\n                  <li class=\"animate__animated animate__fadeInLeft animate__delay-2s\">\r\n                    <i class=\"bi bi-check-circle-fill text-success me-2\"></i>灵活起降能力\r\n                    <span class=\"feature-desc\">垂直起降，适应各种复杂地形和狭小空间</span>\r\n                  </li>\r\n                  <li class=\"animate__animated animate__fadeInLeft animate__delay-3s\">\r\n                    <i class=\"bi bi-check-circle-fill text-success me-2\"></i>多功能负载平台\r\n                    <span class=\"feature-desc\">兼容多种任务载荷，适应不同应用需求</span>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n              <div class=\"col-md-6\">\r\n                <ul class=\"feature-list\">\r\n                  <li class=\"animate__animated animate__fadeInRight animate__delay-1s\">\r\n                    <i class=\"bi bi-check-circle-fill text-success me-2\"></i>智能飞行系统\r\n                    <span class=\"feature-desc\">多种智能飞行模式，简化操作提高效率</span>\r\n                  </li>\r\n                  <li class=\"animate__animated animate__fadeInRight animate__delay-2s\">\r\n                    <i class=\"bi bi-check-circle-fill text-success me-2\"></i>安全保障机制\r\n                    <span class=\"feature-desc\">多重失效保护和返航功能，确保飞行安全</span>\r\n                  </li>\r\n                  <li class=\"animate__animated animate__fadeInRight animate__delay-3s\">\r\n                    <i class=\"bi bi-check-circle-fill text-success me-2\"></i>模块化设计\r\n                    <span class=\"feature-desc\">快速拆装和维护，提高作业效率</span>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n            <div class=\"text-center mt-4\">\r\n              <a href=\"#\" class=\"btn btn-outline-success rounded-pill\">了解更多多旋翼无人机技术 <i class=\"bi bi-arrow-right\"></i></a>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 应用场景 -->\r\n      <div class=\"row mt-5\">\r\n        <div class=\"col-12 text-center mb-4\">\r\n          <h3 class=\"section-title\">应用场景</h3>\r\n          <p class=\"section-description\">多旋翼无人机广泛应用于以下场景</p>\r\n        </div>\r\n        \r\n        <div class=\"col-md-4 mb-4\">\r\n          <div class=\"application-card text-center p-4\">\r\n            <div class=\"application-icon mb-3\">\r\n              <i class=\"bi bi-camera-reels fs-1 text-success\"></i>\r\n            </div>\r\n            <h4>专业航拍</h4>\r\n            <p>高清影像采集，用于电影拍摄、广告创作和风景记录</p>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"col-md-4 mb-4\">\r\n          <div class=\"application-card text-center p-4\">\r\n            <i class=\"bi bi-map fs-1 text-success mb-3\"></i>\r\n            <h4>测绘勘测</h4>\r\n            <p>地理信息采集、三维建模和地形测绘，为规划提供数据支持</p>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"col-md-4 mb-4\">\r\n          <div class=\"application-card text-center p-4\">\r\n            <i class=\"bi bi-flower1 fs-1 text-success mb-3\"></i>\r\n            <h4>农业植保</h4>\r\n            <p>农田喷洒、农作物监测，提高农业生产效率</p>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"col-md-4 mb-4\">\r\n          <div class=\"application-card text-center p-4\">\r\n            <i class=\"bi bi-lightning fs-1 text-success mb-3\"></i>\r\n            <h4>电力巡检</h4>\r\n            <p>输电线路巡检、铁塔检测，提高检测效率和安全性</p>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"col-md-4 mb-4\">\r\n          <div class=\"application-card text-center p-4\">\r\n            <i class=\"bi bi-box-seam fs-1 text-success mb-3\"></i>\r\n            <h4>物流配送</h4>\r\n            <p>小型货物空中配送，解决最后一公里配送难题</p>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"col-md-4 mb-4\">\r\n          <div class=\"application-card text-center p-4\">\r\n            <i class=\"bi bi-shield-check fs-1 text-success mb-3\"></i>\r\n            <h4>安防监控</h4>\r\n            <p>公共安全监控、活动安保和区域巡逻</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'MultiRotorDrone',\r\n  data() {\r\n    return {\r\n      activeFilter: 'all',\r\n      drones: [\r\n        {\r\n          id: 'multi-m300',\r\n          type: 'industrial',\r\n          name: 'M300 工业级八旋翼',\r\n          description: '专业工业级八旋翼无人机，高可靠性设计，适合工业巡检、测绘等复杂环境应用',\r\n          image: require('@/assets/drone-m300.jpg'),\r\n          specifications: {\r\n            '最大起飞重量': '9kg',\r\n            '最大载重': '2.7kg',\r\n            '续航时间': '55分钟',\r\n            '最大飞行速度': '83km/h',\r\n            '悬停精度': '±0.1m',\r\n            '抗风等级': '8级',\r\n            '防护等级': 'IP45',\r\n            '工作温度': '-20°C至50°C',\r\n            '最大通信距离': '15km',\r\n            '最大飞行海拔': '5000m'\r\n          }\r\n        },\r\n        {\r\n          id: 'multi-p4',\r\n          type: 'photography',\r\n          name: 'P4 专业航拍四旋翼',\r\n          description: '高端航拍四旋翼无人机，配备1英寸传感器相机，提供卓越的影像质量',\r\n          image: require('@/assets/drone-p4.jpg'),\r\n          specifications: {\r\n            '最大起飞重量': '1.4kg',\r\n            '最大载重': '0.5kg',\r\n            '续航时间': '30分钟',\r\n            '最大飞行速度': '72km/h',\r\n            '悬停精度': '±0.1m',\r\n            '抗风等级': '5级',\r\n            '工作温度': '0°C至40°C',\r\n            '最大通信距离': '8km',\r\n            '相机参数': '1英寸CMOS，2000万像素',\r\n            '视频分辨率': '4K/60fps'\r\n          }\r\n        },\r\n        {\r\n          id: 'multi-a16',\r\n          type: 'agriculture',\r\n          name: 'A16 植保无人机',\r\n          description: '专业农业植保无人机，16L药箱容量，高效率喷洒系统，适合大面积农田作业',\r\n          image: require('@/assets/drone-a16.jpg'),\r\n          specifications: {\r\n            '最大起飞重量': '42kg',\r\n            '最大载重': '16kg',\r\n            '药箱容量': '16L',\r\n            '续航时间': '15分钟',\r\n            '喷洒效率': '10亩/分钟',\r\n            '喷洒宽度': '5-7m',\r\n            '抗风等级': '6级',\r\n            '防护等级': 'IP67',\r\n            '工作温度': '-10°C至45°C',\r\n            '作业控制': 'RTK厘米级导航'\r\n          }\r\n        },\r\n        {\r\n          id: 'multi-l100',\r\n          type: 'logistics',\r\n          name: 'L100 物流无人机',\r\n          description: '专业物流配送无人机，全自动航线规划，支持精准降落和无人值守运行',\r\n          image: require('@/assets/drone-l100.jpg'),\r\n          specifications: {\r\n            '最大起飞重量': '25kg',\r\n            '最大载重': '5kg',\r\n            '货舱容积': '30L',\r\n            '续航时间': '25分钟',\r\n            '最大飞行速度': '65km/h',\r\n            '巡航速度': '40km/h',\r\n            '抗风等级': '5级',\r\n            '防护等级': 'IP54',\r\n            '工作温度': '-10°C至40°C',\r\n            '最大配送距离': '8km'\r\n          }\r\n        },\r\n        {\r\n          id: 'multi-s600',\r\n          type: 'industrial',\r\n          name: 'S600 折叠六旋翼',\r\n          description: '便携式工业级六旋翼无人机，折叠设计，快速部署，适合各类工业应用',\r\n          image: require('@/assets/drone-s600.jpg'),\r\n          specifications: {\r\n            '最大起飞重量': '5kg',\r\n            '最大载重': '1.5kg',\r\n            '续航时间': '35分钟',\r\n            '最大飞行速度': '70km/h',\r\n            '悬停精度': '±0.2m',\r\n            '抗风等级': '6级',\r\n            '防护等级': 'IP43',\r\n            '工作温度': '-10°C至45°C',\r\n            '最大通信距离': '10km',\r\n            '折叠尺寸': '30×25×20cm'\r\n          }\r\n        },\r\n        {\r\n          id: 'multi-i400',\r\n          type: '