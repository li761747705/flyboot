            <!-- 实际使用时替换为线框图 -->\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 技术参数表格 -->\r\n        <div class=\"specs-table mt-5\">\r\n          <h2 class=\"specs-title\">技术参数</h2>\r\n          <div class=\"table-responsive\">\r\n            <table class=\"table table-bordered\">\r\n              <tbody>\r\n                <tr v-for=\"(spec, index) in droneSpecs\" :key=\"index\">\r\n                  <th class=\"bg-light\">{{ spec.label }}</th>\r\n                  <td>{{ spec.value }}</td>\r\n                  <th class=\"bg-light\">{{ spec.label2 }}</th>\r\n                  <td>{{ spec.value2 }}</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 尺寸图 -->\r\n        <div class=\"dimensions mt-5\">\r\n          <h2 class=\"specs-title\">尺寸图</h2>\r\n          <div class=\"row g-4\">\r\n            <div class=\"col-md-4\" v-for=\"(view, index) in dimensionViews\" :key=\"index\">\r\n              <div class=\"dimension-card\">\r\n                <div class=\"dimension-title\">{{ view.title }}</div>\r\n                <div class=\"dimension-image\" :style=\"{ background: getRandomColor() }\">\r\n                  <!-- 实际使用时替换为尺寸图 -->\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'DroneDetail',\r\n  data() {\r\n    return {\r\n      colors: ['#6ad1e3', '#b6e388', '#f7c873', '#f7a6a6', '#b6b6f7', '#f7e3a6'],\r\n      drone: {\r\n        name: '加载中...',\r\n        code: '',\r\n        description: ''\r\n      },\r\n      droneSpecs: [\r\n        {\r\n          label: '机架',\r\n          value: '碳纤维',\r\n          label2: '电机数量',\r\n          value2: '6个'\r\n        },\r\n        {\r\n          label: '轴距',\r\n          value: '1200mm',