{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nexport default {\n  name: 'PlatformDetail',\n  data() {\n    return {\n      drone: null,\n      drones: []\n    };\n  },\n  created() {\n    const id = this.$route.params.id;\n    console.log('详情页ID:', id);\n    // 优先从本地数据查找\n    if (this.drones.length) {\n      this.drone = this.drones.find(d => d.id === id);\n    }\n    // 如果本地没有，尝试从 localStorage 获取\n    if (!this.drone) {\n      try {\n        const dronesStr = localStorage.getItem('allDrones');\n        if (dronesStr) {\n          const drones = JSON.parse(dronesStr);\n          this.drone = drones.find(d => d.id === id);\n        }\n      } catch (e) {}\n    }\n    // 如果还没有，可以在这里加API请求\n    // if (!this.drone) {\n    //   this.fetchDroneById(id);\n    // }\n    if (!this.drone) {\n      console.warn('未找到产品', id);\n    }\n  },\n  methods: {\n    // fetchDroneById(id) {\n    //   // 这里可以写API请求逻辑\n    // }\n    getMainSpecs(drone) {\n      // 根据产品类型获取关键参数\n      const specKeys = {\n        fixedWing: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\n        multiRotor: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\n        helicopter: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\n        fpv: ['机身尺寸', '重量(不含电池)', '最大速度', '续航时间'],\n        system: ['最大集群数量', '控制距离', '编队精度', '响应时间']\n      };\n      const type = drone.type || 'multiRotor';\n      return specKeys[type] || Object.keys(drone.specifications).slice(0, 4);\n    },\n    getBadgeClass(type) {\n      const classes = {\n        fixedWing: 'bg-primary',\n        multiRotor: 'bg-success',\n        helicopter: 'bg-danger',\n        fpv: 'bg-warning',\n        system: 'bg-info'\n      };\n      return classes[type] || 'bg-secondary';\n    },\n    getTypeName(type) {\n      const names = {\n        fixedWing: '固定翼',\n        multiRotor: '多旋翼',\n        helicopter: '直升机',\n        fpv: 'FPV',\n        system: '系统平台'\n      };\n      return names[type] || '未分类';\n    },\n    getApplicationIcon(app) {\n      const icons = {\n        '电力巡检': 'bi bi-lightning fs-3',\n        '测绘测量': 'bi bi-map fs-3',\n        '管道巡检': 'bi bi-water fs-3',\n        '安防监控': 'bi bi-shield-check fs-3',\n        '应急救援': 'bi bi-heart-pulse fs-3',\n        '农林植保': 'bi bi-tree fs-3',\n        '边境巡逻': 'bi bi-geo-alt fs-3',\n        '森林防火': 'bi bi-fire fs-3',\n        '海洋监测': 'bi bi-water fs-3',\n        '环境监测': 'bi bi-droplet-half fs-3',\n        '航空测绘': 'bi bi-rulers fs-3',\n        '农业植保': 'bi bi-flower1 fs-3',\n        '影视航拍': 'bi bi-camera fs-3',\n        '3D建模': 'bi bi-box fs-3',\n        'FPV竞速': 'bi bi-speedometer2 fs-3',\n        '特技飞行': 'bi bi-stars fs-3',\n        '航拍摄影': 'bi bi-camera-video fs-3',\n        '娱乐休闲': 'bi bi-controller fs-3',\n        '集群表演': 'bi bi-stars fs-3',\n        '协同作业': 'bi bi-people fs-3',\n        '区域覆盖': 'bi bi-grid-3x3 fs-3',\n        '应急通信': 'bi bi-broadcast-pin fs-3',\n        '大范围搜索': 'bi bi-search fs-3',\n        '区域监控': 'bi bi-eye fs-3'\n      };\n      return icons[app] || 'bi bi-rocket';\n    },\n    getApplicationDesc(app) {\n      const descriptions = {\n        '电力巡检': '对电力线路进行巡查，检测设备异常和线路故障',\n        '测绘测量': '进行地形测绘和三维模型构建，为工程规划提供数据支持',\n        '管道巡检': '对油气管道进行巡查监测，及时发现泄漏和故障',\n        '安防监控': '对重要区域进行实时监控，提升安全防护能力',\n        '应急救援': '在灾害现场提供空中视角，协助搜救和物资投放',\n        '农林植保': '进行农田和林地的植保作业，提高效率降低成本',\n        '边境巡逻': '对边境区域进行巡检，维护国土安全',\n        '森林防火': '监测森林火情，提供早期预警和火势监控',\n        '海洋监测': '对海洋环境进行监测，为海洋资源保护提供数据支持',\n        '环境监测': '监测空气质量和环境变化，辅助环保工作',\n        '航空测绘': '进行高精度航空摄影测量，生成正射影像图和数字高程模型',\n        '农业植保': '进行精准农业作业，提高农业生产效率',\n        '影视航拍': '为影视制作提供高质量的航拍素材，展现全新视角',\n        '3D建模': '通过航拍数据生成三维建筑和地形模型',\n        'FPV竞速': '参与第一人称视角竞速比赛，体验速度与激情',\n        '特技飞行': '进行各种特技飞行表演，展示飞行技巧',\n        '航拍摄影': '拍摄高质量的航空摄影作品，捕捉美丽景观',\n        '娱乐休闲': '作为个人休闲娱乐设备，体验飞行乐趣',\n        '集群表演': '多机编队进行灯光表演，创造震撼视觉效果',\n        '协同作业': '多机协同完成复杂任务，提高工作效率',\n        '区域覆盖': '对大面积区域进行快速覆盖，获取全面数据',\n        '应急通信': '在通信中断地区提供临时通信服务',\n        '大范围搜索': '对大范围区域进行搜索作业，提高搜索效率',\n        '区域监控': '对特定区域进行全天候监控，保障区域安全'\n      };\n      return descriptions[app] || '';\n    }\n  }\n};","map":{"version":3,"names":["name","data","drone","drones","created","id","$route","params","console","log","length","find","d","dronesStr","localStorage","getItem","JSON","parse","e","warn","methods","getMainSpecs","specKeys","fixedWing","multiRotor","helicopter","fpv","system","type","Object","keys","specifications","slice","getBadgeClass","classes","getTypeName","names","getApplicationIcon","app","icons","getApplicationDesc","descriptions"],"sources":["C:\\Users\\75132\\Desktop\\flyboot\\flyboot\\src\\views\\products\\details\\PlatformDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"platform-detail-page py-5\">\r\n    <div class=\"container\" v-if=\"drone\">\r\n      <!-- 返回按钮 -->\r\n      <div class=\"mb-4\">\r\n        <router-link :to=\"'/products/platform/' + drone.id.split('-')[0]\" class=\"btn btn-outline-secondary\">\r\n          <i class=\"bi bi-arrow-left me-2\"></i>返回产品列表\r\n        </router-link>\r\n      </div>\r\n\r\n      <!-- 产品标题 -->\r\n      <div class=\"row mb-4\">\r\n        <div class=\"col-12\">\r\n          <div class=\"product-badge mb-2\" :class=\"getBadgeClass(drone.type)\">\r\n            {{ getTypeName(drone.type) }}\r\n          </div>\r\n          <h1 class=\"mb-3\">{{ drone.name }}</h1>\r\n          <p class=\"lead text-secondary\">{{ drone.description }}</p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 产品导航标签 -->\r\n      <ul class=\"nav nav-tabs mb-4\" id=\"productTab\" role=\"tablist\">\r\n        <li class=\"nav-item\" role=\"presentation\">\r\n          <button class=\"nav-link active\" id=\"overview-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#overview\" type=\"button\" role=\"tab\">产品概述</button>\r\n        </li>\r\n        <li class=\"nav-item\" role=\"presentation\">\r\n          <button class=\"nav-link\" id=\"specs-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#specs\" type=\"button\" role=\"tab\">技术参数</button>\r\n        </li>\r\n        <li class=\"nav-item\" role=\"presentation\">\r\n          <button class=\"nav-link\" id=\"features-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#features\" type=\"button\" role=\"tab\">特点优势</button>\r\n        </li>\r\n        <li class=\"nav-item\" role=\"presentation\">\r\n          <button class=\"nav-link\" id=\"applications-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#applications\" type=\"button\" role=\"tab\">应用场景</button>\r\n        </li>\r\n        <li class=\"nav-item\" role=\"presentation\" v-if=\"drone.video\">\r\n          <button class=\"nav-link\" id=\"video-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#video\" type=\"button\" role=\"tab\">产品视频</button>\r\n        </li>\r\n      </ul>\r\n\r\n      <!-- 标签内容 -->\r\n      <div class=\"tab-content bg-white p-4 rounded shadow-sm\" id=\"productTabContent\">\r\n        <!-- 产品概述 -->\r\n        <div class=\"tab-pane fade show active\" id=\"overview\" role=\"tabpanel\">\r\n          <div class=\"row\">\r\n            <div class=\"col-lg-6\">\r\n              <div class=\"product-gallery\">\r\n                <div class=\"main-image mb-3\">\r\n                  <img :src=\"drone.image\" class=\"img-fluid rounded\" :alt=\"drone.name\">\r\n                </div>\r\n                <div class=\"row g-2\">\r\n                  <div class=\"col-3\" v-for=\"(img, index) in drone.gallery\" :key=\"index\">\r\n                    <img :src=\"img\" class=\"img-fluid rounded\" :alt=\"'产品图片 ' + (index + 1)\">\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-lg-6\">\r\n              <h3 class=\"product-title mb-3\">{{ drone.name }}</h3>\r\n              <p class=\"product-description mb-4\">{{ drone.description }}</p>\r\n              \r\n              <h5 class=\"mb-3\">主要特点</h5>\r\n              <div class=\"row g-3 mb-4\">\r\n                <div class=\"col-lg-6\" v-for=\"(feature, index) in drone.features.slice(0, 4)\" :key=\"index\">\r\n                  <div class=\"feature-item d-flex align-items-center\">\r\n                    <div class=\"feature-icon me-2 text-accent\">\r\n                      <i class=\"bi bi-check-circle-fill\"></i>\r\n                    </div>\r\n                    <div class=\"feature-text\">{{ feature }}</div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              \r\n              <h5 class=\"mb-3\">关键参数</h5>\r\n              <div class=\"row g-3\">\r\n                <div class=\"col-6\" v-for=\"(key, index) in getMainSpecs(drone)\" :key=\"index\">\r\n                  <div class=\"spec-box p-3 rounded bg-light\">\r\n                    <div class=\"spec-title text-secondary small\">{{ key }}</div>\r\n                    <div class=\"spec-value fw-bold\">{{ drone.specifications[key] }}</div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              \r\n              <div class=\"mt-4 pt-3 d-flex gap-3\">\r\n                <button class=\"btn btn-primary\">获取报价</button>\r\n                <button class=\"btn btn-outline-secondary\">下载产品手册</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 技术参数 -->\r\n        <div class=\"tab-pane fade\" id=\"specs\" role=\"tabpanel\">\r\n          <div class=\"row\">\r\n            <div class=\"col-lg-6\">\r\n              <div class=\"table-responsive\">\r\n                <table class=\"table table-hover\">\r\n                  <thead>\r\n                    <tr>\r\n                      <th scope=\"col\" class=\"bg-light\">参数</th>\r\n                      <th scope=\"col\" class=\"bg-light\">规格</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    <tr v-for=\"(spec, key) in drone.specifications\" :key=\"key\">\r\n                      <td>{{ key }}</td>\r\n                      <td>{{ spec }}</td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-lg-6\">\r\n              <img :src=\"drone.image\" class=\"img-fluid rounded\" :alt=\"drone.name\">\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 特点优势 -->\r\n        <div class=\"tab-pane fade\" id=\"features\" role=\"tabpanel\">\r\n          <div class=\"row\">\r\n            <div class=\"col-lg-6\">\r\n              <h4 class=\"mb-4\">产品特点</h4>\r\n              <ul class=\"feature-list\">\r\n                <li v-for=\"(feature, index) in drone.features\" :key=\"index\" class=\"mb-3\">\r\n                  <div class=\"d-flex\">\r\n                    <div class=\"feature-icon text-accent me-3\">\r\n                      <i class=\"bi bi-check-circle-fill fs-4\"></i>\r\n                    </div>\r\n                    <div>\r\n                      <p class=\"mb-0\">{{ feature }}</p>\r\n                    </div>\r\n                  </div>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n            <div class=\"col-lg-6\">\r\n              <div class=\"feature-gallery\">\r\n                <img :src=\"drone.gallery[0]\" class=\"img-fluid rounded mb-3\" alt=\"产品特点\">\r\n                <div class=\"row g-3\">\r\n                  <div class=\"col-6\" v-for=\"(img, index) in drone.gallery.slice(1)\" :key=\"index\">\r\n                    <img :src=\"img\" class=\"img-fluid rounded\" :alt=\"'产品特点 ' + (index + 1)\">\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 应用场景 -->\r\n        <div class=\"tab-pane fade\" id=\"applications\" role=\"tabpanel\">\r\n          <div class=\"row\">\r\n            <div class=\"col-12 mb-4\">\r\n              <h4>适用场景</h4>\r\n              <p class=\"text-secondary\">本产品适用于以下行业和应用场景</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"row g-4\">\r\n            <div class=\"col-lg-4 col-md-6\" v-for=\"(app, index) in drone.applications\" :key=\"index\">\r\n              <div class=\"application-card p-4 rounded shadow-sm h-100\">\r\n                <div class=\"application-icon mb-3 text-accent\">\r\n                  <i :class=\"getApplicationIcon(app)\"></i>\r\n                </div>\r\n                <h5>{{ app }}</h5>\r\n                <p class=\"text-secondary mb-0\">{{ getApplicationDesc(app) }}</p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 产品视频 -->\r\n        <div class=\"tab-pane fade\" id=\"video\" role=\"tabpanel\" v-if=\"drone.video\">\r\n          <div class=\"row\">\r\n            <div class=\"col-12 mb-4\">\r\n              <h4>产品视频展示</h4>\r\n              <p class=\"text-secondary\">观看详细的产品演示和操作视频</p>\r\n            </div>\r\n            <div class=\"col-12\">\r\n              <div class=\"ratio ratio-16x9\">\r\n                <iframe :src=\"drone.video\" title=\"产品视频\" allowfullscreen></iframe>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n    <div class=\"container\" v-else>\r\n      <div class=\"alert alert-warning\">\r\n        产品信息加载中或未找到，请稍候...\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'PlatformDetail',\r\n  data() {\r\n    return {\r\n      drone: null,\r\n      drones: []\r\n    }\r\n  },\r\n  created() {\r\n    const id = this.$route.params.id;\r\n    console.log('详情页ID:', id);\r\n    // 优先从本地数据查找\r\n    if (this.drones.length) {\r\n      this.drone = this.drones.find(d => d.id === id);\r\n    }\r\n    // 如果本地没有，尝试从 localStorage 获取\r\n    if (!this.drone) {\r\n      try {\r\n        const dronesStr = localStorage.getItem('allDrones');\r\n        if (dronesStr) {\r\n          const drones = JSON.parse(dronesStr);\r\n          this.drone = drones.find(d => d.id === id);\r\n        }\r\n      } catch (e) {}\r\n    }\r\n    // 如果还没有，可以在这里加API请求\r\n    // if (!this.drone) {\r\n    //   this.fetchDroneById(id);\r\n    // }\r\n    if (!this.drone) {\r\n      console.warn('未找到产品', id);\r\n    }\r\n  },\r\n  methods: {\r\n    // fetchDroneById(id) {\r\n    //   // 这里可以写API请求逻辑\r\n    // }\r\n    getMainSpecs(drone) {\r\n      // 根据产品类型获取关键参数\r\n      const specKeys = {\r\n        fixedWing: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\r\n        multiRotor: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\r\n        helicopter: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\r\n        fpv: ['机身尺寸', '重量(不含电池)', '最大速度', '续航时间'],\r\n        system: ['最大集群数量', '控制距离', '编队精度', '响应时间']\r\n      };\r\n      \r\n      const type = drone.type || 'multiRotor';\r\n      return specKeys[type] || Object.keys(drone.specifications).slice(0, 4);\r\n    },\r\n    getBadgeClass(type) {\r\n      const classes = {\r\n        fixedWing: 'bg-primary',\r\n        multiRotor: 'bg-success',\r\n        helicopter: 'bg-danger',\r\n        fpv: 'bg-warning',\r\n        system: 'bg-info'\r\n      };\r\n      return classes[type] || 'bg-secondary';\r\n    },\r\n    getTypeName(type) {\r\n      const names = {\r\n        fixedWing: '固定翼',\r\n        multiRotor: '多旋翼',\r\n        helicopter: '直升机',\r\n        fpv: 'FPV',\r\n        system: '系统平台'\r\n      };\r\n      return names[type] || '未分类';\r\n    },\r\n    getApplicationIcon(app) {\r\n      const icons = {\r\n        '电力巡检': 'bi bi-lightning fs-3',\r\n        '测绘测量': 'bi bi-map fs-3',\r\n        '管道巡检': 'bi bi-water fs-3',\r\n        '安防监控': 'bi bi-shield-check fs-3',\r\n        '应急救援': 'bi bi-heart-pulse fs-3',\r\n        '农林植保': 'bi bi-tree fs-3',\r\n        '边境巡逻': 'bi bi-geo-alt fs-3',\r\n        '森林防火': 'bi bi-fire fs-3',\r\n        '海洋监测': 'bi bi-water fs-3',\r\n        '环境监测': 'bi bi-droplet-half fs-3',\r\n        '航空测绘': 