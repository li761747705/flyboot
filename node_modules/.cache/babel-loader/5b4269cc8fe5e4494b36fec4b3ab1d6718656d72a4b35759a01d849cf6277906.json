thModifiers","_hoisted_11","_hoisted_10","_createVNode","_component_router_view"],"sources":["D:\\Hys\\flyboot1\\flyboot\\src\\views\\Applications.vue"],"sourcesContent":["<!--行业应用-->\n<template>\n  <div class=\"applications-page\">\n    <!-- 顶部 Banner -->\n    <div class=\"banner-img-wrapper\">\n     <img src=\"/images/applications/行业应用1900X700.png\" alt=\"{{ $t('applications.pageTitle') }}\" class=\"banner-img\" loading=\"lazy\" />\n      <div class=\"banner-content\">\n       <h1>{{ currentTitle }}</h1>\n       <h3>{{ currentDesc }}</h3>\n      </div>\n    </div>\n  \n    <!-- 应用展示区 -->\n    <div class=\"applications-section\" v-if=\"$route.path === '/applications'\">\n      <div class=\"container mt-4\">\n        <h2 class=\"text-center mb-5 position-relative\">{{ $t('applications.sectionTitle') }}<span class=\"section-line\"></span></h2>\n        <h3 class=\"text-center mb-1\">{{ $t('applications.sectionDescription') }}</h3>\n      </div>\n      <div class=\"container\">\n        <div class=\"applications-grid\">\n          <div v-for=\"app in applications\" :key=\"app.id\" class=\"application-card\" @click=\"goToApplication(app.link)\"\n            :style=\"{ backgroundImage: 'url(' + app.image + ')' }\">\n              <h3>{{ app.title }}</h3>\n              <h6>{{ app.remake }}</h6>\n            <a :href=\"app.link\" class=\"btn btn-light rounded-pill px-4\" @click.stop>{{ $t('applications.moreButton') }}</a>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 路由出口 -->\n    <router-view />\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Applications',\n  data() {\n    return {\n      applications: [\n        {\n          id: 1,\n          title: this.$t('applications.cards.mapping.title'),\n          path: 'mapping',\n          remake: this.$t('applications.cards.mapping.description'),\n          image: '/images/applications/测绘400X620.png',\n          link: '/applications/mapping',\n          desc: this.$t('applications.cards.mapping.shortDesc')\n        },\n        {\n          id: 2,\n          title: this.$t('applications.cards.inspection.title'),\n          path: 'inspection',\n          remake: this.$t('applications.cards.inspection.description'),\n          image: '/images/applications/水利400X620.png',\n          link: '/applications/inspection',\n          desc: this.$t('applications.cards.inspection.shortDesc')\n        },\n        {\n          id: 3,\n          title: this.$t('applications.cards.delivery.title'),\n          path: 'delivery',\n          remake: this.$t('applications.cards.delivery.description'),\n          image: '/images/applications/应急400X620.png',\n          link: '/applications/delivery',\n          desc: this.$t('applications.cards.delivery.shortDesc')\n        },\n        {\n          id: 4,\n          title: this.$t('applications.cards.aerial.title'),\n          path: 'aerial',\n          remake: this.$t('applications.cards.aerial.description'),\n          image: '/images/applications/交通400X620.png',\n          link: '/applications/aerial',\n          desc: this.$t('applications.cards.aerial.shortDesc')\n        },\n        {\n          id: 5,\n          title: this.$t('applications.cards.spraying.title'),\n          path: 'spraying',\n          remake: this.$t('applications.cards.spraying.description'),\n          image: '/images/applications/环保400X620.png',\n          link: '/applications/spraying',\n          desc: this.$t('applications.cards.spraying.shortDesc')\n        },\n        {\n          id: 6,\n          title: this.$t('applications.cards.agriculture.title'),\n          path: 'agriculture',\n          remake: this.$t('applications.cards.agriculture.description'),\n          image: '/images/applications/森林400X620.png',\n          link: '/applications/agriculture',\n          desc: this.$t('applications.cards.agriculture.shortDesc')\n        },\n        {\n          id: 7,\n          title: this.$t('applications.cards.powerline.title'),\n          path: 'powerline',\n          remake: this.$t('applications.cards.powerline.description'),\n          image: '/images/applications/电力400X620.png',\n          link: '/applications/powerline',\n          desc: this.$t('applications.cards.powerline.shortDesc')\n        },\n        {\n          id: 8,\n          title: this.$t('applications.cards.farming.title'),\n          path: 'farming',\n          remake: this.$t('applications.cards.farming.description'),\n          image: '/images/applications/农业400X620.png',\n          link: '/applications/farming',\n          desc: this.$t('applications.cards.farming.shortDesc')\n        },\n        {\n          id: 9,\n          title: this.$t('applications.cards.integration.title'),\n          path: 'integration',\n          remake: this.$t('applications.cards.integration.description'),\n          image: '/images/applications/运载400X620.png',\n          link: '/applications/integration',\n          desc: this.$t('applications.cards.integration.shortDesc')\n        }\n      ]\n    }\n  },\n  methods: {\n    goToApplication(route) {\n      this.$router.push(route)\n    }\n  },\n  computed: {\n    currentTitle() {\n      const path = this.$route.path;\n      const match = path.match(/^\\/applications\\/(\\w+)/);\n      if (match) {\n        const found = this.applications.find(app => app.path === match[1]);\n        return found ? found.title : this.$t('applications.pageTitle');\n      }\n      return this.$t('applications.pageTitle');\n    },\n    currentDesc() {\n      const path = this.$route.path;\n      const match = path.match(/^\\/applications\\/(\\w+)/);\n      if (match) {\n        const found = this.applications.find(app => app.path === match[1]);\n        return found ? (found.desc || this.$t('applications.pageDescription')) : this.$t('applications.pageDescription');\n      }\n      return this.$t('applications.pageDescription');\n    }\n  }\n}\n</script>\n\n<style scoped>\n.applications-page {\n  min-height: 100vh;\n  background-color: #f5f5f5;\n}\n\n.banner-img-wrapper {\n  position: relative;\n  width: 100%;\n  overflow: hidden;\n}\n\n.banner-img {\n  width: 100%;\n  height: 700px;\n  object-fit: cover;\n  display: block;\n}\n\n.banner-content {\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  color: #fff;\n  text-shadow: 0 2px 8px rgba(0, 0, 0, 0.36);\n  z-index: 2;\n  pointer-events: none;\n}\n\n.banner h1 {\n  font-size: 48px;\n  margin-bottom: 20px;\n  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5);\n}\n\n.banner p {\n  font-size: 24px;\n  text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.5);\n}\n\n.applications-section {\n  padding: 0 0 50px 0;\n}\n\n.applications-grid {\n  display: grid;\n  grid-template-columns: repeat(3, 1fr);\n  gap: 30px;\n  margin-top: 40px;\n}\n\n.application-card {\n  background: #eee;\n  border-radius: 8px;\n  text-align: center;\n  transition: all 0.3s ease;\n  cursor: pointer;\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n  background-size: cover;\n  background-position: center;\n  position: relative;\n  height: 320px;\n  overflow: hidden;\n}\n\n.application-card:hover {\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);\n  transform: translateY(-5px);\n}\n\n.application-card h3 {\n  margin: 0;\n  width: 100%;\n  text-align: center;\n  position: absolute;\n  top: 32px;\n  left: 0;\n  color: #fff;\n}\n\n.application-card h6 {\n  margin: 0;\n  width: 100%;\n  text-align: center;\n  position: absolute;\n  top: 50%;\n  left: 0;\n  transform: translateY(-50%);\n  color: #fff;\n}\n\n.application-card .btn {\n  position: absolute;\n  left: 50%;\n  bottom: 24px;\n  transform: translateX(-50%);\n}\n\n.section-line {\n  position: absolute;\n  bottom: -10px;\n  left: 50%;\n  transform: translateX(-50%);\n  width: 150px;\n  height: 2px;\n  background-color: rgb(178,0,0);\n}\n\n@media (max-width: 992px) {\n  .applications-grid {\n    grid-template-columns: repeat(2, 1fr);\n  }\n}\n\n@media (max-width: 768px) {\n  .application-card {\n    height: 280px;\n  }\n\n  .application-card h3 {\n    font-size: 1.3rem;\n  }\n}\n\n@media (max-width: 600px) {\n  .applications-grid {\n    grid-template-columns: 1fr;\n  }\n\n  .application-card {\n    height: 220px;\n  }\n}\n</style>"],"mappings":";;EAEOA,KAAK,EAAC;AAAmB;;EAEvBA,KAAK,EAAC;AAAoB;;EAExBA,KAAK,EAAC;AAAgB;;EANjCC,GAAA;EAaSD,KAAK,EAAC;;;EACJA,KAAK,EAAC;AAAgB;;EACrBA,KAAK,EAAC;AAAoC;;EAC1CA,KAAK,EAAC;AAAkB;;EAEzBA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAmB;oBAnBtC;oBAAA;;;uBAEEE,mBAAA,CA8BM,OA9BNC,UA8BM,GA7BJC,mBAAA,eAAkB,EAClBC,mBAAA,CAMM,OANNC,UAMM,G,0BALLD,mBAAA,CAA8H;IAAzHE,GAAG,EAAC,uCAAuC;IAACC,GAAG,EAAC,oCAAoC;IAACR,KAAK,EAAC,YAAY;IAACS,OAAO,EAAC;+BACpHJ,mBAAA,CAGM,OAHNK,UAGM,GAFLL,mBAAA,CAA2B,YAAAM,gBAAA,CAApBC,QAAA,CAAAC,YAAY,kBACnBR,mBAAA,CAA0B,YAAAM,gBAAA,CAAnBC,QAAA,CAAAE,WAAW,iB,KAIrBV,mBAAA,WAAc,EAC0BW,IAAA,CAAAC,MAAM,CAACC,IAAI,wB,cAAnDf,mBAAA,CAeM,OAfNgB,UAeM,GAdJb,mBAAA,CAGM,OAHNc,UAGM,GAFJd,mBAAA,CAA2H,MAA3He,UAA2H,GAfnIC,gBAAA,CAAAV,gBAAA,CAe0DI,IAAA,CAAAO,EAAE,+C,0BAAgCjB,mBAAA,CAAkC;IAA5BL,KAAK,EAAC;EAAc,4B,GAC9GK,mBAAA,CAA6E,MAA7EkB,UAA6E,EAAAZ,gBAAA,CAA7CI,IAAA,CAAAO,EAAE,oD,GAEpCjB,mBAAA,CASM,OATNmB,UASM,GARJnB,mBAAA,CAOM,OAPNoB,UAOM,I,kBANJvB,mBAAA,CAKMwB,SAAA,QAzBhBC,WAAA,CAoB6BC,KAAA,CAAAC,YAAY,EAAnBC,GAAG;yBAAf5B,mBAAA,CAKM;MAL4BD,GAAG,EAAE6B,GAAG,CAACC,EAAE;MAAE/B,KAAK,EAAC,kBAAkB;MAAEgC,OAAK,EAAAC,MAAA,IAAErB,QAAA,CAAAsB,eAAe,CAACJ,GAAG,CAACK,IAAI;MACrGC,KAAK,EArBlBC,eAAA;QAAAC,eAAA,WAqBgDR,GAAG,CAACS,KAAK;MAAA;QAC3ClC,mBAAA,CAAwB,YAAAM,gBAAA,CAAjBmB,GAAG,CAACU,KAAK,kBAChBnC,mBAAA,CAAyB,YAAAM,gBAAA,CAAlBmB,GAAG,CAACW,MAAM,kBACnBpC,mBAAA,CAA+G;MAA3GqC,IAAI,EAAEZ,GAAG,CAACK,IAAI;MAAEnC,KAAK,EAAC,iCAAiC;MAAEgC,OAAK,EAAAW,MAAA,QAAAA,MAAA,MAxB9EC,cAAA,CAwBwE,QAAW;wBAAI7B,IAAA,CAAAO,EAAE,mDAxBzFuB,WAAA,E,yBAAAC,WAAA;0CAAA1C,mBAAA,gBA8BIA,mBAAA,UAAa,EACb2C,YAAA,CAAeC,sBAAA,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependenc