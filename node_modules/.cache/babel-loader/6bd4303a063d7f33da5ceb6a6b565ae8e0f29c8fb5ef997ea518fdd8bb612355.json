{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nexport default {\n  name: 'PlatformDetail',\n  data() {\n    return {\n      drone: null,\n      drones: []\n    };\n  },\n  created() {\n    const id = this.$route.params.id;\n    // 优先从本地 this.drones 查找\n    if (this.drones.length) {\n      this.drone = this.drones.find(d => d.id === id);\n    }\n    // 如果本地没有，尝试从 localStorage 获取\n    if (!this.drone) {\n      try {\n        const dronesStr = localStorage.getItem('allDrones');\n        if (dronesStr) {\n          const drones = JSON.parse(dronesStr);\n          this.drone = drones.find(d => d.id === id);\n        }\n      } catch (e) {}\n    }\n    // 如果还没有，自动内置一份静态数据\n    if (!this.drone) {\n      const staticDrones = [{\n        id: 'fpv-01',\n        type: 'fpv',\n        name: 'SpeedRacer Pro',\n        description: '专业穿越竞速无人机，极致的飞行体验',\n        image: '/images/home/product/product1.png',\n        gallery: ['/images/home/product/product1.png'],\n        features: ['高速穿越飞行体验', '低延迟FPV图传系统', '灵活操控与特技飞行', '高清航拍能力'],\n        specifications: {\n          '最大起飞重量': '0.8kg',\n          '续航时间': '20分钟',\n          '最大飞行速度': '180km/h',\n          '最大抗风等级': '5级'\n        },\n        applications: ['FPV竞速', '特技飞行', '航拍摄影', '娱乐休闲'],\n        video: ''\n      }, {\n        id: 'mr-01',\n        type: 'multiRotor',\n        name: 'QuadX 800',\n        description: '高精度多旋翼无人机，适合精细作业和复杂环境',\n        image: '/images/home/product/product2.png',\n        gallery: ['/images/home/product/product2.png'],\n        features: ['精准悬停与定位', '高效作业能力', '多任务载荷', '智能飞控系统'],\n        specifications: {\n          '最大起飞重量': '8kg',\n          '续航时间': '35分钟',\n          '最大飞行高度': '4500m',\n          '最大抗风等级': '7级'\n        },\n        applications: ['航拍', '测绘', '监控', '农业植保'],\n        video: ''\n      }];\n      this.drone = staticDrones.find(d => d.id === id);\n    }\n    if (!this.drone) {\n      console.warn('未找到产品', id);\n    }\n  },\n  methods: {\n    getMainSpecs(drone) {\n      const specKeys = {\n        fixedWing: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\n        multiRotor: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\n        helicopter: ['最大起飞重量', '最大载重', '续航时间', '最大飞行速度'],\n        fpv: ['机身尺寸', '重量(不含电池)', '最大速度', '续航时间'],\n        system: ['最大集群数量', '控制距离', '编队精度', '响应时间']\n      };\n      const type = drone.type || 'multiRotor';\n      return specKeys[type] || Object.keys(drone.specifications).slice(0, 4);\n    },\n    getBadgeClass(type) {\n      const classes = {\n        fixedWing: 'bg-primary',\n        multiRotor: 'bg-success',\n        helicopter: 'bg-danger',\n        fpv: 'bg-warning',\n        system: 'bg-info'\n      };\n      return classes[type] || 'bg-secondary';\n    },\n    getTypeName(type) {\n      const names = {\n        fixedWing: '固定翼',\n        multiRotor: '多旋翼',\n        helicopter: '直升机',\n        fpv: 'FPV',\n        system: '系统平台'\n      };\n      return names[type] || '未分类';\n    },\n    getApplicationIcon(app) {\n      return 'bi bi-rocket';\n    },\n    getApplicationDesc(app) {\n      return '';\n    }\n  }\n};","map":{"version":3,"names":["name","data","drone","drones","created","id","$route","params","length","find","d","dronesStr","localStorage","getItem","JSON","parse","e","staticDrones","type","description","image","gallery","features","specifications","applications","video","console","warn","methods","getMainSpecs","specKeys","fixedWing","multiRotor","helicopter","fpv","system","Object","keys","slice","getBadgeClass","classes","getTypeName","names","getApplicationIcon","app","getApplicationDesc"],"sources":["C:\\Users\\75132\\Desktop\\flyboot\\flyboot\\src\\views\\products\\details\\PlatformDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"platform-detail-page py-5\">\r\n    <div class=\"container\" v-if=\"drone\">\r\n      <!-- 返回按钮 -->\r\n      <div class=\"mb-4\">\r\n        <router-link :to=\"'/products/platform/' + drone.id.split('-')[0]\" class=\"btn btn-outline-secondary\">\r\n          <i class=\"bi bi-arrow-left me-2\"></i>返回产品列表\r\n        </router-link>\r\n      </div>\r\n\r\n      <!-- 产品标题 -->\r\n      <div class=\"row mb-4\">\r\n        <div class=\"col-12\">\r\n          <div class=\"product-badge mb-2\" :class=\"getBadgeClass(drone.type)\">\r\n            {{ getTypeName(drone.type) }}\r\n          </div>\r\n          <h1 class=\"mb-3\">{{ drone.name }}</h1>\r\n          <p class=\"lead text-secondary\">{{ drone.description }}</p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 产品导航标签 -->\r\n      <ul class=\"nav nav-tabs mb-4\" id=\"productTab\" role=\"tablist\">\r\n        <li class=\"nav-item\" role=\"presentation\">\r\n          <button class=\"nav-link active\" id=\"overview-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#overview\" type=\"button\" role=\"tab\">产品概述</button>\r\n        </li>\r\n        <li class=\"nav-item\" role=\"presentation\">\r\n          <button class=\"nav-link\" id=\"specs-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#specs\" type=\"button\" role=\"tab\">技术参数</button>\r\n        </li>\r\n        <li class=\"nav-item\" role=\"presentation\">\r\n          <button class=\"nav-link\" id=\"features-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#features\" type=\"button\" role=\"tab\">特点优势</button>\r\n        </li>\r\n        <li class=\"nav-item\" role=\"presentation\">\r\n          <button class=\"nav-link\" id=\"applications-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#applications\" type=\"button\" role=\"tab\">应用场景</button>\r\n        </li>\r\n        <li class=\"nav-item\" role=\"presentation\" v-if=\"drone.video\">\r\n          <button class=\"nav-link\" id=\"video-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#video\" type=\"button\" role=\"tab\">产品视频</button>\r\n        </li>\r\n      </ul>\r\n\r\n      <!-- 标签内容 -->\r\n      <div class=\"tab-content bg-white p-4 rounded shadow-sm\" id=\"productTabContent\">\r\n        <!-- 产品概述 -->\r\n        <div class=\"tab-pane fade show active\" id=\"overview\" role=\"tabpanel\">\r\n          <div class=\"row\">\r\n            <div class=\"col-lg-6\">\r\n              <div class=\"product-gallery\">\r\n                <div class=\"main-image mb-3\">\r\n                  <img :src=\"drone.image\" class=\"img-fluid rounded\" :alt=\"drone.name\">\r\n                </div>\r\n                <div class=\"row g-2\">\r\n                  <div class=\"col-3\" v-for=\"(img, index) in drone.gallery\" :key=\"index\">\r\n                    <img :src=\"img\" class=\"img-fluid rounded\" :alt=\"'产品图片 ' + (index + 1)\">\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-lg-6\">\r\n              <h3 class=\"product-title mb-3\">{{ drone.name }}</h3>\r\n              <p class=\"product-description mb-4\">{{ drone.description }}</p>\r\n              \r\n              <h5 class=\"mb-3\">主要特点</h5>\r\n              <div class=\"row g-3 mb-4\">\r\n                <div class=\"col-lg-6\" v-for=\"(feature, index) in drone.features.slice(0, 4)\" :key=\"index\">\r\n                  <div class=\"feature-item d-flex align-items-center\">\r\n                    <div class=\"feature-icon me-2 text-accent\">\r\n                      <i class=\"bi bi-check-circle-fill\"></i>\r\n                    </div>\r\n                    <div class=\"feature-text\">{{ feature }}</div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              \r\n              <h5 class=\"mb-3\">关键参数</h5>\r\n              <div class=\"row g-3\">\r\n                <div class=\"col-6\" v-for=\"(key, index) in getMainSpecs(drone)\" :key=\"index\">\r\n                  <div class=\"spec-box p-3 rounded bg-light\">\r\n                    <div class=\"spec-title text-secondary small\">{{ key }}</div>\r\n                    <div class=\"spec-value fw-bold\">{{ drone.specifications[key] }}</div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              \r\n              <div class=\"mt-4 pt-3 d-flex gap-3\">\r\n                <button class=\"btn btn-primary\">获取报价</button>\r\n                <button class=\"btn btn-outline-secondary\">下载产品手册</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 技术参数 -->\r\n        <div class=\"tab-pane fade\" id=\"specs\" role=\"tabpanel\">\r\n          <div class=\"row\">\r\n            <div class=\"col-lg-6\">\r\n              <div class=\"table-responsive\">\r\n                <table class=\"table table-hover\">\r\n                  <thead>\r\n                    <tr>\r\n                      <th scope=\"col\" class=\"bg-light\">参数</th>\r\n                      <th scope=\"col\" class=\"bg-light\">规格</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    <tr v-for=\"(spec, key) in drone.specifications\" :key=\"key\">\r\n                      <td>{{ key }}</td>\r\n                      <td>{{ spec }}</td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-lg-6\">\r\n              <img :src=\"drone.image\" class=\"img-fluid rounded\" :alt=\"drone.name\">\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 特点优势 -->\r\n        <div class=\"tab-pane fade\" id=\"features\" role=\"tabpanel\">\r\n          <div class=\"row\">\r\n            <div class=\"col-lg-6\">\r\n              <h4 class=\"mb-4\">产品特点</h4>\r\n              <ul class=\"feature-list\">\r\n                <li v-for=\"(feature, index) in drone.features\" :key=\"index\" class=\"mb-3\">\r\n                  <div class=\"d-flex\">\r\n                    <div class=\"feature-icon text-accent me-3\">\r\n                      <i class=\"bi bi-check-circle-fill fs-4\"></i>\r\n                    </div>\r\n                    <div>\r\n                      <p class=\"mb-0\">{{ feature }}</p>\r\n                    </div>\r\n                  </div>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n            <div class=\"col-lg-6\">\r\n              <div class=\"feature-gallery\">\r\n                <img :src=\"drone.gallery[0]\" class=\"img-fluid rounded mb-3\" alt=\"产品特点\">\r\n                <div class=\"row g-3\">\r\n                  <div class=\"col-6\" v-for=\"(img, index) in drone.gallery.slice(1)\" :key=\"index\">\r\n                    <img :src=\"img\" class=\"img-fluid rounded\" :alt=\"'产品特点 ' + (index + 1)\">\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 应用场景 -->\r\n        <div class=\"tab-pane fade\" id=\"applications\" role=\"tabpanel\">\r\n          <div class=\"row\">\r\n            <div class=\"col-12 mb-4\">\r\n              <h4>适用场景</h4>\r\n              <p class=\"text-secondary\">本产品适用于以下行业和应用场景</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"row g-4\">\r\n            <div class=\"col-lg-4 col-md-6\" v-for=\"(app, index) in drone.applications\" :key=\"index\">\r\n              <div class=\"application-card p-4 rounded shadow-sm h-100\">\r\n                <div class=\"application-icon mb-3 text-accent\">\r\n                  <i :class=\"getApplicationIcon(app)\"></i>\r\n                </div>\r\n                <h5>{{ app }}</h5>\r\n                <p class=\"text-secondary mb-0\">{{ getApplicationDesc(app) }}</p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 产品视频 -->\r\n        <div class=\"tab-pane fade\" id=\"video\" role=\"tabpanel\" v-if=\"drone.video\">\r\n          <div class=\"row\">\r\n            <div class=\"col-12 mb-4\">\r\n              <h4>产品视频展示</h4>\r\n              <p class=\"text-secondary\">观看详细的产品演示和操作视频</p>\r\n            </div>\r\n            <div class=\"col-12\">\r\n              <div class=\"ratio ratio-16x9\">\r\n                <iframe :src=\"drone.video\" title=\"产品视频\" allowfullscreen></iframe>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n    <div class=\"container\" v-else>\r\n      <div class=\"alert alert-warning\">\r\n        产品信息加载中或未找到，请稍候...\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'PlatformDetail',\r\n  data() {\r\n    return {\r\n      drone: null,\r\n      drones: []\r\n    }\r\n  },\r\n  created() {\r\n    const id = this.$route.params.id;\r\n    // 优先从本地 this.drones 查找\r\n    if (this.drones.length) {\r\n      this.drone = this.drones.find(d => d.id === id);\r\n    }\r\n    // 如果本地没有，尝试从 localStorage 获取\r\n    if (!this.drone) {\r\n      try {\r\n        const dronesStr = localStorage.getItem('allDrones');\r\n        if (dronesStr) {\r\n          const drones = JSON.parse(dronesStr);\r\n          this.drone = drones.find(d => d.id === id);\r\n        }\r\n      } catch (e) {}\r\n    }\r\n    // 如果还没有，自动内置一份静态数据\r\n    if (!this.drone) {\r\n      const staticDrones = [\r\n        {\r\n          id: 'fpv-01',\r\n          type: 'fpv',\r\n          name: 'SpeedRacer Pro',\r\n          description: '专业穿越竞速无人机，极致的飞行体验',\r\n          image: '/images/home/product/product1.png',\r\n          gallery: ['/images/home/product/product1.png'],\r\n          features: ['高速穿越飞行体验', '低延迟FPV图传系统', '灵活操控与特技飞行', '高清航拍能力'],\r\n          specifications: {\r\n            '最大起飞重量': '0.8kg',\r\n            '续航时间': '20分钟',\r\n            '最大飞行速度': '180km/h',\r\n            '最大抗风等级': '5级'\r\n          },\r\n          applications: ['FPV竞速', '特技飞行', '航拍摄影', '娱乐休闲'],\r\n          video: ''\r\n        },\r\n        {\r\n          id: 'mr-01',\r\n          type: 'multiRotor',\r\n          name: 'QuadX 800',\r\n          descr