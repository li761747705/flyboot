{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nexport default {\n  name: 'MultiRotorDetail',\n  data() {\n    return {\n      products: {\n        'drone1': {\n          name: 'WW-1 六旋翼无人机',\n          banner: 'WW-1 六旋翼无人机',\n          flyingImg: '/images/placeholder.svg',\n          structureImg: '/images/placeholder.svg',\n          basic: [['机身尺寸', '1.2 m', '最大起飞重量', '8 kg'], ['最大载重', '2 kg', '旋翼数量', '6轴'], ['机身材质', '高强度合金+碳纤维', '动力系统', '无刷电机']],\n          tech: [['最大飞行速度', '60 km/h', '巡航速度', '40 km/h'], ['最大航程', '50 km', '最大飞行高度', '3000 m'], ['续航时间', '45 分钟', '抗风能力', '6 级']]\n        },\n        'drone2': {\n          name: 'WW-2 八旋翼无人机',\n          banner: 'WW-2 八旋翼无人机',\n          flyingImg: '/images/placeholder.svg',\n          structureImg: '/images/placeholder.svg',\n          basic: [['机身尺寸', '1.5 m', '最大起飞重量', '15 kg'], ['最大载重', '5 kg', '旋翼数量', '8轴'], ['机身材质', '高强度合金+碳纤维', '动力系统', '无刷电机']],\n          tech: [['最大飞行速度', '70 km/h', '巡航速度', '50 km/h'], ['最大航程', '80 km', '最大飞行高度', '4000 m'], ['续航时间', '60 分钟', '抗风能力', '7 级']]\n        },\n        'drone3': {\n          name: 'WW-3 折叠无人机',\n          banner: 'WW-3 折叠无人机',\n          flyingImg: '/images/placeholder.svg',\n          structureImg: '/images/placeholder.svg',\n          basic: [['机身尺寸', '0.8 m', '最大起飞重量', '5 kg'], ['最大载重', '1.5 kg', '旋翼数量', '4轴'], ['机身材质', '高强度合金+碳纤维', '动力系统', '无刷电机']],\n          tech: [['最大飞行速度', '50 km/h', '巡航速度', '35 km/h'], ['最大航程', '30 km', '最大飞行高度', '2500 m'], ['续航时间', '35 分钟', '抗风能力', '5 级']]\n        },\n        'drone4': {\n          name: 'WW-4 工业无人机',\n          banner: 'WW-4 工业无人机',\n          flyingImg: '/images/placeholder.svg',\n          structureImg: '/images/placeholder.svg',\n          basic: [['机身尺寸', '1.8 m', '最大起飞重量', '25 kg'], ['最大载重', '8 kg', '旋翼数量', '8轴'], ['机身材质', '高强度合金+碳纤维', '动力系统', '无刷电机']],\n          tech: [['最大飞行速度', '80 km/h', '巡航速度', '60 km/h'], ['最大航程', '100 km', '最大飞行高度', '5000 m'], ['续航时间', '90 分钟', '抗风能力', '8 级']]\n        }\n      },\n      current: null\n    };\n  },\n  created() {\n    const id = this.$route.params.id;\n    this.current = this.products[id] || this.products['drone1'];\n  },\n  methods: {\n    goBack() {\n      this.$router.push('/products/multi-rotor');\n    }\n  },\n  mounted() {\n    if (typeof bootstrap !== 'undefined') {\n      const tabList = document.querySelectorAll('#productTabs button');\n      tabList.forEach(tab => {\n        new bootstrap.Tab(tab);\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","products","banner","flyingImg","structureImg","basic","tech","current","created","id","$route","params","methods","goBack","$router","push","mounted","bootstrap","tabList","document","querySelectorAll","forEach","tab","Tab"],"sources":["C:\\Users\\75132\\Desktop\\flyboot\\flyboot\\src\\views\\products\\details\\MultiRotorDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"drone-detail\">\r\n    <!-- 返回按钮 -->\r\n    <div class=\"back-btn-wrap container\">\r\n      <button class=\"btn btn-outline-secondary\" @click=\"goBack\">\r\n        <i class=\"bi bi-arrow-left\"></i> 返回\r\n      </button>\r\n    </div>\r\n\r\n    <!-- 顶部实景图 -->\r\n    <section class=\"hero-section\">\r\n      <div class=\"container\">\r\n        <div class=\"hero-banner\">\r\n          <h1 class=\"section-title text-white\">{{ current.name }}</h1>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- 产品详情 -->\r\n    <section class=\"product-details\">\r\n      <div class=\"container\">\r\n        <!-- 飞行性能展示 -->\r\n        <div class=\"content-section\">\r\n          <div class=\"row align-items-center\">\r\n            <div class=\"col-md-6\">\r\n              <img :src=\"current.flyingImg\" alt=\"飞行展示\" class=\"img-fluid content-image\" />\r\n            </div>\r\n            <div class=\"col-md-6\">\r\n              <div class=\"content-box\">\r\n                <h2 class=\"section-title\">飞行性能</h2>\r\n                <p class=\"section-text\">\r\n                  多旋翼无人机具备高精度、强稳定性和可靠耐用的特点，适合专业级航拍、测绘、巡检等场景。采用先进的飞控系统和动力配置，确保在各种复杂环境下都能稳定飞行，实现精准的作业任务。\r\n                </p>\r\n                <ul class=\"feature-list\">\r\n                  <li><i class=\"bi bi-check2-circle\"></i> 高精度定位系统</li>\r\n                  <li><i class=\"bi bi-check2-circle\"></i> 强稳定性控制</li>\r\n                  <li><i class=\"bi bi-check2-circle\"></i> 可靠耐用设计</li>\r\n                  <li><i class=\"bi bi-check2-circle\"></i> 专业级作业能力</li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 高强结构展示 -->\r\n        <div class=\"content-section\">\r\n          <div class=\"row align-items-center flex-row-reverse\">\r\n            <div class=\"col-md-6\">\r\n              <img :src=\"current.structureImg\" alt=\"高强结构\" class=\"img-fluid content-image\" />\r\n            </div>\r\n            <div class=\"col-md-6\">\r\n              <div class=\"content-box\">\r\n                <h2 class=\"section-title\">高强结构</h2>\r\n                <p class=\"section-text\">\r\n                  采用高强度合金和碳纤维材料，机身坚固耐用，适应恶劣环境，保障飞行安全。碳纤维复合材料的高比强度和比刚度特性使得无人机在保证结构强度的同时，重量大大减轻，具有良好的抗疲劳性和耐久性。\r\n                </p>\r\n                <ul class=\"feature-list\">\r\n                  <li><i class=\"bi bi-check2-circle\"></i> 高强度合金结构</li>\r\n                  <li><i class=\"bi bi-check2-circle\"></i> 碳纤维机身设计</li>\r\n                  <li><i class=\"bi bi-check2-circle\"></i> 耐用抗疲劳性能</li>\r\n                  <li><i class=\"bi bi-check2-circle\"></i> 优秀的结构强度</li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 产品参数标签页 -->\r\n        <div class=\"content-section specs-tabs\">\r\n          <div class=\"container\">\r\n            <ul class=\"nav nav-tabs\" id=\"productTabs\" role=\"tablist\">\r\n              <li class=\"nav-item\" role=\"presentation\">\r\n                <button class=\"nav-link active\" id=\"basic-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#basic\" type=\"button\" role=\"tab\">\r\n                  基本参数\r\n                </button>\r\n              </li>\r\n              <li class=\"nav-item\" role=\"presentation\">\r\n                <button class=\"nav-link\" id=\"tech-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#tech\" type=\"button\" role=\"tab\">\r\n                  技术参数\r\n                </button>\r\n              </li>\r\n              <li class=\"nav-item\" role=\"presentation\">\r\n                <button class=\"nav-link\" id=\"package-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#package\" type=\"button\" role=\"tab\">\r\n                  包装清单\r\n                </button>\r\n              </li>\r\n            </ul>\r\n\r\n            <div class=\"tab-content mt-4\" id=\"productTabContent\">\r\n              <!-- 基本参数 -->\r\n              <div class=\"tab-pane fade show active\" id=\"basic\" role=\"tabpanel\">\r\n                <div class=\"table-responsive\">\r\n                  <table class=\"table table-bordered\">\r\n                    <tbody>\r\n                      <tr>\r\n                        <th>机身尺寸</th>\r\n                        <td>{{ current.basic[0][1] }}</td>\r\n                        <th>最大起飞重量</th>\r\n                        <td>{{ current.basic[0][3] }}</td>\r\n                      </tr>\r\n                      <tr>\r\n                        <th>最大载重</th>\r\n                        <td>{{ current.basic[1][1] }}</td>\r\n                        <th>旋翼数量</th>\r\n                        <td>{{ current.basic[1][3] }}</td>\r\n                      </tr>\r\n                      <tr>\r\n                        <th>机身材质</th>\r\n                        <td>{{ current.basic[2][1] }}</td>\r\n                        <th>动力系统</th>\r\n                        <td>{{ current.basic[2][3] }}</td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n\r\n              <!-- 技术参数 -->\r\n              <div class=\"tab-pane fade\" id=\"tech\" role=\"tabpanel\">\r\n                <div class=\"table-responsive\">\r\n                  <table class=\"table table-bordered\">\r\n                    <tbody>\r\n                      <tr>\r\n                        <th>最大飞行速度</th>\r\n                        <td>{{ current.tech[0][1] }}</td>\r\n                        <th>巡航速度</th>\r\n                        <td>{{ current.tech[0][3] }}</td>\r\n                      </tr>\r\n                      <tr>\r\n                        <th>最大航程</th>\r\n                        <td>{{ current.tech[1][1] }}</td>\r\n                        <th>最大飞行高度</th>\r\n                        <td>{{ current.tech[1][3] }}</td>\r\n                      </tr>\r\n                      <tr>\r\n                        <th>续航时间</th>\r\n                        <td>{{ current.tech[2][1] }}</td>\r\n                        <th>抗风能力</th>\r\n                        <td>{{ current.tech[2][3] }}</td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n\r\n              <!-- 包装清单 -->\r\n              <div class=\"tab-pane fade\" id=\"package\" role=\"tabpanel\">\r\n                <div class=\"package-list\">\r\n                  <div class=\"row\">\r\n                    <div class=\"col-md-6\">\r\n                      <h4>标配清单</h4>\r\n                      <ul class=\"list-unstyled\">\r\n                        <li><i class=\"bi bi-check2\"></i> 飞行器机身 × 1</li>\r\n                        <li><i class=\"bi bi-check2\"></i> 螺旋桨 × 6</li>\r\n                        <li><i class=\"bi bi-check2\"></i> 智能飞控 × 1</li>\r\n                        <li><i class=\"bi bi-check2\"></i> 动力电池 × 2</li>\r\n                        <li><i class=\"bi bi-check2\"></i> 充电器 × 1</li>\r\n                        <li><i class=\"bi bi-check2\"></i> 遥控器 × 1</li>\r\n                      </ul>\r\n                    </div>\r\n                    <div class=\"col-md-6\">\r\n                      <h4>随机文件</h4>\r\n                      <ul class=\"list-unstyled\">\r\n                        <li><i class=\"bi bi-file-text\"></i> 产品说明书</li>\r\n                        <li><i class=\"bi bi-file-text\"></i> 快速入门指南</li>\r\n                        <li><i class=\"bi bi-file-text\"></i> 免责声明</li>\r\n                        <li><i class=\"bi bi-file-text\"></i> 合格证</li>\r\n                      </ul>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 保修服务展示区 -->\r\n        <div class=\"content-section warranty-section\">\r\n          <h2 class=\"section-title text-center\">保修服务</h2>\r\n          <ol class=\"warranty-list\">\r\n            <li>\r\n              <strong>保修期限：</strong>\r\n              我们承诺对提供得产品给予一年保修期限，在这期间，客户可以享受到全方位保修服务。从产品交付之日起计算，无论是产品硬件故障，还是因产品质量问题导致性能异常等情况，我们将负责解决。\r\n            </li>\r\n            <li>\r\n              <strong>硬件维修与更换：</strong>\r\n              涵盖产品得各个零部件，如主板、显示屏、硬盘、电源等，若这些硬件出现非人为损坏故障，我们将免费进行维修或更换。\r\n            </li>\r\n            <li>\r\n              <strong>一般故障：</strong>\r\n              对于一般故障，客户可通过电话、邮件等方式向我们反馈，我们的客服人员会在接到反馈后2小时回复客户，确认已收到问题，然后，根据故障具体情况，在24小时内为客户提供解决方案。\r\n            </li>\r\n          </ol>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'MultiRotorDetail',\r\n  data() {\r\n    return {\r\n      products: {\r\n        'drone1': {\r\n          name: 'WW-1 六旋翼无人机',\r\n          banner: 'WW-1 六旋翼无人机',\r\n          flyingImg: '/images/placeholder.svg',\r\n          structureImg: '/images/placeholder.svg',\r\n          basic: [\r\n            ['机身尺寸', '1.2 m', '最大起飞重量', '8 kg'],\r\n            ['最大载重', '2 kg', '旋翼数量', '6轴'],\r\n            ['机身材质', '高强度合金+碳纤维', '动力系统', '无刷电机']\r\n          ],\r\n          tech: [\r\n            ['最大飞行速度', '60 km/h', '巡航速度', '40 km/h'],\r\n            ['最大航程', '50 km', '最大飞行高度', '3000 m'],\r\n            ['续航时间', '45 分钟', '抗风能力', '6 级']\r\n          ]\r\n        },\r\n        'drone2': {\r\n          name: 'WW-2 八旋翼无人机',\r\n          banner: 'WW-2 八旋翼无人机',\r\n          flyingImg: '/images/placeholder.svg',\r\n          structureImg: '/images/placeholder.svg',\r\n          basic: [\r\n            ['机身尺寸', '1.5 m', '最大起飞重量', '15 kg'],\r\n            ['最大载重', '5 kg', '旋翼数量', '8轴'],\r\n            ['机身材质', '高强度合金+碳纤维', '动力系统', '无刷电机']\r\n          ],\r\n          tech: [\r\n            ['最大飞行速度', '70 km/h', '巡航速度', '50 km/h'],\r\n            ['最大航程', '80 km', '最大飞行高度', '4000 m'],\r\n            ['续航时间', '60 分钟', '抗风能力', '7 级']\r\n          ]\r\n        },\r\n        'drone3': {\r\n          name: 'WW-3 折叠无人机',\r\n          banner: 'WW-3 折叠无人机',\r\n          flyingImg: '/images/placeholder.svg',\r\n          structureImg: '/images/placeholder.svg',\r\n          basic: [\r\n            ['机身尺寸', '0.8 m', '最大起飞重量', '5 kg'],\r\n            ['最大载重', '1.5 kg', '旋翼数量', '4轴'],\r\n            ['机身材质', '高强度合金+碳纤维', '动力系统', '无刷电机']\r\n          ],\r\n          tech: [\r\n            ['最大飞行速度', '50 km/h', '巡航速度', '35 km/h'],\r\n            ['最大航程', '30 km', '最大飞行高度', '2500 m'],\r\n            ['续航时间', '35 分钟', '抗风能力', '5 级']\r\n          ]\r\n        },\r\n        'drone4': {\r\n          name: 'WW-4 工业无人机',\r\n          banner: 'WW-4 工业无人机',\r\n          flyingImg: '/images/placeholder.svg',\r\n          structureImg: '/images/placeholder.svg',\r\n          basic: [\r\n            ['机身尺寸', '1.8 m', '最大起飞重量', '25 kg'],\r\n            ['最大载重', '8 kg', '旋翼数量', '8轴'],\r\n            ['机身材质', '高强度合金+碳纤维', '动力系统', '无刷电机']\r\n          ],\r\n          tech: [\r\n            ['最大飞行速度', '80 km/h', '巡航速度', '60 km/h'],\r\n        