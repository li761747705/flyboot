height: '700px',\n        link: '/support'\n      },\n      allSubBanners: {\n        downloads: [\n          {\n            title: '下载中心',\n            bgImg: '/images/home/download.png',\n            height: '500px',\n            link: '/support/downloads'\n          }\n        ],\n        policy: [\n          {\n            title: '售后服务政策',\n            bgImg: '/images/home/after-sale.png',\n            height: '600px',\n            link: '/support/policy'\n          }\n        ],\n        faq: [\n          {\n            title: '常见问题',\n            bgImg: '/images/home/after-sale.png',\n            height: '600px',\n            link: '/support/faq'\n          }\n        ],\n        hotline: [\n          {\n            title: '服务热线',\n            subtitle: '热线电话：0000000000',\n            subtitle2: '周一至周六：9：00-18：00',\n            bgImg: '/images/home/after-sale.png',\n            height: '600px',\n            link: '/support/hotline'\n          }\n        ]\n      }\n    }\n  },\n  computed: {\n    subBanners() {\n      // 获取当前子路由名\n      const name = this.$route.name;\n      // Downloads, Policy, FAQ, Hotline\n      if (name && this.allSubBanners[name.toLowerCase()]) {\n        return this.allSubBanners[name.toLowerCase()];\n      }\n      return [];\n    }\n  },\n  methods: {\n    goTo(link) {\n      this.$router.push(link)\n    }\n  }\n}\n</script>\n\n<style scoped>\n.text-accent {\n  color: var(--accent-color);\n}\n\n.bg-accent {\n  background-color: var(--accent-color);\n  color: white;\n}\n\n.timeline {\n  position: relative;\n  padding: 2rem 0;\n}\n\n.timeline::before {\n  content: '';\n  position: absolute;\n  height: 100%;\n  width: 2px;\n  background: var(--block-background);\n  left: 50px;\n  top: 0;\n}\n\n.timeline-item {\n  margin-bottom: 3rem;\n  position: relative;\n}\n\n.timeline-item:last-child {\n  margin-bottom: 0;\n}\n\n.timeline-icon {\n  width: 50px;\n  height: 50px;\n  border-radius: 50%;\n  position: absolute;\n  left: 25px;\n  transform: