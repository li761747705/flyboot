{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: 'Products',\n  data() {\n    return {\n      activeCategory: 'all',\n      searchTerm: '',\n      drones: [{\n        id: 'fpv-01',\n        type: 'fpv',\n        name: 'SpeedRacer Pro',\n        description: '专业穿越竞速无人机，极致的飞行体验',\n        image: '/images/home/product/product1.png',\n        specifications: {\n          '最大起飞重量': '0.8kg',\n          '续航时间': '20分钟',\n          '最大飞行速度': '180km/h',\n          '最大抗风等级': '5级'\n        }\n      }, {\n        id: 'fpv-02',\n        type: 'fpv',\n        name: 'FreeStyle X',\n        description: '自由式特技飞行无人机，适合创意表演',\n        image: '/images/products/drones/freestyle.jpg',\n        specifications: {\n          '最大起飞重量': '0.9kg',\n          '续航时间': '18分钟',\n          '最大飞行速度': '160km/h',\n          '最大抗风等级': '5级'\n        }\n      }, {\n        id: 'mr-01',\n        type: 'multiRotor',\n        name: 'QuadX 800',\n        description: '高精度多旋翼无人机，适合精细作业和复杂环境',\n        image: '/images/home/product/product2.png',\n        specifications: {\n          '最大起飞重量': '8kg',\n          '续航时间': '35分钟',\n          '最大飞行高度': '4500m',\n          '最大抗风等级': '7级'\n        }\n      }, {\n        id: 'mr-02',\n        type: 'multiRotor',\n        name: 'HexaCopter Pro',\n        description: '六旋翼专业航拍无人机，稳定性极佳',\n        image: '/images/products/drones/hexacopter.jpg',\n        specifications: {\n          '最大起飞重量': '10kg',\n          '续航时间': '40分钟',\n          '最大飞行高度': '5000m',\n          '最大抗风等级': '7级'\n        }\n      }, {\n        id: 'fw-01',\n        type: 'fixedWing',\n        name: 'FlyWing Pro 200',\n        description: '长航时固定翼无人机，适合大面积测绘和巡检',\n        image: '/images/home/product/product3.png',\n        specifications: {\n          '最大起飞重量': '5.2kg',\n          '续航时间': '120分钟',\n          '最大飞行高度': '5000m',\n          '最大抗风等级': '6级'\n        }\n      }, {\n        id: 'fw-02',\n        type: 'fixedWing',\n        name: 'SurveillanceWing X3',\n        description: '专业航测固定翼，高精度摄影测量系统',\n        image: '/images/products/drones/surveillance.jpg',\n        specifications: {\n          '最大起飞重量': '6.5kg',\n          '续航时间': '150分钟',\n          '最大飞行高度': '6000m',\n          '最大抗风等级': '6级'\n        }\n      }, {\n        id: 'hc-01',\n        type: 'helicopter',\n        name: 'RotorOne X5',\n        description: '工业级无人直升机，适合重载和特殊任务',\n        image: '/images/home/product/product4.png',\n        specifications: {\n          '最大起飞重量': '25kg',\n          '续航时间': '60分钟',\n          '最大飞行高度': '3500m',\n          '最大抗风等级': '8级'\n        }\n      }, {\n        id: 'hc-02',\n        type: 'helicopter',\n        name: 'CargoLifter H8',\n        description: '重型物流无人直升机，大载重运输能力',\n        image: '/images/products/drones/cargolifter.jpg',\n        specifications: {\n          '最大起飞重量': '80kg',\n          '续航时间': '45分钟',\n          '最大飞行高度': '3000m',\n          '最大抗风等级': '9级'\n        }\n      }]\n    };\n  },\n  methods: {\n    setCategory(category) {\n      this.activeCategory = category;\n    },\n    getFpvDrones() {\n      return this.drones.filter(drone => drone.type === 'fpv');\n    },\n    getMultiRotorDrones() {\n      return this.drones.filter(drone => drone.type === 'multiRotor');\n    },\n    getFixedWingDrones() {\n      return this.drones.filter(drone => drone.type === 'fixedWing');\n    },\n    getHelicopterDrones() {\n      return this.drones.filter(drone => drone.type === 'helicopter');\n    },\n    getMainSpecs(drone) {\n      return Object.entries(drone.specifications).slice(0, 3);\n    }\n  }\n};","map":{"version":3,"names":["name","data","activeCategory","searchTerm","drones","id","type","description","image","specifications","methods","setCategory","category","getFpvDrones","filter","drone","getMultiRotorDrones","getFixedWingDrones","getHelicopterDrones","getMainSpecs","Object","entries","slice"],"sources":["C:\\Users\\75132\\Desktop\\flyboot\\flyboot\\src\\views\\Products.vue"],"sourcesContent":["<template>\r\n <template v-if=\"$route.path === '/products'\">\r\n\r\n  <div class=\"products-page\">\r\n    <!-- 产品页面头部 -->\r\n    <div class=\"page-header position-relative\">\r\n      <div class=\"container\">\r\n        <div class=\"row justify-content-center\">\r\n          <div class=\"col-lg-8 text-center\">\r\n            <h1 class=\"display-4 fw-bold text-white mb-3 animate__animated animate__fadeInDown\">产品系列</h1>\r\n            <p class=\"lead text-white mb-4 animate__animated animate__fadeInUp animate__delay-1s\">专业的无人机解决方案提供商，打造高性能、高可靠性的飞行平台</p>\r\n            <div class=\"header-stats d-flex justify-content-center animate__animated animate__fadeIn animate__delay-2s\">\r\n              <div class=\"stat-item\">\r\n                <div class=\"stat-value\">4</div>\r\n                <div class=\"stat-label\">产品系列</div>\r\n              </div>\r\n              <div class=\"stat-item\">\r\n                <div class=\"stat-value\">20+</div>\r\n                <div class=\"stat-label\">产品型号</div>\r\n              </div>\r\n              <div class=\"stat-item\">\r\n                <div class=\"stat-value\">100+</div>\r\n                <div class=\"stat-label\">应用场景</div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"header-wave\">\r\n        <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 1440 100\">\r\n          <path fill=\"#ffffff\" fill-opacity=\"1\" d=\"M0,64L80,58.7C160,53,320,43,480,48C640,53,800,75,960,80C1120,85,1280,75,1360,69.3L1440,64L1440,100L1360,100C1280,100,1120,100,960,100C800,100,640,100,480,100C320,100,160,100,80,100L0,100Z\"></path>\r\n        </svg>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 主内容区域 -->\r\n    <div class=\"container py-4 py-md-5\">\r\n      <!-- 产品分类导航 -->\r\n      <div class=\"row mb-5\">\r\n        <div class=\"col-12\">\r\n          <div class=\"product-nav-container\">\r\n            <ul class=\"nav nav-pills nav-fill product-nav mb-4\">\r\n              <li class=\"nav-item\">\r\n                <button class=\"nav-link\" :class=\"{ active: activeCategory === 'all' }\" @click=\"setCategory('all')\">\r\n                  <i class=\"bi bi-grid-3x3-gap me-2\"></i>全部产品\r\n                </button>\r\n              </li>\r\n              <li class=\"nav-item\">\r\n                <button class=\"nav-link\" :class=\"{ active: activeCategory === 'fpv' }\" @click=\"setCategory('fpv')\">\r\n                  <i class=\"bi bi-camera-video me-2\"></i>穿越无人机\r\n                </button>\r\n              </li>\r\n              <li class=\"nav-item\">\r\n                <button class=\"nav-link\" :class=\"{ active: activeCategory === 'multiRotor' }\" @click=\"setCategory('multiRotor')\">\r\n                  <i class=\"bi bi-hexagon me-2\"></i>多旋翼无人机\r\n                </button>\r\n              </li>\r\n              <li class=\"nav-item\">\r\n                <button class=\"nav-link\" :class=\"{ active: activeCategory === 'fixedWing' }\" @click=\"setCategory('fixedWing')\">\r\n                  <i class=\"bi bi-airplane me-2\"></i>固定翼无人机\r\n                </button>\r\n              </li>\r\n              <li class=\"nav-item\">\r\n                <button class=\"nav-link\" :class=\"{ active: activeCategory === 'helicopter' }\" @click=\"setCategory('helicopter')\">\r\n                  <i class=\"bi bi-wind me-2\"></i>无人直升机\r\n                </button>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n          <div class=\"search-filter text-center mb-5\">\r\n            <div class=\"input-group mx-auto\" style=\"max-width: 500px;\">\r\n              <span class=\"input-group-text bg-light\"><i class=\"bi bi-search\"></i></span>\r\n              <input type=\"text\" class=\"form-control\" placeholder=\"搜索产品...\" v-model=\"searchTerm\">\r\n              <button class=\"btn btn-primary\" type=\"button\">搜索</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 穿越无人机部分 -->\r\n      <div class=\"row mb-5\" v-if=\"activeCategory === 'fpv' || activeCategory === 'all'\">\r\n        <div class=\"col-12\">\r\n          <h2 class=\"section-title mb-4\">穿越无人机</h2>\r\n          <p class=\"section-description mb-4\">高速穿越无人机，搭载高清FPV系统，提供极致的飞行体验，适用于竞速、特技飞行和航拍创作。</p>\r\n          \r\n          <!-- 穿越无人机产品展示 -->\r\n          <div class=\"row g-4\">\r\n            <div class=\"col-12 col-md-6 col-lg-4\" v-for=\"drone in getFpvDrones()\" :key=\"drone.id\" \r\n                 v-show=\"!searchTerm || drone.name.toLowerCase().includes(searchTerm.toLowerCase()) || \r\n                        drone.description.toLowerCase().includes(searchTerm.toLowerCase())\">\r\n              <div class=\"product-card animate__animated animate__fadeInUp\">\r\n                <div class=\"product-image-container\">\r\n                  <img :src=\"drone.image\" :alt=\"drone.name\" class=\"img-fluid\">\r\n                </div>\r\n                <div class=\"product-overlay\">\r\n                  <div class=\"product-content\">\r\n                    <div class=\"product-badge bg-warning\">穿越无人机</div>\r\n                    <h3 class=\"product-title\">{{drone.name}}</h3>\r\n                    <p class=\"product-brief\">{{drone.description}}</p>\r\n                    <div class=\"product-actions\">\r\n                      <router-link :to=\"'/products/platform/detail/' + drone.id\" class=\"btn btn-light rounded-pill px-4\">查看详情</router-link>\r\n                      <button class=\"btn btn-outline-light btn-icon ms-2 rounded-circle\" title=\"添加收藏\">\r\n                        <i class=\"bi bi-heart\"></i>\r\n                      </button>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"product-tags\">\r\n                  <span class=\"product-tag\" v-for=\"(value, key) in getMainSpecs(drone)\" :key=\"key\">\r\n                    {{key}}: {{value}}\r\n                  </span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          \r\n          <!-- 穿越无人机特点 -->\r\n          <div class=\"row mt-5 mb-5\">\r\n            <div class=\"col-12\">\r\n              <div class=\"features-card p-4 animate__animated animate__fadeIn\">\r\n                <div class=\"feature-header mb-4\">\r\n                  <h4 class=\"mb-3\"><i class=\"bi bi-camera-video-fill text-warning me-2\"></i>穿越无人机特点</h4>\r\n                  <p class=\"feature-subtitle\">体验极速飞行与卓越性能，为特技表演和竞速比赛而生</p>\r\n                </div>\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-6\">\r\n                    <ul class=\"feature-list\">\r\n                      <li class=\"animate__animated animate__fadeInLeft animate__delay-1s\">\r\n                        <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>高速穿越飞行体验\r\n                        <span class=\"feature-desc\">最高飞行速度可达180km/h，满足竞速需求</span>\r\n                      </li>\r\n                      <li class=\"animate__animated animate__fadeInLeft animate__delay-2s\">\r\n                        <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>低延迟FPV图传系统\r\n                        <span class=\"feature-desc\">图传延迟低至28ms，实时响应控制指令</span>\r\n                      </li>\r\n                      <li class=\"animate__animated animate__fadeInLeft animate__delay-3s\">\r\n                        <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>灵活操控与特技飞行\r\n                        <span class=\"feature-desc\">支持360°翻转、急速俯冲等复杂动作</span>\r\n                      </li>\r\n                    </ul>\r\n                  </div>\r\n                  <div class=\"col-md-6\">\r\n                    <ul class=\"feature-list\">\r\n                      <li class=\"animate__animated animate__fadeInRight animate__delay-1s\">\r\n                        <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>高清航拍能力\r\n                        <span class=\"feature-desc\">搭载4K高清相机，捕捉稳定流畅的画面</span>\r\n                      </li>\r\n                      <li class=\"animate__animated animate__fadeInRight animate__delay-2s\">\r\n                        <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>轻量化设计\r\n                        <span class=\"feature-desc\">采用碳纤维材料，整机重量低至0.8kg</span>\r\n                      </li>\r\n                      <li class=\"animate__animated animate__fadeInRight animate__delay-3s\">\r\n                        <i class=\"bi bi-check-circle-fill text-warning me-2\"></i>竞速与表演场景应用\r\n                        <span class=\"feature-desc\">广泛应用于竞速比赛、特技表演和创意拍摄</span>\r\n                      </li>\r\n                    </ul>\r\n                  </div>\r\n                </div>\r\n                <div class=\"text-center mt-4\">\r\n                  <a href=\"#\" class=\"btn btn-outline-warning rounded-pill\">了解更多穿越无人机技术 <i class=\"bi bi-arrow-right\"></i></a>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 多旋翼无人机部分 -->\r\n      <div class=\"row mb-5\" v-if=\"activeCategory === 'multiRotor' || activeCategory === 'all'\">\r\n        <div class=\"col-12\">\r\n          <h2 class=\"section-title mb-4\">多旋翼无人机</h2>\r\n          <p class=\"section-description mb-4\">高性能多旋翼无人机，稳定悬停、精准控制，适用于高清航拍、测绘、监控和专业应用场景。</p>\r\n          \r\n          <!-- 多旋翼无人机产品展示 -->\r\n          <div class=\"row g-4\">\r\n            <div class=\"col-12 col-md-6 col-lg-4\" v-for=\"drone in getMultiRotorDrones()\" :key=\"drone.id\">\r\n              <div class=\"product-card\">\r\n                <div class=\"product-image-container\">\r\n                  <img :src=\"drone.image\" :alt=\"drone.name\" class=\"img-fluid\">\r\n                </div>\r\n                <div class=\"product-overlay\">\r\n                  <div class=\"product-content\">\r\n                    <div class=\"product-badge bg-success\">多旋翼无人机</div>\r\n                    <h3 class=\"product-title\">{{drone.name}}</h3>\r\n                    <router-link :to=\"'/products/platform/detail/' + drone.id\" class=\"btn btn-light rounded-pill px-4 mt-2\">查看详情</router-link>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          \r\n          <!-- 多旋翼无人机特点 -->\r\n          <div class=\"row mt-4\">\r\n            <div class=\"col-12\">\r\n              <div class=\"features-card p-4\">\r\n                <h4 class=\"mb-3\">多旋翼无人机特点</h4>\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-6\">\r\n                    <ul class=\"feature-list\">\r\n                      <li><i class=\"bi bi-check-circle-fill text-success me-2\"></i>精准悬停与定位</li>\r\n                      <li><i class=\"bi bi-check-circle-fill text-success me-2\"></i>高清航拍与影像采集</li>\r\n                      <li><i class=\"bi bi-check-circle-fill text-success me-2\"></i>灵活的起降能力</li>\r\n                    </ul>\r\n                  </div>\r\n                  <div class=\"col-md-6\">\r\n                    <ul class=\"feature-list\">\r\n                      <li><i class=\"bi bi-check-circle-fill text-success me-2\"></i>丰富的负载选择</li>\r\n                      <li><i class=\"bi bi-check-circle-fill text-success me-2\"></i>智能飞行模式</li>\r\n                      <li><i class=\"bi bi-check-circle-fill text-success me-2\"></i>行业应用解决方案</li>\r\n                    </ul>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 固定翼无人机部分 -->\r\n      <div class=\"row mb-5\" v-if=\"activeCategory === 'fixedWing' || activeCategory === 'all'\">\r\n        <div class=\"col-12\">\r\n          <h2 class=\"section-title mb-4\">固定翼无人机</h2>\r\n          <p class=\"section-description mb-4\">长航时固定翼无人机，高效飞行、大面积覆盖，适用于测绘、巡检、农业和遥感等应用场景。</p>\r\n          \r\n          <!-- 固定翼无人机产品展示 -->\r\n          <div class=\"row g-4\">\r\n            <div class=\"col-12 col-md-6 col-lg-4\" v-for=\"drone in getFixedWingDrones()\" :key=\"drone.id\">\r\n              <div class=\"product-card\">\r\n                <div class=\"product-image-container\">\r\n                  <img :src=\"drone.image\" :alt=\"drone.name\" class=\"img-fluid\">\r\n                </div>\r\n                <div class=\"product-overlay\">\r\n                  <div class=\"product-content\">\r\n                    <div class=\"product-badge bg-primary\">固定翼无人机</div>\r\n                    <h3 class=\"product-title\">{{drone.name}}</h3>\r\n                    <router-link :to=\"'/products/platform/detail/' + drone.id\" class=\"btn btn-light rounded-pill px-4 mt-2\">查看详情</router-link>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          \r\n          <!-- 固定翼无人机特点 -->\r\n          <div class=\"row mt-4\">\r\n            <div class=\"col-12\">\r\n              <div class=\"features-card p-4\">\r\n                <h4 class=\"mb-3\">固定翼无人机特点</h4>\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-6\">\r\n                    <ul class=\"feature-list\">\r\n                      <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>超长航时飞行</li>\r\n                      <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>大面积作业能力</li>\r\n                      <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>高效能航空测绘</li>\r\n                    </ul>\r\n                  </div>\r\n                  <div class=\"col-md-6\">\r\n                    <ul class=\"feature-list\">\r\n                      <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>远程数据传输</li>\r\n                      <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>自动航线规划</li>\r\n                      <li><i class=\"bi bi-check-circle-fill text-primary me-2\"></i>高效油动/电动动力系统</li>\r\n                    </ul>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 无人直升机部分 -->\r\n      <div class=\"row mb-5\" v-if=\"activeCategory === 'helicopter' || activeCategory === 'all'\">\r\n        <div class=\"col-12\">\r\n          <h2 class=\"section-title mb-4\">无人直升机</h2>\r\n          <p class=\"section-description mb-4\">工业级无人直升机，大载荷、长航时、高可靠性，适用于重载运输、空中吊装和特殊作业场景。</p>\r\n          \r\n          <!-- 无人直升机产品展示 -->\r\n          <div class=\"row g-4\">\r\n            <div class=\"col-12 col-md-6 col-lg-4\" v-for=\"drone in getHelicopterDrones()\" :key=\"drone.id\">\r\n              <div class=\"product-card\">\r\n                <div class=\"product-image-container\">\r\n                  <img :src=\"drone.image\" :alt=\"drone.name\" class=\"img-fluid\">\r\n                </div>\r\n                <div class=\"product-overlay\">\r\n                  <div class=\"product-content\">\r\n                    <div class=\"product-badge bg-danger\">无人直升机</div>\r\n                    <h3 class=\"product-title\">{{drone.name}}</h3>\r\n                    <router-link :to=\"'/products/platform/detail/' + drone.id\" class=\"btn btn-light rounded-pill px-4 mt-2\">查看详情</router-link>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          \r\n          <!-- 无人直升机特点 -->\r\n          <div class=\"row mt-4\">\r\n            <div class=\"col-12\">\r\n              <div class=\"features-card p-4\">\r\n                <h4 class=\"mb-3\">无人直升机特点</h4>\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-6\">\r\n                    <ul class=\"feature-list\">\r\n                      <li><i class=\"bi bi-check-circle-fill tex