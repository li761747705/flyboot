eDroneFilter === 'all') {\r\n        return this.drones;\r\n      }\r\n      return this.drones.filter(drone => drone.type === this.activeDroneFilter);\r\n    },\r\n    filteredBoats() {\r\n      if (this.activeBoatFilter === 'all') {\r\n        return this.boats;\r\n      }\r\n      return this.boats.filter(boat => boat.type === this.activeBoatFilter);\r\n    }\r\n  },\r\n  methods: {\r\n    setCategory(category) {\r\n      this.activeCategory = category;\r\n    },\r\n    setDroneFilter(filter) {\r\n      this.activeDroneFilter = filter;\r\n    },\r\n    setBoatFilter(filter) {\r\n      this.activeBoatFilter = filter;\r\n    },\r\n    getDroneTypeName(type) {\r\n      const types = {\r\n        fixedWing: '固定翼',\r\n        multiRotor: '多旋翼',\r\n        helicopter: '直升机',\r\n        fpv: 'FPV'\r\n      };\r\n      return types[type] || type;\r\n    },\r\n    getBoatTypeName(type) {\r\n      const types = {\r\n        single: '单人船',\r\n        sampling: '采样船',\r\n        patrol: '巡逻船',\r\n        firefighting: '消防船'\r\n      };\r\n      return types[type] || type;\r\n    },\r\n    getBadgeClass(type) {\r\n      const classes = {\r\n        fixedWing: 'bg-primary',\r\n        multiRotor: 'bg-success',\r\n        helicopter: 'bg-danger',\r\n        fpv: 'bg-warning',\r\n        single: 'bg-info',\r\n        sampling: 'bg-secondary',\r\n        patrol: 'bg-dark',\r\n        firefighting: 'bg-danger'\r\n      };\r\n      return classes[type] || 'bg-primary';\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.page-header {\r\n  background: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7)), url('../assets/images/products-header.jpg');\r\n  background-size: cover;\r\n  background-position: center;\r\n  color: white;\r\n  padding: 80px 0;\r\n  margin-bottom: 0;\r\n}\r\n\r\n@media (min-width: 768px) {\r\n  .page-header {\r\n    padding: 120px 0;\r\n  }\r\n}\r\n\r\n.page-header h1 {\r\n  font-size: 2rem;\r\n  font-weight: 600;\r\n  margin-bottom: 1rem;\r\n}\r\n\